[{"type":"line text","chapterNumber":30,"verseNumber":11,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":30,"verseNumber":1,"sectionNumber":1,"value":"I will extol you, Yahweh, for you have raised me up, "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":1,"sectionNumber":2,"value":"and have not made my foes to rejoice over me. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":2,"sectionNumber":1,"value":"Yahweh my God, I cried to you, "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":2,"sectionNumber":2,"value":"and you have healed me. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":3,"sectionNumber":1,"value":"Yahweh, you have brought up my soul from Sheol. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":3,"sectionNumber":2,"value":"You have kept me alive, that I should not go down to the pit. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":4,"sectionNumber":1,"value":"Sing praise to Yahweh, you saints of his. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":4,"sectionNumber":2,"value":"Give thanks to his holy name. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":5,"sectionNumber":1,"value":"For his anger is but for a moment. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":5,"sectionNumber":2,"value":"His favor is for a lifetime. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":5,"sectionNumber":3,"value":"Weeping may stay for the night, "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":5,"sectionNumber":4,"value":"but joy comes in the morning. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":6,"sectionNumber":1,"value":"As for me, I said in my prosperity, "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":6,"sectionNumber":2,"value":"“I shall never be moved.” "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":7,"sectionNumber":1,"value":"You, Yahweh, when you favored me, made my mountain stand strong; "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":7,"sectionNumber":2,"value":"but when you hid your face, I was troubled. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":8,"sectionNumber":1,"value":"I cried to you, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":8,"sectionNumber":2,"value":"I made supplication to the Lord: "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":9,"sectionNumber":1,"value":"“What profit is there in my destruction, if I go down to the pit? "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":9,"sectionNumber":2,"value":"Shall the dust praise you? "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":9,"sectionNumber":3,"value":"Shall it declare your truth? "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":10,"sectionNumber":1,"value":"Hear, Yahweh, and have mercy on me. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":10,"sectionNumber":2,"value":"Yahweh, be my helper.” "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":11,"sectionNumber":1,"value":"You have turned my mourning into dancing for me. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":11,"sectionNumber":2,"value":"You have removed my sackcloth, and clothed me with gladness, "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":12,"sectionNumber":1,"value":"to the end that my heart may sing praise to you, and not be silent. "},{"type":"line break"},{"type":"line text","chapterNumber":30,"verseNumber":12,"sectionNumber":2,"value":"Yahweh my God, I will give thanks to you forever! "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]