[{"type":"line text","chapterNumber":127,"verseNumber":6,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":127,"verseNumber":1,"sectionNumber":1,"value":"Unless Yahweh builds the house, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":1,"sectionNumber":2,"value":"they who build it labor in vain. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":1,"sectionNumber":3,"value":"Unless Yahweh watches over the city, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":1,"sectionNumber":4,"value":"the watchman guards it in vain. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":2,"sectionNumber":1,"value":"It is vain for you to rise up early, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":2,"sectionNumber":2,"value":"to stay up late, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":2,"sectionNumber":3,"value":"eating the bread of toil, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":2,"sectionNumber":4,"value":"for he gives sleep to his loved ones. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":3,"sectionNumber":1,"value":"Behold, children are a heritage of Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":3,"sectionNumber":2,"value":"The fruit of the womb is his reward. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":4,"sectionNumber":1,"value":"As arrows in the hand of a mighty man, "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":4,"sectionNumber":2,"value":"so are the children of youth. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":5,"sectionNumber":1,"value":"Happy is the man who has his quiver full of them. "},{"type":"line break"},{"type":"line text","chapterNumber":127,"verseNumber":5,"sectionNumber":2,"value":"They wonâ€™t be disappointed when they speak with their enemies in the gate. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]