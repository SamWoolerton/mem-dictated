[{"type":"line text","chapterNumber":26,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":26,"verseNumber":1,"sectionNumber":1,"value":"Judge me, Yahweh, for I have walked in my integrity. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":1,"sectionNumber":2,"value":"I have trusted also in Yahweh without wavering. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":2,"sectionNumber":1,"value":"Examine me, Yahweh, and prove me. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":2,"sectionNumber":2,"value":"Try my heart and my mind. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":3,"sectionNumber":1,"value":"For your loving kindness is before my eyes. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":3,"sectionNumber":2,"value":"I have walked in your truth. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":4,"sectionNumber":1,"value":"I have not sat with deceitful men, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":4,"sectionNumber":2,"value":"neither will I go in with hypocrites. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":5,"sectionNumber":1,"value":"I hate the assembly of evildoers, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":5,"sectionNumber":2,"value":"and will not sit with the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":6,"sectionNumber":1,"value":"I will wash my hands in innocence, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":6,"sectionNumber":2,"value":"so I will go about your altar, Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":7,"sectionNumber":1,"value":"that I may make the voice of thanksgiving to be heard "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":7,"sectionNumber":2,"value":"and tell of all your wondrous deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":8,"sectionNumber":1,"value":"Yahweh, I love the habitation of your house, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":8,"sectionNumber":2,"value":"the place where your glory dwells. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":9,"sectionNumber":1,"value":"Donâ€™t gather my soul with sinners, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":9,"sectionNumber":2,"value":"nor my life with bloodthirsty men; "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":10,"sectionNumber":1,"value":"in whose hands is wickedness, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":10,"sectionNumber":2,"value":"their right hand is full of bribes. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":26,"verseNumber":10,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":26,"verseNumber":11,"sectionNumber":1,"value":"But as for me, I will walk in my integrity. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":11,"sectionNumber":2,"value":"Redeem me, and be merciful to me. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":12,"sectionNumber":1,"value":"My foot stands in an even place. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":12,"sectionNumber":2,"value":"In the congregations I will bless Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"By David."},{"type":"stanza start"}]