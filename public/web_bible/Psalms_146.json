[{"type":"line text","chapterNumber":146,"verseNumber":21,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":146,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":1,"sectionNumber":2,"value":"Praise Yahweh, my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":2,"sectionNumber":1,"value":"While I live, I will praise Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":2,"sectionNumber":2,"value":"I will sing praises to my God as long as I exist. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":3,"sectionNumber":1,"value":"Donâ€™t put your trust in princes, "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":3,"sectionNumber":2,"value":"in a son of man in whom there is no help. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":4,"sectionNumber":1,"value":"His spirit departs, and he returns to the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":4,"sectionNumber":2,"value":"In that very day, his thoughts perish. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":5,"sectionNumber":1,"value":"Happy is he who has the God of Jacob for his help, "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":5,"sectionNumber":2,"value":"whose hope is in Yahweh, his God: "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":6,"sectionNumber":1,"value":"who made heaven and earth, "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":6,"sectionNumber":2,"value":"the sea, and all that is in them; "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":6,"sectionNumber":3,"value":"who keeps truth forever; "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":7,"sectionNumber":1,"value":"who executes justice for the oppressed; "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":7,"sectionNumber":2,"value":"who gives food to the hungry. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":7,"sectionNumber":3,"value":"Yahweh frees the prisoners. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":8,"sectionNumber":1,"value":"Yahweh opens the eyes of the blind. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":8,"sectionNumber":2,"value":"Yahweh raises up those who are bowed down. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":8,"sectionNumber":3,"value":"Yahweh loves the righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":9,"sectionNumber":1,"value":"Yahweh preserves the foreigners. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":9,"sectionNumber":2,"value":"He upholds the fatherless and widow, "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":9,"sectionNumber":3,"value":"but he turns the way of the wicked upside down. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":10,"sectionNumber":1,"value":"Yahweh will reign forever; "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":10,"sectionNumber":2,"value":"your God, O Zion, to all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":146,"verseNumber":10,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]