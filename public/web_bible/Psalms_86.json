[{"type":"line text","chapterNumber":86,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":86,"verseNumber":1,"sectionNumber":1,"value":"Hear, Yahweh, and answer me, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":1,"sectionNumber":2,"value":"for I am poor and needy. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":2,"sectionNumber":1,"value":"Preserve my soul, for I am godly. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":2,"sectionNumber":2,"value":"You, my God, save your servant who trusts in you. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":3,"sectionNumber":1,"value":"Be merciful to me, Lord, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":3,"sectionNumber":2,"value":"for I call to you all day long. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":4,"sectionNumber":1,"value":"Bring joy to the soul of your servant, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":4,"sectionNumber":2,"value":"for to you, Lord, do I lift up my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":5,"sectionNumber":1,"value":"For you, Lord, are good, and ready to forgive, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":5,"sectionNumber":2,"value":"abundant in loving kindness to all those who call on you. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":6,"sectionNumber":1,"value":"Hear, Yahweh, my prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":6,"sectionNumber":2,"value":"Listen to the voice of my petitions. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":7,"sectionNumber":1,"value":"In the day of my trouble I will call on you, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":7,"sectionNumber":2,"value":"for you will answer me. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":8,"sectionNumber":1,"value":"There is no one like you among the gods, Lord, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":8,"sectionNumber":2,"value":"nor any deeds like your deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":9,"sectionNumber":1,"value":"All nations you have made will come and worship before you, Lord. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":9,"sectionNumber":2,"value":"They shall glorify your name. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":10,"sectionNumber":1,"value":"For you are great, and do wondrous things. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":10,"sectionNumber":2,"value":"You are God alone. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":11,"sectionNumber":1,"value":"Teach me your way, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":11,"sectionNumber":2,"value":"I will walk in your truth. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":11,"sectionNumber":3,"value":"Make my heart undivided to fear your name. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":12,"sectionNumber":1,"value":"I will praise you, Lord my God, with my whole heart. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":12,"sectionNumber":2,"value":"I will glorify your name forever more. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":13,"sectionNumber":1,"value":"For your loving kindness is great toward me. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":13,"sectionNumber":2,"value":"You have delivered my soul from the lowest Sheol. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":14,"sectionNumber":1,"value":"God, the proud have risen up against me. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":14,"sectionNumber":2,"value":"A company of violent men have sought after my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":14,"sectionNumber":3,"value":"and they donâ€™t hold regard for you before them. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":15,"sectionNumber":1,"value":"But you, Lord, are a merciful and gracious God, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":15,"sectionNumber":2,"value":"slow to anger, and abundant in loving kindness and truth. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":16,"sectionNumber":1,"value":"Turn to me, and have mercy on me! "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":16,"sectionNumber":2,"value":"Give your strength to your servant. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":16,"sectionNumber":3,"value":"Save the son of your servant. "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":17,"sectionNumber":1,"value":"Show me a sign of your goodness, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":17,"sectionNumber":2,"value":"that those who hate me may see it, and be shamed, "},{"type":"line break"},{"type":"line text","chapterNumber":86,"verseNumber":17,"sectionNumber":3,"value":"because you, Yahweh, have helped me, and comforted me. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by the sons of Korah; a Song."},{"type":"stanza start"}]