[{"type":"line text","chapterNumber":21,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":21,"verseNumber":1,"sectionNumber":1,"value":"The king rejoices in your strength, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":1,"sectionNumber":2,"value":"How greatly he rejoices in your salvation! "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":2,"sectionNumber":1,"value":"You have given him his heart’s desire, "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":2,"sectionNumber":2,"value":"and have not withheld the request of his lips. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":3,"sectionNumber":1,"value":"For you meet him with the blessings of goodness. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":3,"sectionNumber":2,"value":"You set a crown of fine gold on his head. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":4,"sectionNumber":1,"value":"He asked life of you and you gave it to him, "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":4,"sectionNumber":2,"value":"even length of days forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":5,"sectionNumber":1,"value":"His glory is great in your salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":5,"sectionNumber":2,"value":"You lay honor and majesty on him. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":6,"sectionNumber":1,"value":"For you make him most blessed forever. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":6,"sectionNumber":2,"value":"You make him glad with joy in your presence. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":7,"sectionNumber":1,"value":"For the king trusts in Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":7,"sectionNumber":2,"value":"Through the loving kindness of the Most High, he shall not be moved. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":8,"sectionNumber":1,"value":"Your hand will find out all of your enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":8,"sectionNumber":2,"value":"Your right hand will find out those who hate you. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":9,"sectionNumber":1,"value":"You will make them as a fiery furnace in the time of your anger. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":9,"sectionNumber":2,"value":"Yahweh will swallow them up in his wrath. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":9,"sectionNumber":3,"value":"The fire shall devour them. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":10,"sectionNumber":1,"value":"You will destroy their descendants from the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":10,"sectionNumber":2,"value":"their posterity from among the children of men. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":11,"sectionNumber":1,"value":"For they intended evil against you. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":11,"sectionNumber":2,"value":"They plotted evil against you which cannot succeed. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":12,"sectionNumber":1,"value":"For you will make them turn their back, "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":12,"sectionNumber":2,"value":"when you aim drawn bows at their face. "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":13,"sectionNumber":1,"value":"Be exalted, Yahweh, in your strength, "},{"type":"line break"},{"type":"line text","chapterNumber":21,"verseNumber":13,"sectionNumber":2,"value":"so we will sing and praise your power. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician; set to “The Doe of the Morning.” A Psalm by David."},{"type":"stanza start"}]