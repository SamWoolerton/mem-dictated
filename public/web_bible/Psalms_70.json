[{"type":"line text","chapterNumber":70,"verseNumber":36,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":70,"verseNumber":1,"sectionNumber":1,"value":"Hurry, God, to deliver me. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":1,"sectionNumber":2,"value":"Come quickly to help me, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":2,"sectionNumber":1,"value":"Let them be disappointed and confounded who seek my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":2,"sectionNumber":2,"value":"Let those who desire my ruin be turned back in disgrace. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":3,"sectionNumber":1,"value":"Let them be turned because of their shame "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":3,"sectionNumber":2,"value":"who say, “Aha! Aha!” "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":4,"sectionNumber":1,"value":"Let all those who seek you rejoice and be glad in you. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":4,"sectionNumber":2,"value":"Let those who love your salvation continually say, "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":4,"sectionNumber":3,"value":"“Let God be exalted!” "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":5,"sectionNumber":1,"value":"But I am poor and needy. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":5,"sectionNumber":2,"value":"Come to me quickly, God. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":5,"sectionNumber":3,"value":"You are my help and my deliverer. "},{"type":"line break"},{"type":"line text","chapterNumber":70,"verseNumber":5,"sectionNumber":4,"value":"Yahweh, don’t delay. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]