[{"type":"line text","chapterNumber":42,"verseNumber":29,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":1,"value":"“Behold, my servant, whom I uphold, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":2,"value":"my chosen, in whom my soul delights: "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":3,"value":"I have put my Spirit on him. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":4,"value":"He will bring justice to the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":2,"sectionNumber":1,"value":"He will not shout, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":2,"sectionNumber":2,"value":"nor raise his voice, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":2,"sectionNumber":3,"value":"nor cause it to be heard in the street. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":3,"sectionNumber":1,"value":"He won’t break a bruised reed. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":3,"sectionNumber":2,"value":"He won’t quench a dimly burning wick. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":3,"sectionNumber":3,"value":"He will faithfully bring justice. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":1,"value":"He will not fail nor be discouraged, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":2,"value":"until he has set justice in the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":3,"value":"and the islands wait for his law.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":1,"value":"God Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":2,"value":"he who created the heavens and stretched them out, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":3,"value":"he who spread out the earth and that which comes out of it, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":4,"value":"he who gives breath to its people and spirit to those who walk in it, says: "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":1,"value":"“I, Yahweh, have called you in righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":2,"value":"I will hold your hand. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":3,"value":"I will keep you, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":4,"value":"and make you a covenant for the people, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":5,"value":"as a light for the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":1,"value":"to open the blind eyes, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":2,"value":"to bring the prisoners out of the dungeon, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":3,"value":"and those who sit in darkness out of the prison. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":1,"value":"“I am Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":2,"value":"That is my name. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":3,"value":"I will not give my glory to another, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":4,"value":"nor my praise to engraved images. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":1,"value":"Behold, the former things have happened "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":2,"value":"and I declare new things. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":3,"value":"I tell you about them before they come up.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":1,"value":"Sing to Yahweh a new song, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":2,"value":"and his praise from the end of the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":3,"value":"you who go down to the sea, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":4,"value":"and all that is therein, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":5,"value":"the islands and their inhabitants. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":1,"value":"Let the wilderness and its cities raise their voices, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":2,"value":"with the villages that Kedar inhabits. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":3,"value":"Let the inhabitants of Sela sing. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":4,"value":"Let them shout from the top of the mountains! "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":12,"sectionNumber":1,"value":"Let them give glory to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":12,"sectionNumber":2,"value":"and declare his praise in the islands. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":1,"value":"Yahweh will go out like a mighty man. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":2,"value":"He will stir up zeal like a man of war. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":3,"value":"He will raise a war cry. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":4,"value":"Yes, he will shout aloud. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":5,"value":"He will triumph over his enemies. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":14,"sectionNumber":1,"value":"“I have been silent a long time. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":14,"sectionNumber":2,"value":"I have been quiet and restrained myself. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":14,"sectionNumber":3,"value":"Now I will cry out like a travailing woman. I will both gasp and pant. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":15,"sectionNumber":1,"value":"I will destroy mountains and hills, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":15,"sectionNumber":2,"value":"and dry up all their herbs. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":15,"sectionNumber":3,"value":"I will make the rivers islands, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":15,"sectionNumber":4,"value":"and will dry up the pools. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":1,"value":"I will bring the blind by a way that they don’t know. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":2,"value":"I will lead them in paths that they don’t know. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":3,"value":"I will make darkness light before them, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":4,"value":"and crooked places straight. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":5,"value":"I will do these things, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":6,"value":"and I will not forsake them. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":16,"sectionNumber":7,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":1,"value":"“Those who trust in engraved images, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":2,"value":"who tell molten images, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":3,"value":"‘You are our gods,’ "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":4,"value":"will be turned back. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":5,"value":"They will be utterly disappointed. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":17,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":18,"sectionNumber":1,"value":"“Hear, you deaf, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":18,"sectionNumber":2,"value":"and look, you blind, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":18,"sectionNumber":3,"value":"that you may see. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":19,"sectionNumber":1,"value":"Who is blind, but my servant? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":19,"sectionNumber":2,"value":"Or who is as deaf as my messenger whom I send? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":19,"sectionNumber":3,"value":"Who is as blind as he who is at peace, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":19,"sectionNumber":4,"value":"and as blind as Yahweh’s servant? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":20,"sectionNumber":1,"value":"You see many things, but don’t observe. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":20,"sectionNumber":2,"value":"His ears are open, but he doesn’t listen. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":21,"sectionNumber":1,"value":"It pleased Yahweh, for his righteousness’ sake, to magnify the law "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":21,"sectionNumber":2,"value":"and make it honorable. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":1,"value":"But this is a robbed and plundered people. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":2,"value":"All of them are snared in holes, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":3,"value":"and they are hidden in prisons. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":4,"value":"They have become captives, and no one delivers, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":5,"value":"and a plunder, and no one says, ‘Restore them!’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":22,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":23,"sectionNumber":1,"value":"Who is there among you who will give ear to this? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":23,"sectionNumber":2,"value":"Who will listen and hear for the time to come? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":24,"sectionNumber":1,"value":"Who gave Jacob as plunder, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":24,"sectionNumber":2,"value":"and Israel to the robbers? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":24,"sectionNumber":3,"value":"Didn’t Yahweh, he against whom we have sinned? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":24,"sectionNumber":4,"value":"For they would not walk in his ways, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":24,"sectionNumber":5,"value":"and they disobeyed his law. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":25,"sectionNumber":1,"value":"Therefore he poured the fierceness of his anger on him, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":25,"sectionNumber":2,"value":"and the strength of battle. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":25,"sectionNumber":3,"value":"It set him on fire all around, but he didn’t know. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":25,"sectionNumber":4,"value":"It burned him, but he didn’t take it to heart.” "},{"type":"line break"}]