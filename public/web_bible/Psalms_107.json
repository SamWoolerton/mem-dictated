[{"type":"line text","chapterNumber":107,"verseNumber":48,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":1,"sectionNumber":1,"value":"Give thanks to Yahweh, for he is good, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":1,"sectionNumber":2,"value":"for his loving kindness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":2,"sectionNumber":1,"value":"Let the redeemed by Yahweh say so, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":2,"sectionNumber":2,"value":"whom he has redeemed from the hand of the adversary, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":3,"sectionNumber":1,"value":"And gathered out of the lands, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":3,"sectionNumber":2,"value":"from the east and from the west, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":3,"sectionNumber":3,"value":"from the north and from the south. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":3,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":4,"sectionNumber":1,"value":"They wandered in the wilderness in a desert way. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":4,"sectionNumber":2,"value":"They found no city to live in. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":5,"sectionNumber":1,"value":"Hungry and thirsty, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":5,"sectionNumber":2,"value":"their soul fainted in them. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":6,"sectionNumber":1,"value":"Then they cried to Yahweh in their trouble, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":6,"sectionNumber":2,"value":"and he delivered them out of their distresses, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":7,"sectionNumber":1,"value":"he led them also by a straight way, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":7,"sectionNumber":2,"value":"that they might go to a city to live in. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":8,"sectionNumber":1,"value":"Let them praise Yahweh for his loving kindness, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":8,"sectionNumber":2,"value":"for his wonderful deeds to the children of men! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":8,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":9,"sectionNumber":1,"value":"For he satisfies the longing soul. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":9,"sectionNumber":2,"value":"He fills the hungry soul with good. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":9,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":10,"sectionNumber":1,"value":"Some sat in darkness and in the shadow of death, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":10,"sectionNumber":2,"value":"being bound in affliction and iron, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":11,"sectionNumber":1,"value":"because they rebelled against the words of God, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":11,"sectionNumber":2,"value":"and condemned the counsel of the Most High. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":12,"sectionNumber":1,"value":"Therefore he brought down their heart with labor. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":12,"sectionNumber":2,"value":"They fell down, and there was no one to help. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":13,"sectionNumber":1,"value":"Then they cried to Yahweh in their trouble, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":13,"sectionNumber":2,"value":"and he saved them out of their distresses. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":14,"sectionNumber":1,"value":"He brought them out of darkness and the shadow of death, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":14,"sectionNumber":2,"value":"and broke away their chains. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":15,"sectionNumber":1,"value":"Let them praise Yahweh for his loving kindness, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":15,"sectionNumber":2,"value":"for his wonderful deeds to the children of men! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":15,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":16,"sectionNumber":1,"value":"For he has broken the gates of bronze, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":16,"sectionNumber":2,"value":"and cut through bars of iron. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":16,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":17,"sectionNumber":1,"value":"Fools are afflicted because of their disobedience, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":17,"sectionNumber":2,"value":"and because of their iniquities. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":18,"sectionNumber":1,"value":"Their soul abhors all kinds of food. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":18,"sectionNumber":2,"value":"They draw near to the gates of death. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":19,"sectionNumber":1,"value":"Then they cry to Yahweh in their trouble, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":19,"sectionNumber":2,"value":"he saves them out of their distresses. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":20,"sectionNumber":1,"value":"He sends his word, and heals them, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":20,"sectionNumber":2,"value":"and delivers them from their graves. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":21,"sectionNumber":1,"value":"Let them praise Yahweh for his loving kindness, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":21,"sectionNumber":2,"value":"for his wonderful deeds to the children of men! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":21,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":22,"sectionNumber":1,"value":"Let them offer the sacrifices of thanksgiving, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":22,"sectionNumber":2,"value":"and declare his deeds with singing. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":22,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":23,"sectionNumber":1,"value":"Those who go down to the sea in ships, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":23,"sectionNumber":2,"value":"who do business in great waters; "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":24,"sectionNumber":1,"value":"These see Yahweh’s deeds, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":24,"sectionNumber":2,"value":"and his wonders in the deep. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":25,"sectionNumber":1,"value":"For he commands, and raises the stormy wind, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":25,"sectionNumber":2,"value":"which lifts up its waves. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":26,"sectionNumber":1,"value":"They mount up to the sky; they go down again to the depths. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":26,"sectionNumber":2,"value":"Their soul melts away because of trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":27,"sectionNumber":1,"value":"They reel back and forth, and stagger like a drunken man, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":27,"sectionNumber":2,"value":"and are at their wits’ end. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":28,"sectionNumber":1,"value":"Then they cry to Yahweh in their trouble, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":28,"sectionNumber":2,"value":"and he brings them out of their distress. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":29,"sectionNumber":1,"value":"He makes the storm a calm, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":29,"sectionNumber":2,"value":"so that its waves are still. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":30,"sectionNumber":1,"value":"Then they are glad because it is calm, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":30,"sectionNumber":2,"value":"so he brings them to their desired haven. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":31,"sectionNumber":1,"value":"Let them praise Yahweh for his loving kindness, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":31,"sectionNumber":2,"value":"for his wonderful deeds for the children of men! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":31,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":32,"sectionNumber":1,"value":"Let them exalt him also in the assembly of the people, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":32,"sectionNumber":2,"value":"and praise him in the seat of the elders. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":107,"verseNumber":32,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":107,"verseNumber":33,"sectionNumber":1,"value":"He turns rivers into a desert, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":33,"sectionNumber":2,"value":"water springs into a thirsty ground, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":34,"sectionNumber":1,"value":"and a fruitful land into a salt waste, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":34,"sectionNumber":2,"value":"for the wickedness of those who dwell in it. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":35,"sectionNumber":1,"value":"He turns a desert into a pool of water, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":35,"sectionNumber":2,"value":"and a dry land into water springs. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":36,"sectionNumber":1,"value":"There he makes the hungry live, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":36,"sectionNumber":2,"value":"that they may prepare a city to live in, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":37,"sectionNumber":1,"value":"sow fields, plant vineyards, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":37,"sectionNumber":2,"value":"and reap the fruits of increase. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":38,"sectionNumber":1,"value":"He blesses them also, so that they are multiplied greatly. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":38,"sectionNumber":2,"value":"He doesn’t allow their livestock to decrease. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":39,"sectionNumber":1,"value":"Again, they are diminished and bowed down "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":39,"sectionNumber":2,"value":"through oppression, trouble, and sorrow. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":40,"sectionNumber":1,"value":"He pours contempt on princes, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":40,"sectionNumber":2,"value":"and causes them to wander in a trackless waste. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":41,"sectionNumber":1,"value":"Yet he lifts the needy out of their affliction, "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":41,"sectionNumber":2,"value":"and increases their families like a flock. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":42,"sectionNumber":1,"value":"The upright will see it, and be glad. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":42,"sectionNumber":2,"value":"All the wicked will shut their mouths. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":43,"sectionNumber":1,"value":"Whoever is wise will pay attention to these things. "},{"type":"line break"},{"type":"line text","chapterNumber":107,"verseNumber":43,"sectionNumber":2,"value":"They will consider the loving kindnesses of Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song. A Psalm by David."},{"type":"stanza start"}]