[{"type":"line text","chapterNumber":136,"verseNumber":21,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":136,"verseNumber":1,"sectionNumber":1,"value":"Give thanks to Yahweh, for he is good; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":1,"sectionNumber":2,"value":"for his loving kindness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":2,"sectionNumber":1,"value":"Give thanks to the God of gods; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":2,"sectionNumber":2,"value":"for his loving kindness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":3,"sectionNumber":1,"value":"Give thanks to the Lord of lords; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":3,"sectionNumber":2,"value":"for his loving kindness endures forever: "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":4,"sectionNumber":1,"value":"to him who alone does great wonders; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":4,"sectionNumber":2,"value":"for his loving kindness endures forever: "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":5,"sectionNumber":1,"value":"to him who by understanding made the heavens; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":5,"sectionNumber":2,"value":"for his loving kindness endures forever: "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":6,"sectionNumber":1,"value":"to him who spread out the earth above the waters; "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":6,"sectionNumber":2,"value":"for his loving kindness endures forever: "},{"type":"line break"},{"type":"line text","chapterNumber":136,"verseNumber":7,"sectionNumber":1,"value":"to him who made the great lights; "},{"type":"li