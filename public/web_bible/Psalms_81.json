[{"type":"line text","chapterNumber":81,"verseNumber":19,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":81,"verseNumber":1,"sectionNumber":1,"value":"Sing aloud to God, our strength! "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":1,"sectionNumber":2,"value":"Make a joyful shout to the God of Jacob! "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":2,"sectionNumber":1,"value":"Raise a song, and bring here the tambourine, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":2,"sectionNumber":2,"value":"the pleasant lyre with the harp. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":3,"sectionNumber":1,"value":"Blow the trumpet at the New Moon, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":3,"sectionNumber":2,"value":"at the full moon, on our feast day. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":4,"sectionNumber":1,"value":"For it is a statute for Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":4,"sectionNumber":2,"value":"an ordinance of the God of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":5,"sectionNumber":1,"value":"He appointed it in Joseph for a covenant, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":5,"sectionNumber":2,"value":"when he went out over the land of Egypt, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":5,"sectionNumber":3,"value":"I heard a language that I didn’t know. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":6,"sectionNumber":1,"value":"“I removed his shoulder from the burden. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":6,"sectionNumber":2,"value":"His hands were freed from the basket. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":7,"sectionNumber":1,"value":"You called in trouble, and I delivered you. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":7,"sectionNumber":2,"value":"I answered you in the secret place of thunder. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":7,"sectionNumber":3,"value":"I tested you at the waters of Meribah.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":81,"verseNumber":7,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":81,"verseNumber":8,"sectionNumber":1,"value":"“Hear, my people, and I will testify to you, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":8,"sectionNumber":2,"value":"Israel, if you would listen to me! "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":9,"sectionNumber":1,"value":"There shall be no strange god in you, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":9,"sectionNumber":2,"value":"neither shall you worship any foreign god. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":10,"sectionNumber":1,"value":"I am Yahweh, your God, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":10,"sectionNumber":2,"value":"who brought you up out of the land of Egypt. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":10,"sectionNumber":3,"value":"Open your mouth wide, and I will fill it. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":11,"sectionNumber":1,"value":"But my people didn’t listen to my voice. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":11,"sectionNumber":2,"value":"Israel desired none of me. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":12,"sectionNumber":1,"value":"So I let them go after the stubbornness of their hearts, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":12,"sectionNumber":2,"value":"that they might walk in their own counsels. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":13,"sectionNumber":1,"value":"Oh that my people would listen to me, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":13,"sectionNumber":2,"value":"that Israel would walk in my ways! "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":14,"sectionNumber":1,"value":"I would soon subdue their enemies, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":14,"sectionNumber":2,"value":"and turn my hand against their adversaries. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":15,"sectionNumber":1,"value":"The haters of Yahweh would cringe before him, "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":15,"sectionNumber":2,"value":"and their punishment would last forever. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":16,"sectionNumber":1,"value":"But he would have also fed them with the finest of the wheat. "},{"type":"line break"},{"type":"line text","chapterNumber":81,"verseNumber":16,"sectionNumber":2,"value":"I will satisfy you with honey out of the rock.” "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by Asaph."},{"type":"stanza start"}]