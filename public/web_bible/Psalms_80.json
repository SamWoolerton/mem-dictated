[{"type":"line text","chapterNumber":80,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":80,"verseNumber":1,"sectionNumber":1,"value":"Hear us, Shepherd of Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":1,"sectionNumber":2,"value":"you who lead Joseph like a flock, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":1,"sectionNumber":3,"value":"you who sit above the cherubim, shine out. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":2,"sectionNumber":1,"value":"Before Ephraim and Benjamin and Manasseh, stir up your might! "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":2,"sectionNumber":2,"value":"Come to save us! "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":3,"sectionNumber":1,"value":"Turn us again, God. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":3,"sectionNumber":2,"value":"Cause your face to shine, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":3,"sectionNumber":3,"value":"and we will be saved. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":80,"verseNumber":3,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":80,"verseNumber":4,"sectionNumber":1,"value":"Yahweh God of Armies, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":4,"sectionNumber":2,"value":"how long will you be angry against the prayer of your people? "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":5,"sectionNumber":1,"value":"You have fed them with the bread of tears, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":5,"sectionNumber":2,"value":"and given them tears to drink in large measure. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":6,"sectionNumber":1,"value":"You make us a source of contention to our neighbors. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":6,"sectionNumber":2,"value":"Our enemies laugh among themselves. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":7,"sectionNumber":1,"value":"Turn us again, God of Armies. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":7,"sectionNumber":2,"value":"Cause your face to shine, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":7,"sectionNumber":3,"value":"and we will be saved. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":80,"verseNumber":7,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":80,"verseNumber":8,"sectionNumber":1,"value":"You brought a vine out of Egypt. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":8,"sectionNumber":2,"value":"You drove out the nations, and planted it. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":9,"sectionNumber":1,"value":"You cleared the ground for it. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":9,"sectionNumber":2,"value":"It took deep root, and filled the land. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":10,"sectionNumber":1,"value":"The mountains were covered with its shadow. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":10,"sectionNumber":2,"value":"Its boughs were like God’s cedars. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":11,"sectionNumber":1,"value":"It sent out its branches to the sea, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":11,"sectionNumber":2,"value":"its shoots to the River. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":12,"sectionNumber":1,"value":"Why have you broken down its walls, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":12,"sectionNumber":2,"value":"so that all those who pass by the way pluck it? "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":13,"sectionNumber":1,"value":"The boar out of the wood ravages it. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":13,"sectionNumber":2,"value":"The wild animals of the field feed on it. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":14,"sectionNumber":1,"value":"Turn again, we beg you, God of Armies. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":14,"sectionNumber":2,"value":"Look down from heaven, and see, and visit this vine, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":15,"sectionNumber":1,"value":"the stock which your right hand planted, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":15,"sectionNumber":2,"value":"the branch that you made strong for yourself. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":16,"sectionNumber":1,"value":"It’s burned with fire. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":16,"sectionNumber":2,"value":"It’s cut down. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":16,"sectionNumber":3,"value":"They perish at your rebuke. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":17,"sectionNumber":1,"value":"Let your hand be on the man of your right hand, "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":17,"sectionNumber":2,"value":"on the son of man whom you made strong for yourself. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":18,"sectionNumber":1,"value":"So we will not turn away from you. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":18,"sectionNumber":2,"value":"Revive us, and we will call on your name. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":19,"sectionNumber":1,"value":"Turn us again, Yahweh God of Armies. "},{"type":"line break"},{"type":"line text","chapterNumber":80,"verseNumber":19,"sectionNumber":2,"value":"Cause your face to shine, and we will be saved. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. On an instrument of Gath. By Asaph."},{"type":"stanza start"}]