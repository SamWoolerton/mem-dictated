[{"type":"line text","chapterNumber":111,"verseNumber":7,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":111,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":1,"sectionNumber":2,"value":"I will give thanks to Yahweh with my whole heart, "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":1,"sectionNumber":3,"value":"in the council of the upright, and in the congregation. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":2,"sectionNumber":1,"value":"Yahwehâ€™s works are great, "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":2,"sectionNumber":2,"value":"pondered by all those who delight in them. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":3,"sectionNumber":1,"value":"His work is honor and majesty. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":3,"sectionNumber":2,"value":"His righteousness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":4,"sectionNumber":1,"value":"He has caused his wonderful works to be remembered. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":4,"sectionNumber":2,"value":"Yahweh is gracious and merciful. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":5,"sectionNumber":1,"value":"He has given food to those who fear him. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":5,"sectionNumber":2,"value":"He always remembers his covenant. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":6,"sectionNumber":1,"value":"He has shown his people the power of his works, "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":6,"sectionNumber":2,"value":"in giving them the heritage of the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":7,"sectionNumber":1,"value":"The works of his hands are truth and justice. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":7,"sectionNumber":2,"value":"All his precepts are sure. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":8,"sectionNumber":1,"value":"They are established forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":8,"sectionNumber":2,"value":"They are done in truth and uprightness. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":9,"sectionNumber":1,"value":"He has sent redemption to his people. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":9,"sectionNumber":2,"value":"He has ordained his covenant forever. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":9,"sectionNumber":3,"value":"His name is holy and awesome! "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":10,"sectionNumber":1,"value":"The fear of Yahweh is the beginning of wisdom. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":10,"sectionNumber":2,"value":"All those who do his work have a good understanding. "},{"type":"line break"},{"type":"line text","chapterNumber":111,"verseNumber":10,"sectionNumber":3,"value":"His praise endures forever! "},{"type":"line break"}]