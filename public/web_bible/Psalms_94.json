[{"type":"line text","chapterNumber":94,"verseNumber":5,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":94,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, you God to whom vengeance belongs, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":1,"sectionNumber":2,"value":"you God to whom vengeance belongs, shine out. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":2,"sectionNumber":1,"value":"Rise up, you judge of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":2,"sectionNumber":2,"value":"Pay back the proud what they deserve. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":3,"sectionNumber":1,"value":"Yahweh, how long will the wicked, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":3,"sectionNumber":2,"value":"how long will the wicked triumph? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":4,"sectionNumber":1,"value":"They pour out arrogant words. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":4,"sectionNumber":2,"value":"All the evildoers boast. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":5,"sectionNumber":1,"value":"They break your people in pieces, Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":5,"sectionNumber":2,"value":"and afflict your heritage. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":6,"sectionNumber":1,"value":"They kill the widow and the alien, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":6,"sectionNumber":2,"value":"and murder the fatherless. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":7,"sectionNumber":1,"value":"They say, “Yah will not see, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":7,"sectionNumber":2,"value":"neither will Jacob’s God consider.” "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":8,"sectionNumber":1,"value":"Consider, you senseless among the people; "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":8,"sectionNumber":2,"value":"you fools, when will you be wise? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":9,"sectionNumber":1,"value":"He who implanted the ear, won’t he hear? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":9,"sectionNumber":2,"value":"He who formed the eye, won’t he see? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":10,"sectionNumber":1,"value":"He who disciplines the nations, won’t he punish? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":10,"sectionNumber":2,"value":"He who teaches man knows. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":11,"sectionNumber":1,"value":"Yahweh knows the thoughts of man, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":11,"sectionNumber":2,"value":"that they are futile. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":12,"sectionNumber":1,"value":"Blessed is the man whom you discipline, Yah, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":12,"sectionNumber":2,"value":"and teach out of your law, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":13,"sectionNumber":1,"value":"that you may give him rest from the days of adversity, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":13,"sectionNumber":2,"value":"until the pit is dug for the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":14,"sectionNumber":1,"value":"For Yahweh won’t reject his people, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":14,"sectionNumber":2,"value":"neither will he forsake his inheritance. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":15,"sectionNumber":1,"value":"For judgment will return to righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":15,"sectionNumber":2,"value":"All the upright in heart shall follow it. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":16,"sectionNumber":1,"value":"Who will rise up for me against the wicked? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":16,"sectionNumber":2,"value":"Who will stand up for me against the evildoers? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":17,"sectionNumber":1,"value":"Unless Yahweh had been my help, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":17,"sectionNumber":2,"value":"my soul would have soon lived in silence. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":18,"sectionNumber":1,"value":"When I said, “My foot is slipping!” "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":18,"sectionNumber":2,"value":"Your loving kindness, Yahweh, held me up. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":19,"sectionNumber":1,"value":"In the multitude of my thoughts within me, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":19,"sectionNumber":2,"value":"your comforts delight my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":20,"sectionNumber":1,"value":"Shall the throne of wickedness have fellowship with you, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":20,"sectionNumber":2,"value":"which brings about mischief by statute? "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":21,"sectionNumber":1,"value":"They gather themselves together against the soul of the righteous, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":21,"sectionNumber":2,"value":"and condemn the innocent blood. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":22,"sectionNumber":1,"value":"But Yahweh has been my high tower, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":22,"sectionNumber":2,"value":"my God, the rock of my refuge. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":23,"sectionNumber":1,"value":"He has brought on them their own iniquity, "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":23,"sectionNumber":2,"value":"and will cut them off in their own wickedness. "},{"type":"line break"},{"type":"line text","chapterNumber":94,"verseNumber":23,"sectionNumber":3,"value":"Yahweh, our God, will cut them off. "},{"type":"line break"}]