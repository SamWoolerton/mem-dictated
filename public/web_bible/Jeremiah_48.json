[{"type":"paragraph text","chapterNumber":48,"verseNumber":1,"sectionNumber":1,"value":"Of Moab. Yahweh of Armies, the God of Israel, says: "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":2,"value":"“Woe to Nebo! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":3,"value":"For it is laid waste. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":4,"value":"Kiriathaim is disappointed. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":5,"value":"It is taken. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":6,"value":"Misgab is put to shame "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":7,"value":"and broken down. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":1,"value":"The praise of Moab is no more. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":2,"value":"In Heshbon they have devised evil against her: "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":3,"value":"‘Come! Let’s cut her off from being a nation.’ "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":4,"value":"You also, Madmen, will be brought to silence. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":5,"value":"The sword will pursue you. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":3,"sectionNumber":1,"value":"The sound of a cry from Horonaim, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":3,"sectionNumber":2,"value":"desolation and great destruction! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":4,"sectionNumber":1,"value":"Moab is destroyed. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":4,"sectionNumber":2,"value":"Her little ones have caused a cry to be heard. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":5,"sectionNumber":1,"value":"For they will go up by the ascent of Luhith with continual weeping. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":5,"sectionNumber":2,"value":"For at the descent of Horonaim they have heard the distress of the cry of destruction. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":6,"sectionNumber":1,"value":"Flee! Save your lives! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":6,"sectionNumber":2,"value":"Be like the juniper bush in the wilderness. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":7,"sectionNumber":1,"value":"For, because you have trusted in your works and in your treasures, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":7,"sectionNumber":2,"value":"you also will be taken. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":7,"sectionNumber":3,"value":"Chemosh will go out into captivity, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":7,"sectionNumber":4,"value":"his priests and his princes together. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":1,"value":"The destroyer will come on every city, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":2,"value":"and no city will escape; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":3,"value":"the valley also will perish, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":4,"value":"and the plain will be destroyed; as Yahweh has spoken. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":1,"value":"Give wings to Moab, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":2,"value":"that she may fly and get herself away: "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":3,"value":"and her cities will become a desolation, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":4,"value":"without anyone to dwell in them. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":1,"value":"“Cursed is he who does the work of Yahweh negligently; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":2,"value":"and cursed is he who keeps back his sword from blood. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":1,"value":"“Moab has been at ease from his youth, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":2,"value":"and he has settled on his lees, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":3,"value":"and has not been emptied from vessel to vessel, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":4,"value":"neither has he gone into captivity: "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":5,"value":"therefore his taste remains in him, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":6,"value":"and his scent is not changed. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":1,"value":"Therefore behold, the days come,” says Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":2,"value":"“that I will send to him those who pour off, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":3,"value":"and they will pour him off; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":4,"value":"and they will empty his vessels, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":5,"value":"and break their containers in pieces. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":1,"value":"Moab will be ashamed of Chemosh, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":2,"value":"as the house of Israel was ashamed of Bethel, their confidence. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":14,"sectionNumber":1,"value":"“How do you say, ‘We are mighty men, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":14,"sectionNumber":2,"value":"and valiant men for the war?’ "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":15,"sectionNumber":1,"value":"Moab is laid waste, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":15,"sectionNumber":2,"value":"and they have gone up into his cities, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":15,"sectionNumber":3,"value":"and his chosen young men have gone down to the slaughter,” "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":15,"sectionNumber":4,"value":"says the King, whose name is Yahweh of Armies. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":16,"sectionNumber":1,"value":"“The calamity of Moab is near to come, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":16,"sectionNumber":2,"value":"and his affliction hurries fast. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":17,"sectionNumber":1,"value":"All you who are around him, bemoan him, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":17,"sectionNumber":2,"value":"and all you who know his name; say, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":17,"sectionNumber":3,"value":"‘How the strong staff is broken, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":17,"sectionNumber":4,"value":"the beautiful rod!’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":17,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":18,"sectionNumber":1,"value":"“You daughter who dwells in Dibon, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":18,"sectionNumber":2,"value":"come down from your glory, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":18,"sectionNumber":3,"value":"and sit in thirst; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":18,"sectionNumber":4,"value":"for the destroyer of Moab has come up against you. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":18,"sectionNumber":5,"value":"He has destroyed your strongholds. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":19,"sectionNumber":1,"value":"Inhabitant of Aroer, stand by the way and watch. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":19,"sectionNumber":2,"value":"Ask him who flees, and her who escapes; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":19,"sectionNumber":3,"value":"say, ‘What has been done?’ "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":20,"sectionNumber":1,"value":"Moab is disappointed; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":20,"sectionNumber":2,"value":"for it is broken down. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":20,"sectionNumber":3,"value":"Wail and cry! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":20,"sectionNumber":4,"value":"Tell it by the Arnon, that Moab is laid waste. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":21,"sectionNumber":1,"value":"Judgment has come on the plain country, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":21,"sectionNumber":2,"value":"on Holon, on Jahzah, on Mephaath, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":22,"sectionNumber":1,"value":"on Dibon, on Nebo, on Beth Diblathaim, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":23,"sectionNumber":1,"value":"on Kiriathaim, on Beth Gamul, on Beth Meon, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":24,"sectionNumber":1,"value":"on Kerioth, on Bozrah, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":24,"sectionNumber":2,"value":"and on all the cities of the land of Moab, far or near. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":25,"sectionNumber":1,"value":"The horn of Moab is cut off, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":25,"sectionNumber":2,"value":"and his arm is broken,” says Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":25,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":26,"sectionNumber":1,"value":"“Make him drunken; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":26,"sectionNumber":2,"value":"for he magnified himself against Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":26,"sectionNumber":3,"value":"Moab will wallow in his vomit, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":26,"sectionNumber":4,"value":"and he also will be in derision. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":27,"sectionNumber":1,"value":"For wasn’t Israel a derision to you? "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":27,"sectionNumber":2,"value":"Was he found among thieves? "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":27,"sectionNumber":3,"value":"For as often as you speak of him, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":27,"sectionNumber":4,"value":"you shake your head. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":28,"sectionNumber":1,"value":"You inhabitants of Moab, leave the cities, and dwell in the rock. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":28,"sectionNumber":2,"value":"Be like the dove that makes her nest over the mouth of the abyss. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":28,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":29,"sectionNumber":1,"value":"“We have heard of the pride of Moab. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":29,"sectionNumber":2,"value":"He is very proud in his loftiness, his pride, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":29,"sectionNumber":3,"value":"his arrogance, and the arrogance of his heart. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":30,"sectionNumber":1,"value":"I know his wrath,” says Yahweh, “that it is nothing; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":30,"sectionNumber":2,"value":"his boastings have done nothing. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":31,"sectionNumber":1,"value":"Therefore I will wail for Moab. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":31,"sectionNumber":2,"value":"Yes, I will cry out for all Moab. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":31,"sectionNumber":3,"value":"They will mourn for the men of Kir Heres. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":1,"value":"With more than the weeping of Jazer "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":2,"value":"I will weep for you, vine of Sibmah. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":3,"value":"Your branches passed over the sea. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":4,"value":"They reached even to the sea of Jazer. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":5,"value":"The destroyer has fallen on your summer fruits "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":32,"sectionNumber":6,"value":"and on your vintage. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":33,"sectionNumber":1,"value":"Gladness and joy is taken away from the fruitful field "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":33,"sectionNumber":2,"value":"and from the land of Moab. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":33,"sectionNumber":3,"value":"I have caused wine to cease from the wine presses. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":33,"sectionNumber":4,"value":"No one will tread with shouting. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":33,"sectionNumber":5,"value":"The shouting will be no shouting. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":34,"sectionNumber":1,"value":"From the cry of Heshbon even to Elealeh, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":34,"sectionNumber":2,"value":"even to Jahaz they have uttered their voice, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":34,"sectionNumber":3,"value":"from Zoar even to Horonaim, to Eglath Shelishiyah; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":34,"sectionNumber":4,"value":"for the waters of Nimrim will also become desolate. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":35,"sectionNumber":1,"value":"Moreover I will cause to cease in Moab,” says Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":35,"sectionNumber":2,"value":"“him who offers in the high place, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":35,"sectionNumber":3,"value":"and him who burns incense to his gods. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":36,"sectionNumber":1,"value":"Therefore my heart sounds for Moab like pipes, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":36,"sectionNumber":2,"value":"and my heart sounds like pipes for the men of Kir Heres. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":36,"sectionNumber":3,"value":"Therefore the abundance that he has gotten has perished. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":37,"sectionNumber":1,"value":"For every head is bald, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":37,"sectionNumber":2,"value":"and every beard clipped. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":37,"sectionNumber":3,"value":"There are cuttings on all the hands, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":37,"sectionNumber":4,"value":"and sackcloth on the waist. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":38,"sectionNumber":1,"value":"On all the housetops of Moab, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":38,"sectionNumber":2,"value":"and in its streets, there is lamentation everywhere; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":38,"sectionNumber":3,"value":"for I have broken Moab like a vessel in which no one delights,” says Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":39,"sectionNumber":1,"value":"“How it is broken down! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":39,"sectionNumber":2,"value":"How they wail! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":39,"sectionNumber":3,"value":"How Moab has turned the back with shame! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":39,"sectionNumber":4,"value":"So will Moab become a derision "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":39,"sectionNumber":5,"value":"and a terror to all who are around him.” "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":40,"sectionNumber":1,"value":"For Yahweh says: “Behold, he will fly as an eagle, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":40,"sectionNumber":2,"value":"and will spread out his wings against Moab. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":41,"sectionNumber":1,"value":"Kerioth is taken, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":41,"sectionNumber":2,"value":"and the strongholds are seized. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":41,"sectionNumber":3,"value":"The heart of the mighty men of Moab at that day "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":41,"sectionNumber":4,"value":"will be as the heart of a woman in her pangs. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":42,"sectionNumber":1,"value":"Moab will be destroyed from being a people, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":42,"sectionNumber":2,"value":"because he has magnified himself against Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":43,"sectionNumber":1,"value":"Terror, the pit, and the snare are on you, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":43,"sectionNumber":2,"value":"inhabitant of Moab,” says Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":44,"sectionNumber":1,"value":"“He who flees from the terror will fall into the pit; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":44,"sectionNumber":2,"value":"and he who gets up out of the pit will be taken in the snare: "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":44,"sectionNumber":3,"value":"for I will bring on him, even on Moab, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":44,"sectionNumber":4,"value":"the year of their visitation,” says Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":44,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":45,"sectionNumber":1,"value":"“Those who fled stand without strength under the shadow of Heshbon; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":45,"sectionNumber":2,"value":"for a fire has gone out of Heshbon, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":45,"sectionNumber":3,"value":"and a flame from the middle of Sihon, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":45,"sectionNumber":4,"value":"and has devoured the corner of Moab, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":45,"sectionNumber":5,"value":"and the crown of the head of the tumultuous ones. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":46,"sectionNumber":1,"value":"Woe to you, O Moab! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":46,"sectionNumber":2,"value":"The people of Chemosh are undone; "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":46,"sectionNumber":3,"value":"for your sons are taken away captive, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":46,"sectionNumber":4,"value":"and your daughters into captivity. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":48,"verseNumber":46,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":47,"sectionNumber":1,"value":"“Yet I will reverse the captivity of Moab in the latter days,” "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":47,"sectionNumber":2,"value":"says Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":48,"verseNumber":47,"sectionNumber":3,"value":"Thus far is the judgment of Moab. "},{"type":"paragraph end"},{"type":"paragraph start"}]