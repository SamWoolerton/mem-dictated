[{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":1,"value":"“Arise, shine; for your light has come, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":2,"value":"and Yahweh’s glory has risen on you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":1,"value":"For, behold, darkness will cover the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":2,"value":"and thick darkness the peoples; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":3,"value":"but Yahweh will arise on you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":4,"value":"and his glory shall be seen on you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":3,"sectionNumber":1,"value":"Nations will come to your light, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":3,"sectionNumber":2,"value":"and kings to the brightness of your rising. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":3,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":1,"value":"“Lift up your eyes all around, and see: "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":2,"value":"they all gather themselves together. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":3,"value":"They come to you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":4,"value":"Your sons will come from far away, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":5,"value":"and your daughters will be carried in arms. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":1,"value":"Then you shall see and be radiant, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":2,"value":"and your heart will thrill and be enlarged; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":3,"value":"because the abundance of the sea will be turned to you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":4,"value":"The wealth of the nations will come to you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":1,"value":"A multitude of camels will cover you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":2,"value":"the dromedaries of Midian and Ephah. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":3,"value":"All from Sheba will come. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":4,"value":"They will bring gold and frankincense, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":5,"value":"and will proclaim the praises of Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":1,"value":"All the flocks of Kedar will be gathered together to you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":2,"value":"The rams of Nebaioth will serve you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":3,"value":"They will be accepted as offerings on my altar; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":4,"value":"and I will beautify my glorious house. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":1,"value":"“Who are these who fly as a cloud, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":2,"value":"and as the doves to their windows? "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":1,"value":"Surely the islands will wait for me, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":2,"value":"and the ships of Tarshish first, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":3,"value":"to bring your sons from far, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":4,"value":"their silver and their gold with them, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":5,"value":"for the name of Yahweh your God, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":6,"value":"and for the Holy One of Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":7,"value":"because he has glorified you. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":8,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":1,"value":"“Foreigners will build up your walls, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":2,"value":"and their kings will serve you: "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":3,"value":"for in my wrath I struck you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":4,"value":"but in my favor I have had mercy on you. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":11,"sectionNumber":1,"value":"Your gates also shall be open continually; they shall not be shut day nor night, that men may bring to you the wealth of the nations, and their kings led captive. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":12,"sectionNumber":1,"value":"For that nation and kingdom that will not serve you shall perish; yes, those nations shall be utterly wasted. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":12,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":13,"sectionNumber":1,"value":"“The glory of Lebanon shall come to you, the cypress tree, the pine, and the box tree together, to beautify the place of my sanctuary; and I will make the place of my feet glorious. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":14,"sectionNumber":1,"value":"The sons of those who afflicted you will come bowing to you; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":14,"sectionNumber":2,"value":"and all those who despised you will bow themselves down at the soles of your feet. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":14,"sectionNumber":3,"value":"They will call you Yahweh’s City, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":14,"sectionNumber":4,"value":"the Zion of the Holy One of Israel. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":14,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":15,"sectionNumber":1,"value":"“Whereas you have been forsaken and hated, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":15,"sectionNumber":2,"value":"so that no one passed through you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":15,"sectionNumber":3,"value":"I will make you an eternal excellency, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":15,"sectionNumber":4,"value":"a joy of many generations. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":16,"sectionNumber":1,"value":"You will also drink the milk of the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":16,"sectionNumber":2,"value":"and will nurse from royal breasts. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":16,"sectionNumber":3,"value":"Then you will know that I, Yahweh, am your Savior, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":16,"sectionNumber":4,"value":"your Redeemer, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":16,"sectionNumber":5,"value":"the Mighty One of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":1,"value":"For bronze I will bring gold; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":2,"value":"for iron I will bring silver; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":3,"value":"for wood, bronze, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":4,"value":"and for stones, iron. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":5,"value":"I will also make peace your governor, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":6,"value":"and righteousness your ruler. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":18,"sectionNumber":1,"value":"Violence shall no more be heard in your land, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":18,"sectionNumber":2,"value":"nor desolation or destruction within your borders; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":18,"sectionNumber":3,"value":"but you will call your walls Salvation, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":18,"sectionNumber":4,"value":"and your gates Praise. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":19,"sectionNumber":1,"value":"The sun will be no more your light by day; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":19,"sectionNumber":2,"value":"nor will the brightness of the moon give light to you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":19,"sectionNumber":3,"value":"but Yahweh will be your everlasting light, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":19,"sectionNumber":4,"value":"and your God will be your glory. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":20,"sectionNumber":1,"value":"Your sun will not go down any more, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":20,"sectionNumber":2,"value":"nor will your moon withdraw itself; "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":20,"sectionNumber":3,"value":"for Yahweh will be your everlasting light, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":20,"sectionNumber":4,"value":"and the days of your mourning will end. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":1,"value":"Then your people will all be righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":2,"value":"They will inherit the land forever, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":3,"value":"the branch of my planting, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":4,"value":"the work of my hands, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":21,"sectionNumber":5,"value":"that I may be glorified. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":22,"sectionNumber":1,"value":"The little one will become a thousand, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":22,"sectionNumber":2,"value":"and the small one a strong nation. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":22,"sectionNumber":3,"value":"I, Yahweh, will do this quickly in its time.” "},{"type":"line break"}]