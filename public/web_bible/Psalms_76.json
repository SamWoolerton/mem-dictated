[{"type":"line text","chapterNumber":76,"verseNumber":10,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":76,"verseNumber":1,"sectionNumber":1,"value":"In Judah, God is known. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":1,"sectionNumber":2,"value":"His name is great in Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":2,"sectionNumber":1,"value":"His tabernacle is also in Salem; "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":2,"sectionNumber":2,"value":"His dwelling place in Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":3,"sectionNumber":1,"value":"There he broke the flaming arrows of the bow, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":3,"sectionNumber":2,"value":"the shield, and the sword, and the weapons of war. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":4,"sectionNumber":1,"value":"Glorious are you, and excellent, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":4,"sectionNumber":2,"value":"more than mountains of game. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":5,"sectionNumber":1,"value":"Valiant men lie plundered, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":5,"sectionNumber":2,"value":"they have slept their last sleep. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":5,"sectionNumber":3,"value":"None of the men of war can lift their hands. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":6,"sectionNumber":1,"value":"At your rebuke, God of Jacob, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":6,"sectionNumber":2,"value":"both chariot and horse are cast into a dead sleep. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":7,"sectionNumber":1,"value":"You, even you, are to be feared. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":7,"sectionNumber":2,"value":"Who can stand in your sight when you are angry? "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":8,"sectionNumber":1,"value":"You pronounced judgment from heaven. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":8,"sectionNumber":2,"value":"The earth feared, and was silent, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":9,"sectionNumber":1,"value":"when God arose to judgment, "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":9,"sectionNumber":2,"value":"to save all the afflicted ones of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":10,"sectionNumber":1,"value":"Surely the wrath of man praises you. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":10,"sectionNumber":2,"value":"The survivors of your wrath are restrained. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":11,"sectionNumber":1,"value":"Make vows to Yahweh your God, and fulfill them! "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":11,"sectionNumber":2,"value":"Let all of his neighbors bring presents to him who is to be feared. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":12,"sectionNumber":1,"value":"He will cut off the spirit of princes. "},{"type":"line break"},{"type":"line text","chapterNumber":76,"verseNumber":12,"sectionNumber":2,"value":"He is feared by the kings of the earth. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. To Jeduthun. A Psalm by Asaph."},{"type":"stanza start"}]