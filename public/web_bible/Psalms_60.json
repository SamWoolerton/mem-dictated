[{"type":"line text","chapterNumber":60,"verseNumber":17,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":1,"value":"God, you have rejected us. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":2,"value":"You have broken us down. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":3,"value":"You have been angry. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":1,"sectionNumber":4,"value":"Restore us, again. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":1,"value":"You have made the land tremble. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":2,"value":"You have torn it. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":3,"value":"Mend its fractures, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":2,"sectionNumber":4,"value":"for it quakes. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":3,"sectionNumber":1,"value":"You have shown your people hard things. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":3,"sectionNumber":2,"value":"You have made us drink the wine that makes us stagger. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":1,"value":"You have given a banner to those who fear you, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":2,"value":"that it may be displayed because of the truth. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":4,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":1,"value":"So that your beloved may be delivered, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":5,"sectionNumber":2,"value":"save with your right hand, and answer us. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":1,"value":"God has spoken from his sanctuary: "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":2,"value":"“I will triumph. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":3,"value":"I will divide Shechem, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":6,"sectionNumber":4,"value":"and measure out the valley of Succoth. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":1,"value":"Gilead is mine, and Manasseh is mine. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":2,"value":"Ephraim also is the defense of my head. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":7,"sectionNumber":3,"value":"Judah is my scepter. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":1,"value":"Moab is my wash basin. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":2,"value":"I will throw my sandal on Edom. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":3,"value":"I shout in triumph over Philistia.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":60,"verseNumber":8,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":1,"value":"Who will bring me into the strong city? "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":9,"sectionNumber":2,"value":"Who has led me to Edom? "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":1,"value":"Haven’t you, God, rejected us? "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":10,"sectionNumber":2,"value":"You don’t go out with our armies, God. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":11,"sectionNumber":1,"value":"Give us help against the adversary, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":11,"sectionNumber":2,"value":"for the help of man is vain. "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":12,"sectionNumber":1,"value":"Through God we will do valiantly, "},{"type":"line break"},{"type":"line text","chapterNumber":60,"verseNumber":12,"sectionNumber":2,"value":"for it is he who will tread down our adversaries. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. For a stringed instrument. By David."},{"type":"stanza start"}]