[{"type":"line text","chapterNumber":139,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":139,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, you have searched me, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":1,"sectionNumber":2,"value":"and you know me. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":2,"sectionNumber":1,"value":"You know my sitting down and my rising up. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":2,"sectionNumber":2,"value":"You perceive my thoughts from afar. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":3,"sectionNumber":1,"value":"You search out my path and my lying down, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":3,"sectionNumber":2,"value":"and are acquainted with all my ways. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":4,"sectionNumber":1,"value":"For there is not a word on my tongue, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":4,"sectionNumber":2,"value":"but, behold, Yahweh, you know it altogether. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":5,"sectionNumber":1,"value":"You hem me in behind and before. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":5,"sectionNumber":2,"value":"You laid your hand on me. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":6,"sectionNumber":1,"value":"This knowledge is beyond me. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":6,"sectionNumber":2,"value":"It’s lofty. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":6,"sectionNumber":3,"value":"I can’t attain it. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":7,"sectionNumber":1,"value":"Where could I go from your Spirit? "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":7,"sectionNumber":2,"value":"Or where could I flee from your presence? "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":8,"sectionNumber":1,"value":"If I ascend up into heaven, you are there. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":8,"sectionNumber":2,"value":"If I make my bed in Sheol, behold, you are there! "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":9,"sectionNumber":1,"value":"If I take the wings of the dawn, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":9,"sectionNumber":2,"value":"and settle in the uttermost parts of the sea, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":10,"sectionNumber":1,"value":"even there your hand will lead me, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":10,"sectionNumber":2,"value":"and your right hand will hold me. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":11,"sectionNumber":1,"value":"If I say, “Surely the darkness will overwhelm me. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":11,"sectionNumber":2,"value":"The light around me will be night,” "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":12,"sectionNumber":1,"value":"even the darkness doesn’t hide from you, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":12,"sectionNumber":2,"value":"but the night shines as the day. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":12,"sectionNumber":3,"value":"The darkness is like light to you. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":13,"sectionNumber":1,"value":"For you formed my inmost being. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":13,"sectionNumber":2,"value":"You knit me together in my mother’s womb. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":14,"sectionNumber":1,"value":"I will give thanks to you, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":14,"sectionNumber":2,"value":"for I am fearfully and wonderfully made. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":14,"sectionNumber":3,"value":"Your works are wonderful. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":14,"sectionNumber":4,"value":"My soul knows that very well. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":15,"sectionNumber":1,"value":"My frame wasn’t hidden from you, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":15,"sectionNumber":2,"value":"when I was made in secret, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":15,"sectionNumber":3,"value":"woven together in the depths of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":16,"sectionNumber":1,"value":"Your eyes saw my body. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":16,"sectionNumber":2,"value":"In your book they were all written, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":16,"sectionNumber":3,"value":"the days that were ordained for me, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":16,"sectionNumber":4,"value":"when as yet there were none of them. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":17,"sectionNumber":1,"value":"How precious to me are your thoughts, God! "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":17,"sectionNumber":2,"value":"How vast is their sum! "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":18,"sectionNumber":1,"value":"If I would count them, they are more in number than the sand. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":18,"sectionNumber":2,"value":"When I wake up, I am still with you. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":19,"sectionNumber":1,"value":"If only you, God, would kill the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":19,"sectionNumber":2,"value":"Get away from me, you bloodthirsty men! "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":20,"sectionNumber":1,"value":"For they speak against you wickedly. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":20,"sectionNumber":2,"value":"Your enemies take your name in vain. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":21,"sectionNumber":1,"value":"Yahweh, don’t I hate those who hate you? "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":21,"sectionNumber":2,"value":"Am I not grieved with those who rise up against you? "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":22,"sectionNumber":1,"value":"I hate them with perfect hatred. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":22,"sectionNumber":2,"value":"They have become my enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":23,"sectionNumber":1,"value":"Search me, God, and know my heart. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":23,"sectionNumber":2,"value":"Try me, and know my thoughts. "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":24,"sectionNumber":1,"value":"See if there is any wicked way in me, "},{"type":"line break"},{"type":"line text","chapterNumber":139,"verseNumber":24,"sectionNumber":2,"value":"and lead me in the everlasting way. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]