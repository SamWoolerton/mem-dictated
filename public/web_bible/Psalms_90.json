[{"type":"line text","chapterNumber":90,"verseNumber":52,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":90,"verseNumber":1,"sectionNumber":1,"value":"Lord, you have been our dwelling place for all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":2,"sectionNumber":1,"value":"Before the mountains were born, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":2,"sectionNumber":2,"value":"before you had formed the earth and the world, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":2,"sectionNumber":3,"value":"even from everlasting to everlasting, you are God. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":3,"sectionNumber":1,"value":"You turn man to destruction, saying, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":3,"sectionNumber":2,"value":"“Return, you children of men.” "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":4,"sectionNumber":1,"value":"For a thousand years in your sight are just like yesterday when it is past, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":4,"sectionNumber":2,"value":"like a watch in the night. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":5,"sectionNumber":1,"value":"You sweep them away as they sleep. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":5,"sectionNumber":2,"value":"In the morning they sprout like new grass. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":6,"sectionNumber":1,"value":"In the morning it sprouts and springs up. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":6,"sectionNumber":2,"value":"By evening, it is withered and dry. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":7,"sectionNumber":1,"value":"For we are consumed in your anger. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":7,"sectionNumber":2,"value":"We are troubled in your wrath. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":8,"sectionNumber":1,"value":"You have set our iniquities before you, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":8,"sectionNumber":2,"value":"our secret sins in the light of your presence. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":9,"sectionNumber":1,"value":"For all our days have passed away in your wrath. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":9,"sectionNumber":2,"value":"We bring our years to an end as a sigh. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":10,"sectionNumber":1,"value":"The days of our years are seventy, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":10,"sectionNumber":2,"value":"or even by reason of strength eighty years; "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":10,"sectionNumber":3,"value":"yet their pride is but labor and sorrow, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":10,"sectionNumber":4,"value":"for it passes quickly, and we fly away. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":11,"sectionNumber":1,"value":"Who knows the power of your anger, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":11,"sectionNumber":2,"value":"your wrath according to the fear that is due to you? "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":12,"sectionNumber":1,"value":"So teach us to count our days, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":12,"sectionNumber":2,"value":"that we may gain a heart of wisdom. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":13,"sectionNumber":1,"value":"Relent, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":13,"sectionNumber":2,"value":"How long? "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":13,"sectionNumber":3,"value":"Have compassion on your servants! "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":14,"sectionNumber":1,"value":"Satisfy us in the morning with your loving kindness, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":14,"sectionNumber":2,"value":"that we may rejoice and be glad all our days. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":15,"sectionNumber":1,"value":"Make us glad for as many days as you have afflicted us, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":15,"sectionNumber":2,"value":"for as many years as we have seen evil. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":16,"sectionNumber":1,"value":"Let your work appear to your servants, "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":16,"sectionNumber":2,"value":"your glory to their children. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":17,"sectionNumber":1,"value":"Let the favor of the Lord our God be on us. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":17,"sectionNumber":2,"value":"Establish the work of our hands for us. "},{"type":"line break"},{"type":"line text","chapterNumber":90,"verseNumber":17,"sectionNumber":3,"value":"Yes, establish the work of our hands. "},{"type":"line break"}]