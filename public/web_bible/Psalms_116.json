[{"type":"line text","chapterNumber":116,"verseNumber":18,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":116,"verseNumber":1,"sectionNumber":1,"value":"I love Yahweh, because he listens to my voice, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":1,"sectionNumber":2,"value":"and my cries for mercy. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":2,"sectionNumber":1,"value":"Because he has turned his ear to me, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":2,"sectionNumber":2,"value":"therefore I will call on him as long as I live. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":3,"sectionNumber":1,"value":"The cords of death surrounded me, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":3,"sectionNumber":2,"value":"the pains of Sheol got a hold of me. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":3,"sectionNumber":3,"value":"I found trouble and sorrow. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":4,"sectionNumber":1,"value":"Then I called on Yahweh’s name: "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":4,"sectionNumber":2,"value":"“Yahweh, I beg you, deliver my soul.” "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":5,"sectionNumber":1,"value":"Yahweh is Gracious and righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":5,"sectionNumber":2,"value":"Yes, our God is merciful. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":6,"sectionNumber":1,"value":"Yahweh preserves the simple. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":6,"sectionNumber":2,"value":"I was brought low, and he saved me. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":7,"sectionNumber":1,"value":"Return to your rest, my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":7,"sectionNumber":2,"value":"for Yahweh has dealt bountifully with you. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":8,"sectionNumber":1,"value":"For you have delivered my soul from death, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":8,"sectionNumber":2,"value":"my eyes from tears, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":8,"sectionNumber":3,"value":"and my feet from falling. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":9,"sectionNumber":1,"value":"I will walk before Yahweh in the land of the living. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":10,"sectionNumber":1,"value":"I believed, therefore I said, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":10,"sectionNumber":2,"value":"“I was greatly afflicted.” "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":11,"sectionNumber":1,"value":"I said in my haste, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":11,"sectionNumber":2,"value":"“All people are liars.” "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":12,"sectionNumber":1,"value":"What will I give to Yahweh for all his benefits toward me? "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":13,"sectionNumber":1,"value":"I will take the cup of salvation, and call on Yahweh’s name. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":14,"sectionNumber":1,"value":"I will pay my vows to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":14,"sectionNumber":2,"value":"yes, in the presence of all his people. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":15,"sectionNumber":1,"value":"Precious in Yahweh’s sight is the death of his saints. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":16,"sectionNumber":1,"value":"Yahweh, truly I am your servant. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":16,"sectionNumber":2,"value":"I am your servant, the son of your servant girl. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":16,"sectionNumber":3,"value":"You have freed me from my chains. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":17,"sectionNumber":1,"value":"I will offer to you the sacrifice of thanksgiving, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":17,"sectionNumber":2,"value":"and will call on Yahweh’s name. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":18,"sectionNumber":1,"value":"I will pay my vows to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":18,"sectionNumber":2,"value":"yes, in the presence of all his people, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":19,"sectionNumber":1,"value":"in the courts of Yahweh’s house, "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":19,"sectionNumber":2,"value":"in the middle of you, Jerusalem. "},{"type":"line break"},{"type":"line text","chapterNumber":116,"verseNumber":19,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"}]