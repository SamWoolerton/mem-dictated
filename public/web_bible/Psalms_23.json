[{"type":"line text","chapterNumber":23,"verseNumber":31,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":23,"verseNumber":1,"sectionNumber":1,"value":"Yahweh is my shepherd: "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":1,"sectionNumber":2,"value":"I shall lack nothing. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":2,"sectionNumber":1,"value":"He makes me lie down in green pastures. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":2,"sectionNumber":2,"value":"He leads me beside still waters. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":3,"sectionNumber":1,"value":"He restores my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":3,"sectionNumber":2,"value":"He guides me in the paths of righteousness for his name’s sake. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":4,"sectionNumber":1,"value":"Even though I walk through the valley of the shadow of death, "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":4,"sectionNumber":2,"value":"I will fear no evil, for you are with me. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":4,"sectionNumber":3,"value":"Your rod and your staff, "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":4,"sectionNumber":4,"value":"they comfort me. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":5,"sectionNumber":1,"value":"You prepare a table before me "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":5,"sectionNumber":2,"value":"in the presence of my enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":5,"sectionNumber":3,"value":"You anoint my head with oil. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":5,"sectionNumber":4,"value":"My cup runs over. "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":6,"sectionNumber":1,"value":"Surely goodness and loving kindness shall follow me all the days of my life, "},{"type":"line break"},{"type":"line text","chapterNumber":23,"verseNumber":6,"sectionNumber":2,"value":"and I will dwell in Yahweh’s house forever. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by David."},{"type":"stanza start"}]