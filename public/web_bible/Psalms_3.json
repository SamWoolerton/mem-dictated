[{"type":"line text","chapterNumber":3,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":3,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, how my adversaries have increased! "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":1,"sectionNumber":2,"value":"Many are those who rise up against me. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":1,"value":"Many there are who say of my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":2,"value":"“There is no help for him in God.” "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":1,"value":"But you, Yahweh, are a shield around me, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":2,"value":"my glory, and the one who lifts up my head. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":1,"value":"I cry to Yahweh with my voice, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":2,"value":"and he answers me out of his holy hill. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":5,"sectionNumber":1,"value":"I laid myself down and slept. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":5,"sectionNumber":2,"value":"I awakened; for Yahweh sustains me. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":6,"sectionNumber":1,"value":"I will not be afraid of tens of thousands of people "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":6,"sectionNumber":2,"value":"who have set themselves against me on every side. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":1,"value":"Arise, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":2,"value":"Save me, my God! "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":3,"value":"For you have struck all of my enemies on the cheek bone. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":4,"value":"You have broken the teeth of the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":1,"value":"Salvation belongs to Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":2,"value":"May your blessing be on your people. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician; on stringed instruments. A Psalm by David."},{"type":"stanza start"}]