[{"type":"line text","chapterNumber":5,"verseNumber":27,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":1,"value":"My son, pay attention to my wisdom. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":2,"value":"Turn your ear to my understanding, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":1,"value":"that you may maintain discretion, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":2,"value":"that your lips may preserve knowledge. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":1,"value":"For the lips of an adulteress drip honey. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":2,"value":"Her mouth is smoother than oil, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":1,"value":"but in the end she is as bitter as wormwood, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":2,"value":"and as sharp as a two-edged sword. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":1,"value":"Her feet go down to death. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":2,"value":"Her steps lead straight to Sheol. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":1,"value":"She gives no thought to the way of life. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":2,"value":"Her ways are crooked, and she doesn’t know it. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":1,"value":"Now therefore, my sons, listen to me. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":2,"value":"Don’t depart from the words of my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":1,"value":"Remove your way far from her. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":2,"value":"Don’t come near the door of her house, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":1,"value":"lest you give your honor to others, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":2,"value":"and your years to the cruel one; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":1,"value":"lest strangers feast on your wealth, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":2,"value":"and your labors enrich another man’s house. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":1,"value":"You will groan at your latter end, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":2,"value":"when your flesh and your body are consumed, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":1,"value":"and say, “How I have hated instruction, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":2,"value":"and my heart despised reproof; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":1,"value":"neither have I obeyed the voice of my teachers, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":2,"value":"nor turned my ear to those who instructed me! "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":1,"value":"I have come to the brink of utter ruin, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":2,"value":"among the gathered assembly.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":1,"value":"Drink water out of your own cistern, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":2,"value":"running water out of your own well. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":1,"value":"Should your springs overflow in the streets, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":2,"value":"streams of water in the public squares? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":17,"sectionNumber":1,"value":"Let them be for yourself alone, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":17,"sectionNumber":2,"value":"not for strangers with you. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":18,"sectionNumber":1,"value":"Let your spring be blessed. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":18,"sectionNumber":2,"value":"Rejoice in the wife of your youth. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":19,"sectionNumber":1,"value":"A loving doe and a graceful deer— "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":19,"sectionNumber":2,"value":"let her breasts satisfy you at all times. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":19,"sectionNumber":3,"value":"Be captivated always with her love. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":20,"sectionNumber":1,"value":"For why should you, my son, be captivated with an adulteress? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":20,"sectionNumber":2,"value":"Why embrace the bosom of another? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":21,"sectionNumber":1,"value":"For the ways of man are before Yahweh’s eyes. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":21,"sectionNumber":2,"value":"He examines all his paths. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":22,"sectionNumber":1,"value":"The evil deeds of the wicked ensnare him. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":22,"sectionNumber":2,"value":"The cords of his sin hold him firmly. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":23,"sectionNumber":1,"value":"He will die for lack of instruction. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":23,"sectionNumber":2,"value":"In the greatness of his folly, he will go astray. "},{"type":"line break"}]