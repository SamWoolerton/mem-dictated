[{"type":"line text","chapterNumber":142,"verseNumber":10,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":142,"verseNumber":1,"sectionNumber":1,"value":"I cry with my voice to Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":1,"sectionNumber":2,"value":"With my voice, I ask Yahweh for mercy. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":2,"sectionNumber":1,"value":"I pour out my complaint before him. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":2,"sectionNumber":2,"value":"I tell him my troubles. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":3,"sectionNumber":1,"value":"When my spirit was overwhelmed within me, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":3,"sectionNumber":2,"value":"you knew my route. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":3,"sectionNumber":3,"value":"On the path in which I walk, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":3,"sectionNumber":4,"value":"they have hidden a snare for me. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":4,"sectionNumber":1,"value":"Look on my right, and see; "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":4,"sectionNumber":2,"value":"for there is no one who is concerned for me. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":4,"sectionNumber":3,"value":"Refuge has fled from me. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":4,"sectionNumber":4,"value":"No one cares for my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":5,"sectionNumber":1,"value":"I cried to you, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":5,"sectionNumber":2,"value":"I said, “You are my refuge, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":5,"sectionNumber":3,"value":"my portion in the land of the living.” "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":6,"sectionNumber":1,"value":"Listen to my cry, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":6,"sectionNumber":2,"value":"for I am in desperate need. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":6,"sectionNumber":3,"value":"Deliver me from my persecutors, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":6,"sectionNumber":4,"value":"for they are too strong for me. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":7,"sectionNumber":1,"value":"Bring my soul out of prison, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":7,"sectionNumber":2,"value":"that I may give thanks to your name. "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":7,"sectionNumber":3,"value":"The righteous will surround me, "},{"type":"line break"},{"type":"line text","chapterNumber":142,"verseNumber":7,"sectionNumber":4,"value":"for you will be good to me. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by David."},{"type":"stanza start"}]