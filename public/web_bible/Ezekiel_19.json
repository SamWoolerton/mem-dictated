[{"type":"paragraph text","chapterNumber":19,"verseNumber":1,"sectionNumber":1,"value":"“Moreover, take up a lamentation for the princes of Israel,  "},{"type":"paragraph text","chapterNumber":19,"verseNumber":2,"sectionNumber":1,"value":"and say, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":2,"value":"‘What was your mother? "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":3,"value":"A lioness. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":4,"value":"She couched among lions, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":5,"value":"in the middle of the young lions she nourished her cubs. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":1,"value":"She brought up one of her cubs. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":2,"value":"He became a young lion. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":3,"value":"He learned to catch the prey. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":4,"value":"He devoured men. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":1,"value":"The nations also heard of him. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":2,"value":"He was taken in their pit; "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":3,"value":"and they brought him with hooks to the land of Egypt. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":1,"value":"“ ‘Now when she saw that she had waited, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":2,"value":"and her hope was lost, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":3,"value":"then she took another of her cubs, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":4,"value":"and made him a young lion. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":1,"value":"He went up and down among the lions. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":2,"value":"He became a young lion. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":3,"value":"He learned to catch the prey. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":4,"value":"He devoured men. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":1,"value":"He knew their palaces, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":2,"value":"and laid waste their cities. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":3,"value":"The land was desolate, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":4,"value":"with its fullness, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":5,"value":"because of the noise of his roaring. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":8,"sectionNumber":1,"value":"Then the nations attacked him on every side from the provinces. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":8,"sectionNumber":2,"value":"They spread their net over him. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":8,"sectionNumber":3,"value":"He was taken in their pit. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":1,"value":"They put him in a cage with hooks, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":2,"value":"and brought him to the king of Babylon. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":3,"value":"They brought him into strongholds, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":4,"value":"so that his voice should no more be heard on the mountains of Israel. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":1,"value":"“ ‘Your mother was like a vine "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":2,"value":"in your blood, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":3,"value":"planted by the waters. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":4,"value":"It was fruitful and full of branches by reason of many waters. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":1,"value":"It had strong branches for the scepters of those who ruled. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":2,"value":"Their stature was exalted among the thick boughs. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":3,"value":"They were seen in their height "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":4,"value":"with the multitude of their branches. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":1,"value":"But it was plucked up in fury. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":2,"value":"It was cast down to the ground, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":3,"value":"and the east wind dried up its fruit. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":4,"value":"Its strong branches were broken off and withered. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":5,"value":"The fire consumed them. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":1,"value":"Now it is planted in the wilderness, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":2,"value":"in a dry and thirsty land. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":1,"value":"Fire has gone out of its branches. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":2,"value":"It has devoured its fruit, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":3,"value":"so that there is in it no strong branch to be a scepter to rule.’ "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":4,"value":"This is a lamentation, and shall be for a lamentation.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]