[{"type":"line text","chapterNumber":100,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":100,"verseNumber":1,"sectionNumber":1,"value":"Shout for joy to Yahweh, all you lands! "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":2,"sectionNumber":1,"value":"Serve Yahweh with gladness. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":2,"sectionNumber":2,"value":"Come before his presence with singing. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":3,"sectionNumber":1,"value":"Know that Yahweh, he is God. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":3,"sectionNumber":2,"value":"It is he who has made us, and we are his. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":3,"sectionNumber":3,"value":"We are his people, and the sheep of his pasture. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":4,"sectionNumber":1,"value":"Enter into his gates with thanksgiving, "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":4,"sectionNumber":2,"value":"and into his courts with praise. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":4,"sectionNumber":3,"value":"Give thanks to him, and bless his name. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":5,"sectionNumber":1,"value":"For Yahweh is good. "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":5,"sectionNumber":2,"value":"His loving kindness endures forever, "},{"type":"line break"},{"type":"line text","chapterNumber":100,"verseNumber":5,"sectionNumber":3,"value":"his faithfulness to all generations. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by David."},{"type":"stanza start"}]