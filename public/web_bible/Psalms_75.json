[{"type":"line text","chapterNumber":75,"verseNumber":23,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":75,"verseNumber":1,"sectionNumber":1,"value":"We give thanks to you, God. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":1,"sectionNumber":2,"value":"We give thanks, for your Name is near. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":1,"sectionNumber":3,"value":"Men tell about your wondrous works. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":75,"verseNumber":1,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":75,"verseNumber":2,"sectionNumber":1,"value":"When I choose the appointed time, "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":2,"sectionNumber":2,"value":"I will judge blamelessly. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":3,"sectionNumber":1,"value":"The earth and all its inhabitants quake. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":3,"sectionNumber":2,"value":"I firmly hold its pillars. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":4,"sectionNumber":1,"value":"I said to the arrogant, “Don’t boast!” "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":4,"sectionNumber":2,"value":"I said to the wicked, “Don’t lift up the horn. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":5,"sectionNumber":1,"value":"Don’t lift up your horn on high. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":5,"sectionNumber":2,"value":"Don’t speak with a stiff neck.” "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":6,"sectionNumber":1,"value":"For neither from the east, nor from the west, "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":6,"sectionNumber":2,"value":"nor yet from the south, comes exaltation. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":7,"sectionNumber":1,"value":"But God is the judge. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":7,"sectionNumber":2,"value":"He puts down one, and lifts up another. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":8,"sectionNumber":1,"value":"For in Yahweh’s hand there is a cup, "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":8,"sectionNumber":2,"value":"full of foaming wine mixed with spices. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":8,"sectionNumber":3,"value":"He pours it out. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":8,"sectionNumber":4,"value":"Indeed the wicked of the earth drink and drink it to its very dregs. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":75,"verseNumber":8,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":75,"verseNumber":9,"sectionNumber":1,"value":"But I will declare this forever: "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":9,"sectionNumber":2,"value":"I will sing praises to the God of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":10,"sectionNumber":1,"value":"I will cut off all the horns of the wicked, "},{"type":"line break"},{"type":"line text","chapterNumber":75,"verseNumber":10,"sectionNumber":2,"value":"but the horns of the righteous shall be lifted up. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. On stringed instruments. A Psalm by Asaph. A song."},{"type":"stanza start"}]