[{"type":"line text","chapterNumber":3,"verseNumber":32,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":3,"verseNumber":1,"sectionNumber":1,"value":"“For, behold, in those days, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":1,"sectionNumber":2,"value":"and in that time, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":1,"sectionNumber":3,"value":"when I restore the fortunes of Judah and Jerusalem, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":1,"value":"I will gather all nations, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":2,"value":"and will bring them down into the valley of Jehoshaphat; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":3,"value":"and I will execute judgment on them there for my people, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":4,"value":"and for my heritage, Israel, whom they have scattered among the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":2,"sectionNumber":5,"value":"They have divided my land, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":1,"value":"and have cast lots for my people, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":2,"value":"and have given a boy for a prostitute, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":3,"value":"and sold a girl for wine, that they may drink. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":3,"verseNumber":3,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":1,"value":"“Yes, and what are you to me, Tyre, and Sidon, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":2,"value":"and all the regions of Philistia? "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":3,"value":"Will you repay me? "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":4,"value":"And if you repay me, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":4,"sectionNumber":5,"value":"I will swiftly and speedily return your repayment on your own head. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":5,"sectionNumber":1,"value":"Because you have taken my silver and my gold, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":5,"sectionNumber":2,"value":"and have carried my finest treasures into your temples, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":6,"sectionNumber":1,"value":"and have sold the children of Judah and the children of Jerusalem to the sons of the Greeks, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":6,"sectionNumber":2,"value":"that you may remove them far from their border. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":1,"value":"Behold, I will stir them up out of the place where you have sold them, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":7,"sectionNumber":2,"value":"and will return your repayment on your own head; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":1,"value":"and I will sell your sons and your daughters into the hands of the children of Judah, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":2,"value":"and they will sell them to the men of Sheba, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":3,"value":"to a faraway nation, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":4,"value":"for Yahweh has spoken it.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":3,"verseNumber":8,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":3,"verseNumber":9,"sectionNumber":1,"value":"Proclaim this among the nations: "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":9,"sectionNumber":2,"value":"“Prepare for war! "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":9,"sectionNumber":3,"value":"Stir up the mighty men. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":9,"sectionNumber":4,"value":"Let all the warriors draw near. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":9,"sectionNumber":5,"value":"Let them come up.  "},{"type":"line text","chapterNumber":3,"verseNumber":10,"sectionNumber":1,"value":"Beat your plowshares into swords, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":10,"sectionNumber":2,"value":"and your pruning hooks into spears. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":10,"sectionNumber":3,"value":"Let the weak say, ‘I am strong.’ "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":11,"sectionNumber":1,"value":"Hurry and come, all you surrounding nations, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":11,"sectionNumber":2,"value":"and gather yourselves together.” "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":11,"sectionNumber":3,"value":"Cause your mighty ones to come down there, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":12,"sectionNumber":1,"value":"“Let the nations arouse themselves, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":12,"sectionNumber":2,"value":"and come up to the valley of Jehoshaphat; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":12,"sectionNumber":3,"value":"for there I will sit to judge all the surrounding nations. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":13,"sectionNumber":1,"value":"Put in the sickle; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":13,"sectionNumber":2,"value":"for the harvest is ripe. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":13,"sectionNumber":3,"value":"Come, tread, for the wine press is full, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":13,"sectionNumber":4,"value":"the vats overflow, for their wickedness is great.” "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":14,"sectionNumber":1,"value":"Multitudes, multitudes in the valley of decision! "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":14,"sectionNumber":2,"value":"For the day of Yahweh is near, in the valley of decision. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":15,"sectionNumber":1,"value":"The sun and the moon are darkened, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":15,"sectionNumber":2,"value":"and the stars withdraw their shining. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":16,"sectionNumber":1,"value":"Yahweh will roar from Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":16,"sectionNumber":2,"value":"and thunder from Jerusalem; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":16,"sectionNumber":3,"value":"and the heavens and the earth will shake; "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":16,"sectionNumber":4,"value":"but Yahweh will be a refuge to his people, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":16,"sectionNumber":5,"value":"and a stronghold to the children of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":17,"sectionNumber":1,"value":"“So you will know that I am Yahweh, your God, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":17,"sectionNumber":2,"value":"dwelling in Zion, my holy mountain. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":17,"sectionNumber":3,"value":"Then Jerusalem will be holy, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":17,"sectionNumber":4,"value":"and no strangers will pass through her any more. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":1,"value":"It will happen in that day, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":2,"value":"that the mountains will drop down sweet wine, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":3,"value":"the hills will flow with milk, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":4,"value":"all the brooks of Judah will flow with waters, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":5,"value":"and a fountain will flow out from Yahweh’s house, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":18,"sectionNumber":6,"value":"and will water the valley of Shittim. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":19,"sectionNumber":1,"value":"Egypt will be a desolation, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":19,"sectionNumber":2,"value":"and Edom will be a desolate wilderness, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":19,"sectionNumber":3,"value":"for the violence done to the children of Judah, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":19,"sectionNumber":4,"value":"because they have shed innocent blood in their land. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":20,"sectionNumber":1,"value":"But Judah will be inhabited forever, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":20,"sectionNumber":2,"value":"and Jerusalem from generation to generation. "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":21,"sectionNumber":1,"value":"I will cleanse their blood, "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":21,"sectionNumber":2,"value":"that I have not cleansed: "},{"type":"line break"},{"type":"line text","chapterNumber":3,"verseNumber":21,"sectionNumber":3,"value":"for Yahweh dwells in Zion.” "},{"type":"line break"}]