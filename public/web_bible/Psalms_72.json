[{"type":"line text","chapterNumber":72,"verseNumber":24,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":72,"verseNumber":1,"sectionNumber":1,"value":"God, give the king your justice; "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":1,"sectionNumber":2,"value":"your righteousness to the royal son. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":2,"sectionNumber":1,"value":"He will judge your people with righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":2,"sectionNumber":2,"value":"and your poor with justice. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":3,"sectionNumber":1,"value":"The mountains shall bring prosperity to the people. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":3,"sectionNumber":2,"value":"The hills bring the fruit of righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":4,"sectionNumber":1,"value":"He will judge the poor of the people. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":4,"sectionNumber":2,"value":"He will save the children of the needy, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":4,"sectionNumber":3,"value":"and will break the oppressor in pieces. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":5,"sectionNumber":1,"value":"They shall fear you while the sun endures; "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":5,"sectionNumber":2,"value":"and as long as the moon, throughout all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":6,"sectionNumber":1,"value":"He will come down like rain on the mown grass, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":6,"sectionNumber":2,"value":"as showers that water the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":7,"sectionNumber":1,"value":"In his days, the righteous shall flourish, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":7,"sectionNumber":2,"value":"and abundance of peace, until the moon is no more. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":8,"sectionNumber":1,"value":"He shall have dominion also from sea to sea, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":8,"sectionNumber":2,"value":"from the River to the ends of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":9,"sectionNumber":1,"value":"Those who dwell in the wilderness shall bow before him. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":9,"sectionNumber":2,"value":"His enemies shall lick the dust. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":10,"sectionNumber":1,"value":"The kings of Tarshish and of the islands will bring tribute. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":10,"sectionNumber":2,"value":"The kings of Sheba and Seba shall offer gifts. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":11,"sectionNumber":1,"value":"Yes, all kings shall fall down before him. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":11,"sectionNumber":2,"value":"All nations shall serve him. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":12,"sectionNumber":1,"value":"For he will deliver the needy when he cries; "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":12,"sectionNumber":2,"value":"the poor, who has no helper. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":13,"sectionNumber":1,"value":"He will have pity on the poor and needy. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":13,"sectionNumber":2,"value":"He will save the souls of the needy. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":14,"sectionNumber":1,"value":"He will redeem their soul from oppression and violence. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":14,"sectionNumber":2,"value":"Their blood will be precious in his sight. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":15,"sectionNumber":1,"value":"They shall live, and to him shall be given of the gold of Sheba. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":15,"sectionNumber":2,"value":"Men shall pray for him continually. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":15,"sectionNumber":3,"value":"They shall bless him all day long. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":16,"sectionNumber":1,"value":"Abundance of grain shall be throughout the land. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":16,"sectionNumber":2,"value":"Its fruit sways like Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":16,"sectionNumber":3,"value":"Let it flourish, thriving like the grass of the field. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":17,"sectionNumber":1,"value":"His name endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":17,"sectionNumber":2,"value":"His name continues as long as the sun. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":17,"sectionNumber":3,"value":"Men shall be blessed by him. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":17,"sectionNumber":4,"value":"All nations will call him blessed. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":72,"verseNumber":17,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":72,"verseNumber":18,"sectionNumber":1,"value":"Praise be to Yahweh God, the God of Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":18,"sectionNumber":2,"value":"who alone does marvelous deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":19,"sectionNumber":1,"value":"Blessed be his glorious name forever! "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":19,"sectionNumber":2,"value":"Let the whole earth be filled with his glory! "},{"type":"line break"},{"type":"line text","chapterNumber":72,"verseNumber":19,"sectionNumber":3,"value":"Amen and amen. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":72,"verseNumber":20,"sectionNumber":1,"value":"This ends the prayers by David, the son of Jesse. "},{"type":"paragraph end"},{"type":"header","value":"A Psalm by Asaph."},{"type":"stanza start"}]