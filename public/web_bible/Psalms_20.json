[{"type":"line text","chapterNumber":20,"verseNumber":14,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":20,"verseNumber":1,"sectionNumber":1,"value":"May Yahweh answer you in the day of trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":1,"sectionNumber":2,"value":"May the name of the God of Jacob set you up on high, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":2,"sectionNumber":1,"value":"send you help from the sanctuary, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":2,"sectionNumber":2,"value":"grant you support from Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":3,"sectionNumber":1,"value":"remember all your offerings, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":3,"sectionNumber":2,"value":"and accept your burned sacrifice. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":4,"sectionNumber":1,"value":"May he grant you your heartâ€™s desire, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":4,"sectionNumber":2,"value":"and fulfill all your counsel. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":5,"sectionNumber":1,"value":"We will triumph in your salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":5,"sectionNumber":2,"value":"In the name of our God, we will set up our banners. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":5,"sectionNumber":3,"value":"May Yahweh grant all your requests. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":6,"sectionNumber":1,"value":"Now I know that Yahweh saves his anointed. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":6,"sectionNumber":2,"value":"He will answer him from his holy heaven, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":6,"sectionNumber":3,"value":"with the saving strength of his right hand. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":7,"sectionNumber":1,"value":"Some trust in chariots, and some in horses, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":7,"sectionNumber":2,"value":"but we trust in the name of Yahweh our God. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":8,"sectionNumber":1,"value":"They are bowed down and fallen, "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":8,"sectionNumber":2,"value":"but we rise up, and stand upright. "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":9,"sectionNumber":1,"value":"Save, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":20,"verseNumber":9,"sectionNumber":2,"value":"Let the King answer us when we call! "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]