[{"type":"line text","chapterNumber":14,"verseNumber":6,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":14,"verseNumber":1,"sectionNumber":1,"value":"The fool has said in his heart, “There is no God.” "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":1,"sectionNumber":2,"value":"They are corrupt. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":1,"sectionNumber":3,"value":"They have done abominable deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":1,"sectionNumber":4,"value":"There is no one who does good. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":2,"sectionNumber":1,"value":"Yahweh looked down from heaven on the children of men, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":2,"sectionNumber":2,"value":"to see if there were any who understood, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":2,"sectionNumber":3,"value":"who sought after God. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":3,"sectionNumber":1,"value":"They have all gone aside. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":3,"sectionNumber":2,"value":"They have together become corrupt. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":3,"sectionNumber":3,"value":"There is no one who does good, no, not one. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":4,"sectionNumber":1,"value":"Have all the workers of iniquity no knowledge, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":4,"sectionNumber":2,"value":"who eat up my people as they eat bread, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":4,"sectionNumber":3,"value":"and don’t call on Yahweh? "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":5,"sectionNumber":1,"value":"There they were in great fear, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":5,"sectionNumber":2,"value":"for God is in the generation of the righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":6,"sectionNumber":1,"value":"You frustrate the plan of the poor, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":6,"sectionNumber":2,"value":"because Yahweh is his refuge. "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":7,"sectionNumber":1,"value":"Oh that the salvation of Israel would come out of Zion! "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":7,"sectionNumber":2,"value":"When Yahweh restores the fortunes of his people, "},{"type":"line break"},{"type":"line text","chapterNumber":14,"verseNumber":7,"sectionNumber":3,"value":"then Jacob shall rejoice, and Israel shall be glad. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by David."},{"type":"stanza start"}]