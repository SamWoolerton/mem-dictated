[{"type":"line text","chapterNumber":54,"verseNumber":6,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":54,"verseNumber":1,"sectionNumber":1,"value":"Save me, God, by your name. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":1,"sectionNumber":2,"value":"Vindicate me in your might. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":2,"sectionNumber":1,"value":"Hear my prayer, God. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":2,"sectionNumber":2,"value":"Listen to the words of my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":3,"sectionNumber":1,"value":"For strangers have risen up against me. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":3,"sectionNumber":2,"value":"Violent men have sought after my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":3,"sectionNumber":3,"value":"They havenâ€™t set God before them. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":4,"sectionNumber":1,"value":"Behold, God is my helper. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":4,"sectionNumber":2,"value":"The Lord is the one who sustains my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":5,"sectionNumber":1,"value":"He will repay the evil to my enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":5,"sectionNumber":2,"value":"Destroy them in your truth. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":6,"sectionNumber":1,"value":"With a free will offering, I will sacrifice to you. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":6,"sectionNumber":2,"value":"I will give thanks to your name, Yahweh, for it is good. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":7,"sectionNumber":1,"value":"For he has delivered me out of all trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":54,"verseNumber":7,"sectionNumber":2,"value":"My eye has seen triumph over my enemies. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. On stringed instruments. A contemplation by David."},{"type":"stanza start"}]