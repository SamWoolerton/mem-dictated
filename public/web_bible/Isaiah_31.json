[{"type":"line text","chapterNumber":31,"verseNumber":33,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":1,"value":"Woe to those who go down to Egypt for help, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":2,"value":"and rely on horses, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":3,"value":"and trust in chariots because they are many, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":4,"value":"and in horsemen because they are very strong, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":5,"value":"but they don’t look to the Holy One of Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":1,"sectionNumber":6,"value":"and they don’t seek Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":2,"sectionNumber":1,"value":"Yet he also is wise, and will bring disaster, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":2,"sectionNumber":2,"value":"and will not call back his words, but will arise against the house of the evildoers, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":2,"sectionNumber":3,"value":"and against the help of those who work iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":1,"value":"Now the Egyptians are men, and not God; "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":2,"value":"and their horses flesh, and not spirit. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":3,"value":"When Yahweh stretches out his hand, both he who helps shall stumble, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":4,"value":"and he who is helped shall fall, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":5,"value":"and they all shall be consumed together. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":31,"verseNumber":3,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":1,"value":"For Yahweh says to me, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":2,"value":"“As the lion and the young lion growling over his prey, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":3,"value":"if a multitude of shepherds is called together against him, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":4,"value":"will not be dismayed at their voice, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":5,"value":"nor abase himself for their noise, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":4,"sectionNumber":6,"value":"so Yahweh of Armies will come down to fight on Mount Zion and on its heights. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":5,"sectionNumber":1,"value":"As birds hovering, so Yahweh of Armies will protect Jerusalem. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":5,"sectionNumber":2,"value":"He will protect and deliver it. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":5,"sectionNumber":3,"value":"He will pass over and preserve it.” "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":6,"sectionNumber":1,"value":"Return to him from whom you have deeply revolted, children of Israel.  "},{"type":"line text","chapterNumber":31,"verseNumber":7,"sectionNumber":1,"value":"For in that day everyone shall cast away his idols of silver and his idols of gold—sin which your own hands have made for you. "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":31,"verseNumber":8,"sectionNumber":1,"value":"“The Assyrian will fall by the sword, not of man; "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":31,"verseNumber":8,"sectionNumber":2,"value":"and the sword, not of mankind, shall devour him. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":8,"sectionNumber":3,"value":"He will flee from the sword, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":8,"sectionNumber":4,"value":"and his young men will become subject to forced labor. "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":9,"sectionNumber":1,"value":"His rock will pass away by reason of terror, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":9,"sectionNumber":2,"value":"and his princes will be afraid of the banner,” "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":9,"sectionNumber":3,"value":"says Yahweh, whose fire is in Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":31,"verseNumber":9,"sectionNumber":4,"value":"and his furnace in Jerusalem. "},{"type":"line break"}]