[{"type":"line text","chapterNumber":9,"verseNumber":36,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":9,"verseNumber":1,"sectionNumber":1,"value":"Wisdom has built her house. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":1,"sectionNumber":2,"value":"She has carved out her seven pillars. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":2,"sectionNumber":1,"value":"She has prepared her meat. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":2,"sectionNumber":2,"value":"She has mixed her wine. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":2,"sectionNumber":3,"value":"She has also set her table. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":3,"sectionNumber":1,"value":"She has sent out her maidens. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":3,"sectionNumber":2,"value":"She cries from the highest places of the city: "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":4,"sectionNumber":1,"value":"“Whoever is simple, let him turn in here!” "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":4,"sectionNumber":2,"value":"As for him who is void of understanding, she says to him, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":5,"sectionNumber":1,"value":"“Come, eat some of my bread, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":5,"sectionNumber":2,"value":"Drink some of the wine which I have mixed! "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":6,"sectionNumber":1,"value":"Leave your simple ways, and live. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":6,"sectionNumber":2,"value":"Walk in the way of understanding.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":9,"verseNumber":6,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":9,"verseNumber":7,"sectionNumber":1,"value":"One who corrects a mocker invites insult. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":7,"sectionNumber":2,"value":"One who reproves a wicked man invites abuse. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":8,"sectionNumber":1,"value":"Don’t reprove a scoffer, lest he hate you. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":8,"sectionNumber":2,"value":"Reprove a wise person, and he will love you. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":9,"sectionNumber":1,"value":"Instruct a wise person, and he will be still wiser. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":9,"sectionNumber":2,"value":"Teach a righteous person, and he will increase in learning. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":10,"sectionNumber":1,"value":"The fear of Yahweh is the beginning of wisdom. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":10,"sectionNumber":2,"value":"The knowledge of the Holy One is understanding. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":11,"sectionNumber":1,"value":"For by me your days will be multiplied. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":11,"sectionNumber":2,"value":"The years of your life will be increased. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":12,"sectionNumber":1,"value":"If you are wise, you are wise for yourself. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":12,"sectionNumber":2,"value":"If you mock, you alone will bear it. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":9,"verseNumber":12,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":9,"verseNumber":13,"sectionNumber":1,"value":"The foolish woman is loud, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":13,"sectionNumber":2,"value":"undisciplined, and knows nothing. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":14,"sectionNumber":1,"value":"She sits at the door of her house, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":14,"sectionNumber":2,"value":"on a seat in the high places of the city, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":15,"sectionNumber":1,"value":"to call to those who pass by, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":15,"sectionNumber":2,"value":"who go straight on their ways, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":16,"sectionNumber":1,"value":"“Whoever is simple, let him turn in here.” "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":16,"sectionNumber":2,"value":"as for him who is void of understanding, she says to him, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":17,"sectionNumber":1,"value":"“Stolen water is sweet. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":17,"sectionNumber":2,"value":"Food eaten in secret is pleasant.” "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":18,"sectionNumber":1,"value":"But he doesn’t know that the departed spirits are there, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":18,"sectionNumber":2,"value":"that her guests are in the depths of Sheol. "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]