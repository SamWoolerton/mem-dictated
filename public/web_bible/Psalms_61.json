[{"type":"line text","chapterNumber":61,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":61,"verseNumber":1,"sectionNumber":1,"value":"Hear my cry, God. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":1,"sectionNumber":2,"value":"Listen to my prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":2,"sectionNumber":1,"value":"From the end of the earth, I will call to you when my heart is overwhelmed. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":2,"sectionNumber":2,"value":"Lead me to the rock that is higher than I. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":3,"sectionNumber":1,"value":"For you have been a refuge for me, "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":3,"sectionNumber":2,"value":"a strong tower from the enemy. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":4,"sectionNumber":1,"value":"I will dwell in your tent forever. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":4,"sectionNumber":2,"value":"I will take refuge in the shelter of your wings. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":5,"sectionNumber":1,"value":"For you, God, have heard my vows. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":5,"sectionNumber":2,"value":"You have given me the heritage of those who fear your name. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":6,"sectionNumber":1,"value":"You will prolong the king’s life. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":6,"sectionNumber":2,"value":"His years will be for generations. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":7,"sectionNumber":1,"value":"He shall be enthroned in God’s presence forever. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":7,"sectionNumber":2,"value":"Appoint your loving kindness and truth, that they may preserve him. "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":8,"sectionNumber":1,"value":"So I will sing praise to your name forever, "},{"type":"line break"},{"type":"line text","chapterNumber":61,"verseNumber":8,"sectionNumber":2,"value":"that I may fulfill my vows daily. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. To Jeduthun. A Psalm by David."},{"type":"stanza start"}]