[{"type":"line text","chapterNumber":131,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":131,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, my heart isnâ€™t arrogant, nor my eyes lofty; "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":1,"sectionNumber":2,"value":"nor do I concern myself with great matters, "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":1,"sectionNumber":3,"value":"or things too wonderful for me. "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":2,"sectionNumber":1,"value":"Surely I have stilled and quieted my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":2,"sectionNumber":2,"value":"like a weaned child with his mother, "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":2,"sectionNumber":3,"value":"like a weaned child is my soul within me. "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":3,"sectionNumber":1,"value":"Israel, hope in Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":131,"verseNumber":3,"sectionNumber":2,"value":"from this time forward and forever more. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]