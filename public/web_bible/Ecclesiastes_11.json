[{"type":"paragraph text","chapterNumber":11,"verseNumber":1,"sectionNumber":1,"value":"Cast your bread on the waters; "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":11,"verseNumber":1,"sectionNumber":2,"value":"for you shall find it after many days. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":2,"sectionNumber":1,"value":"Give a portion to seven, yes, even to eight; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":2,"sectionNumber":2,"value":"for you don’t know what evil will be on the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":3,"sectionNumber":1,"value":"If the clouds are full of rain, they empty themselves on the earth; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":3,"sectionNumber":2,"value":"and if a tree falls toward the south, or toward the north, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":3,"sectionNumber":3,"value":"in the place where the tree falls, there shall it be. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":4,"sectionNumber":1,"value":"He who observes the wind won’t sow; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":4,"sectionNumber":2,"value":"and he who regards the clouds won’t reap. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":5,"sectionNumber":1,"value":"As you don’t know what is the way of the wind, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":5,"sectionNumber":2,"value":"nor how the bones grow in the womb of her who is with child; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":5,"sectionNumber":3,"value":"even so you don’t know the work of God who does all. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":6,"sectionNumber":1,"value":"In the morning sow your seed, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":6,"sectionNumber":2,"value":"and in the evening don’t withhold your hand; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":6,"sectionNumber":3,"value":"for you don’t know which will prosper, whether this or that, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":6,"sectionNumber":4,"value":"or whether they both will be equally good. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":7,"sectionNumber":1,"value":"Truly the light is sweet, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":7,"sectionNumber":2,"value":"and it is a pleasant thing for the eyes to see the sun. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":8,"sectionNumber":1,"value":"Yes, if a man lives many years, let him rejoice in them all; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":8,"sectionNumber":2,"value":"but let him remember the days of darkness, for they shall be many. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":8,"sectionNumber":3,"value":"All that comes is vanity. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":9,"sectionNumber":1,"value":"Rejoice, young man, in your youth, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":9,"sectionNumber":2,"value":"and let your heart cheer you in the days of your youth, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":9,"sectionNumber":3,"value":"and walk in the ways of your heart, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":9,"sectionNumber":4,"value":"and in the sight of your eyes; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":9,"sectionNumber":5,"value":"but know that for all these things God will bring you into judgment. "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":10,"sectionNumber":1,"value":"Therefore remove sorrow from your heart, "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":10,"sectionNumber":2,"value":"and put away evil from your flesh; "},{"type":"line break"},{"type":"line text","chapterNumber":11,"verseNumber":10,"sectionNumber":3,"value":"for youth and the dawn of life are vanity. "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]