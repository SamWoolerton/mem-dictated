[{"type":"line text","chapterNumber":84,"verseNumber":18,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":84,"verseNumber":1,"sectionNumber":1,"value":"How lovely are your dwellings, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":1,"sectionNumber":2,"value":"Yahweh of Armies! "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":2,"sectionNumber":1,"value":"My soul longs, and even faints for the courts of Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":2,"sectionNumber":2,"value":"My heart and my flesh cry out for the living God. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":3,"sectionNumber":1,"value":"Yes, the sparrow has found a home, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":3,"sectionNumber":2,"value":"and the swallow a nest for herself, where she may have her young, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":3,"sectionNumber":3,"value":"near your altars, Yahweh of Armies, my King, and my God. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":4,"sectionNumber":1,"value":"Blessed are those who dwell in your house. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":4,"sectionNumber":2,"value":"They are always praising you. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":5,"sectionNumber":1,"value":"Blessed are those whose strength is in you, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":5,"sectionNumber":2,"value":"who have set their hearts on a pilgrimage. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":6,"sectionNumber":1,"value":"Passing through the valley of Weeping, they make it a place of springs. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":6,"sectionNumber":2,"value":"Yes, the autumn rain covers it with blessings. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":7,"sectionNumber":1,"value":"They go from strength to strength. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":7,"sectionNumber":2,"value":"Every one of them appears before God in Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":8,"sectionNumber":1,"value":"Yahweh, God of Armies, hear my prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":8,"sectionNumber":2,"value":"Listen, God of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":9,"sectionNumber":1,"value":"Behold, God our shield, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":9,"sectionNumber":2,"value":"look at the face of your anointed. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":10,"sectionNumber":1,"value":"For a day in your courts is better than a thousand. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":10,"sectionNumber":2,"value":"I would rather be a doorkeeper in the house of my God, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":10,"sectionNumber":3,"value":"than to dwell in the tents of wickedness. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":11,"sectionNumber":1,"value":"For Yahweh God is a sun and a shield. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":11,"sectionNumber":2,"value":"Yahweh will give grace and glory. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":11,"sectionNumber":3,"value":"He withholds no good thing from those who walk blamelessly. "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":12,"sectionNumber":1,"value":"Yahweh of Armies, "},{"type":"line break"},{"type":"line text","chapterNumber":84,"verseNumber":12,"sectionNumber":2,"value":"blessed is the man who trusts in you. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by the sons of Korah."},{"type":"stanza start"}]