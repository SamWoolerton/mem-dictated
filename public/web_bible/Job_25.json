[{"type":"paragraph text","chapterNumber":25,"verseNumber":1,"sectionNumber":1,"value":"Then Bildad the Shuhite answered, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":25,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":25,"verseNumber":2,"sectionNumber":1,"value":"“Dominion and fear are with him. "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":2,"sectionNumber":2,"value":"He makes peace in his high places. "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":3,"sectionNumber":1,"value":"Can his armies be counted? "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":3,"sectionNumber":2,"value":"On whom does his light not arise? "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":4,"sectionNumber":1,"value":"How then can man be just with God? "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":4,"sectionNumber":2,"value":"Or how can he who is born of a woman be clean? "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":5,"sectionNumber":1,"value":"Behold, even the moon has no brightness, "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":5,"sectionNumber":2,"value":"and the stars are not pure in his sight; "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":6,"sectionNumber":1,"value":"How much less man, who is a worm, "},{"type":"line break"},{"type":"line text","chapterNumber":25,"verseNumber":6,"sectionNumber":2,"value":"the son of man, who is a worm!” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]