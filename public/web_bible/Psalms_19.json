[{"type":"line text","chapterNumber":19,"verseNumber":50,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":19,"verseNumber":1,"sectionNumber":1,"value":"The heavens declare the glory of God. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":1,"sectionNumber":2,"value":"The expanse shows his handiwork. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":1,"value":"Day after day they pour out speech, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":2,"sectionNumber":2,"value":"and night after night they display knowledge. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":1,"value":"There is no speech nor language, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":3,"sectionNumber":2,"value":"where their voice is not heard. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":1,"value":"Their voice has gone out through all the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":2,"value":"their words to the end of the world. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":4,"sectionNumber":3,"value":"In them he has set a tent for the sun, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":1,"value":"which is as a bridegroom coming out of his room, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":5,"sectionNumber":2,"value":"like a strong man rejoicing to run his course. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":1,"value":"His going out is from the end of the heavens, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":2,"value":"his circuit to its ends. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":3,"value":"There is nothing hidden from its heat. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":19,"verseNumber":6,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":1,"value":"Yahweh’s law is perfect, restoring the soul. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":7,"sectionNumber":2,"value":"Yahweh’s covenant is sure, making wise the simple. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":8,"sectionNumber":1,"value":"Yahweh’s precepts are right, rejoicing the heart. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":8,"sectionNumber":2,"value":"Yahweh’s commandment is pure, enlightening the eyes. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":1,"value":"The fear of Yahweh is clean, enduring forever. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":9,"sectionNumber":2,"value":"Yahweh’s ordinances are true, and righteous altogether. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":1,"value":"They are more to be desired than gold, yes, than much fine gold, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":10,"sectionNumber":2,"value":"sweeter also than honey and the extract of the honeycomb. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":1,"value":"Moreover your servant is warned by them. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":11,"sectionNumber":2,"value":"In keeping them there is great reward. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":1,"value":"Who can discern his errors? "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":2,"value":"Forgive me from hidden errors. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":19,"verseNumber":12,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":1,"value":"Keep back your servant also from presumptuous sins. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":2,"value":"Let them not have dominion over me. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":3,"value":"Then I will be upright. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":13,"sectionNumber":4,"value":"I will be blameless and innocent of great transgression. "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":1,"value":"Let the words of my mouth and the meditation of my heart "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":2,"value":"be acceptable in your sight, "},{"type":"line break"},{"type":"line text","chapterNumber":19,"verseNumber":14,"sectionNumber":3,"value":"Yahweh, my rock, and my redeemer. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]