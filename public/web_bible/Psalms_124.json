[{"type":"line text","chapterNumber":124,"verseNumber":4,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":124,"verseNumber":1,"sectionNumber":1,"value":"If it had not been Yahweh who was on our side, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":1,"sectionNumber":2,"value":"let Israel now say, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":2,"sectionNumber":1,"value":"if it had not been Yahweh who was on our side, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":2,"sectionNumber":2,"value":"when men rose up against us; "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":3,"sectionNumber":1,"value":"then they would have swallowed us up alive, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":3,"sectionNumber":2,"value":"when their wrath was kindled against us; "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":4,"sectionNumber":1,"value":"then the waters would have overwhelmed us, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":4,"sectionNumber":2,"value":"the stream would have gone over our soul; "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":5,"sectionNumber":1,"value":"then the proud waters would have gone over our soul. "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":6,"sectionNumber":1,"value":"Blessed be Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":6,"sectionNumber":2,"value":"who has not given us as a prey to their teeth. "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":7,"sectionNumber":1,"value":"Our soul has escaped like a bird out of the fowler’s snare. "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":7,"sectionNumber":2,"value":"The snare is broken, and we have escaped. "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":8,"sectionNumber":1,"value":"Our help is in Yahweh’s name, "},{"type":"line break"},{"type":"line text","chapterNumber":124,"verseNumber":8,"sectionNumber":2,"value":"who made heaven and earth. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]