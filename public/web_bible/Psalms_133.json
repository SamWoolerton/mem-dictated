[{"type":"line text","chapterNumber":133,"verseNumber":18,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":133,"verseNumber":1,"sectionNumber":1,"value":"See how good and how pleasant it is "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":1,"sectionNumber":2,"value":"for brothers to live together in unity! "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":2,"sectionNumber":1,"value":"It is like the precious oil on the head, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":2,"sectionNumber":2,"value":"that ran down on the beard, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":2,"sectionNumber":3,"value":"even Aaronâ€™s beard, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":2,"sectionNumber":4,"value":"that came down on the edge of his robes, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":3,"sectionNumber":1,"value":"like the dew of Hermon, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":3,"sectionNumber":2,"value":"that comes down on the hills of Zion; "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":3,"sectionNumber":3,"value":"for there Yahweh gives the blessing, "},{"type":"line break"},{"type":"line text","chapterNumber":133,"verseNumber":3,"sectionNumber":4,"value":"even life forever more. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]