[{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":62,"verseNumber":1,"sectionNumber":1,"value":"For Zion’s sake I will not hold my peace, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":1,"sectionNumber":2,"value":"and for Jerusalem’s sake I will not rest, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":1,"sectionNumber":3,"value":"until her righteousness shines out like the dawn, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":1,"sectionNumber":4,"value":"and her salvation like a burning lamp. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":2,"sectionNumber":1,"value":"The nations will see your righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":2,"sectionNumber":2,"value":"and all kings your glory. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":2,"sectionNumber":3,"value":"You will be called by a new name, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":2,"sectionNumber":4,"value":"which Yahweh’s mouth will name. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":3,"sectionNumber":1,"value":"You will also be a crown of beauty in Yahweh‘s hand, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":3,"sectionNumber":2,"value":"and a royal diadem in your God’s hand. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":1,"value":"You will not be called Forsaken any more, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":2,"value":"nor will your land be called Desolate any more; "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":3,"value":"but you will be called Hephzibah, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":4,"value":"and your land Beulah; "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":5,"value":"for Yahweh delights in you, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":4,"sectionNumber":6,"value":"and your land will be married. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":5,"sectionNumber":1,"value":"For as a young man marries a virgin, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":5,"sectionNumber":2,"value":"so your sons will marry you. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":5,"sectionNumber":3,"value":"As a bridegroom rejoices over his bride, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":5,"sectionNumber":4,"value":"so your God will rejoice over you. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":62,"verseNumber":5,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":62,"verseNumber":6,"sectionNumber":1,"value":"I have set watchmen on your walls, Jerusalem. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":6,"sectionNumber":2,"value":"They will never be silent day nor night. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":6,"sectionNumber":3,"value":"You who call on Yahweh, take no rest, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":7,"sectionNumber":1,"value":"and give him no rest, until he establishes, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":7,"sectionNumber":2,"value":"and until he makes Jerusalem a praise in the earth. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":62,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":62,"verseNumber":8,"sectionNumber":1,"value":"Yahweh has sworn by his right hand, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":8,"sectionNumber":2,"value":"and by the arm of his strength, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":8,"sectionNumber":3,"value":"“Surely I will no more give your grain to be food for your enemies, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":8,"sectionNumber":4,"value":"and foreigners will not drink your new wine, for which you have labored, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":9,"sectionNumber":1,"value":"but those who have harvested it will eat it, and praise Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":9,"sectionNumber":2,"value":"Those who have gathered it will drink it in the courts of my sanctuary.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":62,"verseNumber":9,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":62,"verseNumber":10,"sectionNumber":1,"value":"Go through, go through the gates! "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":10,"sectionNumber":2,"value":"Prepare the way of the people! "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":10,"sectionNumber":3,"value":"Build up, build up the highway! "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":10,"sectionNumber":4,"value":"Gather out the stones! "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":10,"sectionNumber":5,"value":"Lift up a banner for the peoples. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":1,"value":"Behold, Yahweh has proclaimed to the end of the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":2,"value":"“Say to the daughter of Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":3,"value":"‘Behold, your salvation comes! "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":4,"value":"Behold, his reward is with him, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":11,"sectionNumber":5,"value":"and his recompense before him!’ ” "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":12,"sectionNumber":1,"value":"They will call them “The Holy People, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":12,"sectionNumber":2,"value":"Yahweh’s Redeemed”. "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":12,"sectionNumber":3,"value":"You will be called “Sought Out, "},{"type":"line break"},{"type":"line text","chapterNumber":62,"verseNumber":12,"sectionNumber":4,"value":"A City Not Forsaken”. "},{"type":"line break"}]