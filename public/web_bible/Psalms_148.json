[{"type":"line text","chapterNumber":148,"verseNumber":20,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":148,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":1,"sectionNumber":2,"value":"Praise Yahweh from the heavens! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":1,"sectionNumber":3,"value":"Praise him in the heights! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":2,"sectionNumber":1,"value":"Praise him, all his angels! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":2,"sectionNumber":2,"value":"Praise him, all his army! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":3,"sectionNumber":1,"value":"Praise him, sun and moon! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":3,"sectionNumber":2,"value":"Praise him, all you shining stars! "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":4,"sectionNumber":1,"value":"Praise him, you heavens of heavens, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":4,"sectionNumber":2,"value":"you waters that are above the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":5,"sectionNumber":1,"value":"Let them praise Yahweh’s name, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":5,"sectionNumber":2,"value":"for he commanded, and they were created. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":6,"sectionNumber":1,"value":"He has also established them forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":6,"sectionNumber":2,"value":"He has made a decree which will not pass away. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":7,"sectionNumber":1,"value":"Praise Yahweh from the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":7,"sectionNumber":2,"value":"you great sea creatures, and all depths; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":8,"sectionNumber":1,"value":"lightning and hail, snow and clouds; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":8,"sectionNumber":2,"value":"stormy wind, fulfilling his word; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":9,"sectionNumber":1,"value":"mountains and all hills; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":9,"sectionNumber":2,"value":"fruit trees and all cedars; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":10,"sectionNumber":1,"value":"wild animals and all livestock; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":10,"sectionNumber":2,"value":"small creatures and flying birds; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":11,"sectionNumber":1,"value":"kings of the earth and all peoples; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":11,"sectionNumber":2,"value":"princes and all judges of the earth; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":12,"sectionNumber":1,"value":"both young men and maidens; "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":12,"sectionNumber":2,"value":"old men and children: "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":13,"sectionNumber":1,"value":"let them praise Yahweh’s name, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":13,"sectionNumber":2,"value":"for his name alone is exalted. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":13,"sectionNumber":3,"value":"His glory is above the earth and the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":14,"sectionNumber":1,"value":"He has lifted up the horn of his people, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":14,"sectionNumber":2,"value":"the praise of all his saints, "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":14,"sectionNumber":3,"value":"even of the children of Israel, a people near to him. "},{"type":"line break"},{"type":"line text","chapterNumber":148,"verseNumber":14,"sectionNumber":4,"value":"Praise Yah! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]