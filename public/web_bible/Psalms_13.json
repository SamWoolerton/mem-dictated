[{"type":"line text","chapterNumber":13,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":13,"verseNumber":1,"sectionNumber":1,"value":"How long, Yahweh? "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":1,"sectionNumber":2,"value":"Will you forget me forever? "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":1,"sectionNumber":3,"value":"How long will you hide your face from me? "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":2,"sectionNumber":1,"value":"How long shall I take counsel in my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":2,"sectionNumber":2,"value":"having sorrow in my heart every day? "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":2,"sectionNumber":3,"value":"How long shall my enemy triumph over me? "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":3,"sectionNumber":1,"value":"Behold, and answer me, Yahweh, my God. "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":3,"sectionNumber":2,"value":"Give light to my eyes, lest I sleep in death; "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":4,"sectionNumber":1,"value":"lest my enemy say, “I have prevailed against him;” "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":4,"sectionNumber":2,"value":"lest my adversaries rejoice when I fall. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":13,"verseNumber":4,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":13,"verseNumber":5,"sectionNumber":1,"value":"But I trust in your loving kindness. "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":5,"sectionNumber":2,"value":"My heart rejoices in your salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":6,"sectionNumber":1,"value":"I will sing to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":13,"verseNumber":6,"sectionNumber":2,"value":"because he has been good to me. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. By David."},{"type":"stanza start"}]