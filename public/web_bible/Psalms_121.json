[{"type":"line text","chapterNumber":121,"verseNumber":7,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":121,"verseNumber":1,"sectionNumber":1,"value":"I will lift up my eyes to the hills. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":1,"sectionNumber":2,"value":"Where does my help come from? "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":2,"sectionNumber":1,"value":"My help comes from Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":2,"sectionNumber":2,"value":"who made heaven and earth. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":121,"verseNumber":2,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":121,"verseNumber":3,"sectionNumber":1,"value":"He will not allow your foot to be moved. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":3,"sectionNumber":2,"value":"He who keeps you will not slumber. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":4,"sectionNumber":1,"value":"Behold, he who keeps Israel "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":4,"sectionNumber":2,"value":"will neither slumber nor sleep. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":5,"sectionNumber":1,"value":"Yahweh is your keeper. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":5,"sectionNumber":2,"value":"Yahweh is your shade on your right hand. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":6,"sectionNumber":1,"value":"The sun will not harm you by day, "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":6,"sectionNumber":2,"value":"nor the moon by night. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":7,"sectionNumber":1,"value":"Yahweh will keep you from all evil. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":7,"sectionNumber":2,"value":"He will keep your soul. "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":8,"sectionNumber":1,"value":"Yahweh will keep your going out and your coming in, "},{"type":"line break"},{"type":"line text","chapterNumber":121,"verseNumber":8,"sectionNumber":2,"value":"from this time forward, and forever more. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents. By David."},{"type":"stanza start"}]