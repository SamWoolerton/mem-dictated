[{"type":"paragraph text","chapterNumber":33,"verseNumber":1,"sectionNumber":1,"value":"This is the blessing with which Moses the man of God blessed the children of Israel before his death.  "},{"type":"paragraph text","chapterNumber":33,"verseNumber":2,"sectionNumber":1,"value":"He said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":2,"value":"“Yahweh came from Sinai, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":3,"value":"and rose from Seir to them. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":4,"value":"He shone from Mount Paran. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":5,"value":"He came from the ten thousands of holy ones. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":6,"value":"At his right hand was a fiery law for them. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":1,"value":"Yes, he loves the people. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":2,"value":"All his saints are in your hand. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":3,"value":"They sat down at your feet. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":4,"value":"Each receives your words. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":4,"sectionNumber":1,"value":"Moses commanded us a law, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":4,"sectionNumber":2,"value":"an inheritance for the assembly of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":1,"value":"He was king in Jeshurun, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":2,"value":"when the heads of the people were gathered, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":3,"value":"all the tribes of Israel together. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":6,"sectionNumber":1,"value":"“Let Reuben live, and not die; "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":6,"sectionNumber":2,"value":"Nor let his men be few.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":7,"sectionNumber":1,"value":"This is for Judah. He said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":2,"value":"“Hear, Yahweh, the voice of Judah. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":3,"value":"Bring him in to his people. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":4,"value":"With his hands he contended for himself. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":5,"value":"You shall be a help against his adversaries.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":8,"sectionNumber":1,"value":"About Levi he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":8,"sectionNumber":2,"value":"“Your Thummim and your Urim are with your godly one, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":8,"sectionNumber":3,"value":"whom you proved at Massah, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":8,"sectionNumber":4,"value":"with whom you contended at the waters of Meribah. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":1,"value":"He said of his father, and of his mother, ‘I have not seen him.’ "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":2,"value":"He didn’t acknowledge his brothers, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":3,"value":"nor did he know his own children; "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":4,"value":"for they have observed your word, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":5,"value":"and keep your covenant. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":1,"value":"They shall teach Jacob your ordinances, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":2,"value":"and Israel your law. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":3,"value":"They shall put incense before you, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":4,"value":"and whole burnt offering on your altar. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":1,"value":"Yahweh, bless his skills. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":2,"value":"Accept the work of his hands. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":3,"value":"Strike through the hips of those who rise up against him, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":4,"value":"of those who hate him, that they not rise again.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":12,"sectionNumber":1,"value":"About Benjamin he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":2,"value":"“The beloved of Yahweh will dwell in safety by him. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":3,"value":"He covers him all day long. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":4,"value":"He dwells between his shoulders.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":13,"sectionNumber":1,"value":"About Joseph he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":13,"sectionNumber":2,"value":"“His land is blessed by Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":13,"sectionNumber":3,"value":"for the precious things of the heavens, for the dew, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":13,"sectionNumber":4,"value":"for the deep that couches beneath, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":14,"sectionNumber":1,"value":"for the precious things of the fruits of the sun, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":14,"sectionNumber":2,"value":"for the precious things that the moon can yield, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":15,"sectionNumber":1,"value":"for the best things of the ancient mountains, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":15,"sectionNumber":2,"value":"for the precious things of the everlasting hills, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":1,"value":"for the precious things of the earth and its fullness, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":2,"value":"the good will of him who lived in the bush. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":3,"value":"Let this come on the head of Joseph, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":4,"value":"on the crown of the head of him who was separated from his brothers. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":1,"value":"Majesty belongs to the firstborn of his herd. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":2,"value":"His horns are the horns of the wild ox. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":3,"value":"With them he will push all the peoples to the ends of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":4,"value":"They are the ten thousands of Ephraim. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":5,"value":"They are the thousands of Manasseh.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":18,"sectionNumber":1,"value":"About Zebulun he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":18,"sectionNumber":2,"value":"“Rejoice, Zebulun, in your going out; "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":18,"sectionNumber":3,"value":"and Issachar, in your tents. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":1,"value":"They will call the peoples to the mountain. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":2,"value":"There they will offer sacrifices of righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":3,"value":"for they will draw out the abundance of the seas, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":4,"value":"the hidden treasures of the sand.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":20,"sectionNumber":1,"value":"About Gad he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":20,"sectionNumber":2,"value":"“He who enlarges Gad is blessed. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":20,"sectionNumber":3,"value":"He dwells as a lioness, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":20,"sectionNumber":4,"value":"and tears the arm and the crown of the head. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":1,"value":"He provided the first part for himself, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":2,"value":"for the lawgiver’s portion reserved was reserved for him. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":3,"value":"He came with the heads of the people. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":4,"value":"He executed the righteousness of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":5,"value":"His ordinances with Israel.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":22,"sectionNumber":1,"value":"About Dan he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":2,"value":"“Dan is a lion’s cub "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":3,"value":"that leaps out of Bashan.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":23,"sectionNumber":1,"value":"About Naphtali he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":2,"value":"“Naphtali, satisfied with favor, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":3,"value":"full of Yahweh’s blessing, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":4,"value":"Possess the west and the south.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":33,"verseNumber":24,"sectionNumber":1,"value":"About Asher he said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":2,"value":"“Asher is blessed with children. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":3,"value":"Let him be acceptable to his brothers. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":4,"value":"Let him dip his foot in oil. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":25,"sectionNumber":1,"value":"Your bars will be iron and bronze. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":25,"sectionNumber":2,"value":"As your days, so your strength will be. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":25,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":1,"value":"“There is no one like God, Jeshurun, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":2,"value":"who rides on the heavens for your help, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":3,"value":"in his excellency on the skies. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":1,"value":"The eternal God is your dwelling place. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":2,"value":"Underneath are the everlasting arms. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":3,"value":"He thrust out the enemy from before you, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":4,"value":"and said, ‘Destroy!’ "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":1,"value":"Israel dwells in safety, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":2,"value":"the fountain of Jacob alone, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":3,"value":"In a land of grain and new wine. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":4,"value":"Yes, his heavens drop down dew. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":1,"value":"You are happy, Israel! "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":2,"value":"Who is like you, a people saved by Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":3,"value":"the shield of your help, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":4,"value":"the sword of your excellency? "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":5,"value":"Your enemies will submit themselves to you. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":6,"value":"You will tread on their high places.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]