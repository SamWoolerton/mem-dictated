[{"type":"line text","chapterNumber":8,"verseNumber":27,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":8,"verseNumber":1,"sectionNumber":1,"value":"Doesn’t wisdom cry out? "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":1,"sectionNumber":2,"value":"Doesn’t understanding raise her voice? "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":2,"sectionNumber":1,"value":"On the top of high places by the way, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":2,"sectionNumber":2,"value":"where the paths meet, she stands. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":3,"sectionNumber":1,"value":"Beside the gates, at the entry of the city, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":3,"sectionNumber":2,"value":"at the entry doors, she cries aloud: "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":4,"sectionNumber":1,"value":"“I call to you men! "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":4,"sectionNumber":2,"value":"I send my voice to the sons of mankind. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":5,"sectionNumber":1,"value":"You simple, understand prudence! "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":5,"sectionNumber":2,"value":"You fools, be of an understanding heart! "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":6,"sectionNumber":1,"value":"Hear, for I will speak excellent things. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":6,"sectionNumber":2,"value":"The opening of my lips is for right things. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":7,"sectionNumber":1,"value":"For my mouth speaks truth. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":7,"sectionNumber":2,"value":"Wickedness is an abomination to my lips. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":8,"sectionNumber":1,"value":"All the words of my mouth are in righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":8,"sectionNumber":2,"value":"There is nothing crooked or perverse in them. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":9,"sectionNumber":1,"value":"They are all plain to him who understands, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":9,"sectionNumber":2,"value":"right to those who find knowledge. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":10,"sectionNumber":1,"value":"Receive my instruction rather than silver, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":10,"sectionNumber":2,"value":"knowledge rather than choice gold. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":11,"sectionNumber":1,"value":"For wisdom is better than rubies. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":11,"sectionNumber":2,"value":"All the things that may be desired can’t be compared to it. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":8,"verseNumber":11,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":8,"verseNumber":12,"sectionNumber":1,"value":"“I, wisdom, have made prudence my dwelling. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":12,"sectionNumber":2,"value":"Find out knowledge and discretion. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":13,"sectionNumber":1,"value":"The fear of Yahweh is to hate evil. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":13,"sectionNumber":2,"value":"I hate pride, arrogance, the evil way, and the perverse mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":14,"sectionNumber":1,"value":"Counsel and sound knowledge are mine. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":14,"sectionNumber":2,"value":"I have understanding and power. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":15,"sectionNumber":1,"value":"By me kings reign, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":15,"sectionNumber":2,"value":"and princes decree justice. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":16,"sectionNumber":1,"value":"By me princes rule, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":16,"sectionNumber":2,"value":"nobles, and all the righteous rulers of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":17,"sectionNumber":1,"value":"I love those who love me. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":17,"sectionNumber":2,"value":"Those who seek me diligently will find me. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":18,"sectionNumber":1,"value":"With me are riches, honor, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":18,"sectionNumber":2,"value":"enduring wealth, and prosperity. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":19,"sectionNumber":1,"value":"My fruit is better than gold, yes, than fine gold, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":19,"sectionNumber":2,"value":"my yield than choice silver. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":20,"sectionNumber":1,"value":"I walk in the way of righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":20,"sectionNumber":2,"value":"in the middle of the paths of justice, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":21,"sectionNumber":1,"value":"that I may give wealth to those who love me. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":21,"sectionNumber":2,"value":"I fill their treasuries. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":8,"verseNumber":21,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":8,"verseNumber":22,"sectionNumber":1,"value":"“Yahweh possessed me in the beginning of his work, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":22,"sectionNumber":2,"value":"before his deeds of old. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":23,"sectionNumber":1,"value":"I was set up from everlasting, from the beginning, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":23,"sectionNumber":2,"value":"before the earth existed. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":24,"sectionNumber":1,"value":"When there were no depths, I was born, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":24,"sectionNumber":2,"value":"when there were no springs abounding with water. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":25,"sectionNumber":1,"value":"Before the mountains were settled in place, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":25,"sectionNumber":2,"value":"before the hills, I was born; "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":26,"sectionNumber":1,"value":"while as yet he had not made the earth, nor the fields, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":26,"sectionNumber":2,"value":"nor the beginning of the dust of the world. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":27,"sectionNumber":1,"value":"When he established the heavens, I was there. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":27,"sectionNumber":2,"value":"When he set a circle on the surface of the deep, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":28,"sectionNumber":1,"value":"when he established the clouds above, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":28,"sectionNumber":2,"value":"when the springs of the deep became strong, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":29,"sectionNumber":1,"value":"when he gave to the sea its boundary, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":29,"sectionNumber":2,"value":"that the waters should not violate his commandment, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":29,"sectionNumber":3,"value":"when he marked out the foundations of the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":30,"sectionNumber":1,"value":"then I was the craftsman by his side. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":30,"sectionNumber":2,"value":"I was a delight day by day, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":30,"sectionNumber":3,"value":"always rejoicing before him, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":31,"sectionNumber":1,"value":"rejoicing in his whole world. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":31,"sectionNumber":2,"value":"My delight was with the sons of men. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":8,"verseNumber":31,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":8,"verseNumber":32,"sectionNumber":1,"value":"“Now therefore, my sons, listen to me, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":32,"sectionNumber":2,"value":"for blessed are those who keep my ways. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":33,"sectionNumber":1,"value":"Hear instruction, and be wise. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":33,"sectionNumber":2,"value":"Don’t refuse it. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":34,"sectionNumber":1,"value":"Blessed is the man who hears me, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":34,"sectionNumber":2,"value":"watching daily at my gates, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":34,"sectionNumber":3,"value":"waiting at my door posts. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":35,"sectionNumber":1,"value":"For whoever finds me, finds life, "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":35,"sectionNumber":2,"value":"and will obtain favor from Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":36,"sectionNumber":1,"value":"But he who sins against me wrongs his own soul. "},{"type":"line break"},{"type":"line text","chapterNumber":8,"verseNumber":36,"sectionNumber":2,"value":"All those who hate me love death.” "},{"type":"line break"}]