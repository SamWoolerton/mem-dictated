[{"type":"line text","chapterNumber":10,"verseNumber":35,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":10,"verseNumber":1,"sectionNumber":1,"value":"“My soul is weary of my life. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":1,"sectionNumber":2,"value":"I will give free course to my complaint. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":1,"sectionNumber":3,"value":"I will speak in the bitterness of my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":2,"sectionNumber":1,"value":"I will tell God, ‘Do not condemn me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":2,"sectionNumber":2,"value":"Show me why you contend with me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":3,"sectionNumber":1,"value":"Is it good to you that you should oppress, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":3,"sectionNumber":2,"value":"that you should despise the work of your hands, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":3,"sectionNumber":3,"value":"and smile on the counsel of the wicked? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":4,"sectionNumber":1,"value":"Do you have eyes of flesh? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":4,"sectionNumber":2,"value":"Or do you see as man sees? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":5,"sectionNumber":1,"value":"Are your days as the days of mortals, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":5,"sectionNumber":2,"value":"or your years as man’s years, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":6,"sectionNumber":1,"value":"that you inquire after my iniquity, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":6,"sectionNumber":2,"value":"and search after my sin? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":7,"sectionNumber":1,"value":"Although you know that I am not wicked, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":7,"sectionNumber":2,"value":"there is no one who can deliver out of your hand. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":10,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":10,"verseNumber":8,"sectionNumber":1,"value":"“ ‘Your hands have framed me and fashioned me altogether, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":8,"sectionNumber":2,"value":"yet you destroy me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":9,"sectionNumber":1,"value":"Remember, I beg you, that you have fashioned me as clay. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":9,"sectionNumber":2,"value":"Will you bring me into dust again? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":10,"sectionNumber":1,"value":"Haven’t you poured me out like milk, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":10,"sectionNumber":2,"value":"and curdled me like cheese? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":11,"sectionNumber":1,"value":"You have clothed me with skin and flesh, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":11,"sectionNumber":2,"value":"and knit me together with bones and sinews. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":12,"sectionNumber":1,"value":"You have granted me life and loving kindness. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":12,"sectionNumber":2,"value":"Your visitation has preserved my spirit. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":13,"sectionNumber":1,"value":"Yet you hid these things in your heart. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":13,"sectionNumber":2,"value":"I know that this is with you: "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":14,"sectionNumber":1,"value":"if I sin, then you mark me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":14,"sectionNumber":2,"value":"You will not acquit me from my iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":15,"sectionNumber":1,"value":"If I am wicked, woe to me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":15,"sectionNumber":2,"value":"If I am righteous, I still will not lift up my head, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":15,"sectionNumber":3,"value":"being filled with disgrace, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":15,"sectionNumber":4,"value":"and conscious of my affliction. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":16,"sectionNumber":1,"value":"If my head is held high, you hunt me like a lion. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":16,"sectionNumber":2,"value":"Again you show yourself powerful to me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":17,"sectionNumber":1,"value":"You renew your witnesses against me, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":17,"sectionNumber":2,"value":"and increase your indignation on me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":17,"sectionNumber":3,"value":"Changes and warfare are with me. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":10,"verseNumber":17,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":10,"verseNumber":18,"sectionNumber":1,"value":"“ ‘Why, then, have you brought me out of the womb? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":18,"sectionNumber":2,"value":"I wish I had given up the spirit, and no eye had seen me. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":19,"sectionNumber":1,"value":"I should have been as though I had not been. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":19,"sectionNumber":2,"value":"I should have been carried from the womb to the grave. "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":20,"sectionNumber":1,"value":"Aren’t my days few? "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":20,"sectionNumber":2,"value":"Stop! "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":20,"sectionNumber":3,"value":"Leave me alone, that I may find a little comfort, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":21,"sectionNumber":1,"value":"before I go where I will not return from, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":21,"sectionNumber":2,"value":"to the land of darkness and of the shadow of death; "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":22,"sectionNumber":1,"value":"the land dark as midnight, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":22,"sectionNumber":2,"value":"of the shadow of death, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":22,"sectionNumber":3,"value":"without any order, "},{"type":"line break"},{"type":"line text","chapterNumber":10,"verseNumber":22,"sectionNumber":4,"value":"where the light is as midnight.’ ” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]