[{"type":"line text","chapterNumber":128,"verseNumber":5,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":128,"verseNumber":1,"sectionNumber":1,"value":"Blessed is everyone who fears Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":1,"sectionNumber":2,"value":"who walks in his ways. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":2,"sectionNumber":1,"value":"For you will eat the labor of your hands. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":2,"sectionNumber":2,"value":"You will be happy, and it will be well with you. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":3,"sectionNumber":1,"value":"Your wife will be as a fruitful vine in the innermost parts of your house, "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":3,"sectionNumber":2,"value":"your children like olive plants around your table. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":4,"sectionNumber":1,"value":"Behold, thus is the man blessed who fears Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":5,"sectionNumber":1,"value":"May Yahweh bless you out of Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":5,"sectionNumber":2,"value":"and may you see the good of Jerusalem all the days of your life. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":6,"sectionNumber":1,"value":"Yes, may you see your childrenâ€™s children. "},{"type":"line break"},{"type":"line text","chapterNumber":128,"verseNumber":6,"sectionNumber":2,"value":"Peace be upon Israel. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]