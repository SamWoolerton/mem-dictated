[{"type":"line text","chapterNumber":106,"verseNumber":45,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":106,"verseNumber":1,"sectionNumber":1,"value":"Praise Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":1,"sectionNumber":2,"value":"Give thanks to Yahweh, for he is good, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":1,"sectionNumber":3,"value":"for his loving kindness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":2,"sectionNumber":1,"value":"Who can utter the mighty acts of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":2,"sectionNumber":2,"value":"or fully declare all his praise? "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":3,"sectionNumber":1,"value":"Blessed are those who keep justice. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":3,"sectionNumber":2,"value":"Blessed is one who does what is right at all times. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":4,"sectionNumber":1,"value":"Remember me, Yahweh, with the favor that you show to your people. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":4,"sectionNumber":2,"value":"Visit me with your salvation, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":5,"sectionNumber":1,"value":"that I may see the prosperity of your chosen, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":5,"sectionNumber":2,"value":"that I may rejoice in the gladness of your nation, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":5,"sectionNumber":3,"value":"that I may glory with your inheritance. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":106,"verseNumber":5,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":106,"verseNumber":6,"sectionNumber":1,"value":"We have sinned with our fathers. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":6,"sectionNumber":2,"value":"We have committed iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":6,"sectionNumber":3,"value":"We have done wickedly. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":7,"sectionNumber":1,"value":"Our fathers didn’t understand your wonders in Egypt. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":7,"sectionNumber":2,"value":"They didn’t remember the multitude of your loving kindnesses, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":7,"sectionNumber":3,"value":"but were rebellious at the sea, even at the Red Sea. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":8,"sectionNumber":1,"value":"Nevertheless he saved them for his name’s sake, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":8,"sectionNumber":2,"value":"that he might make his mighty power known. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":9,"sectionNumber":1,"value":"He rebuked the Red Sea also, and it was dried up; "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":9,"sectionNumber":2,"value":"so he led them through the depths, as through a desert. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":10,"sectionNumber":1,"value":"He saved them from the hand of him who hated them, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":10,"sectionNumber":2,"value":"and redeemed them from the hand of the enemy. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":11,"sectionNumber":1,"value":"The waters covered their adversaries. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":11,"sectionNumber":2,"value":"There was not one of them left. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":12,"sectionNumber":1,"value":"Then they believed his words. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":12,"sectionNumber":2,"value":"They sang his praise. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":106,"verseNumber":12,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":106,"verseNumber":13,"sectionNumber":1,"value":"They soon forgot his works. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":13,"sectionNumber":2,"value":"They didn’t wait for his counsel, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":14,"sectionNumber":1,"value":"but gave in to craving in the desert, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":14,"sectionNumber":2,"value":"and tested God in the wasteland. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":15,"sectionNumber":1,"value":"He gave them their request, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":15,"sectionNumber":2,"value":"but sent leanness into their soul. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":16,"sectionNumber":1,"value":"They envied Moses also in the camp, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":16,"sectionNumber":2,"value":"and Aaron, Yahweh’s saint. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":17,"sectionNumber":1,"value":"The earth opened and swallowed up Dathan, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":17,"sectionNumber":2,"value":"and covered the company of Abiram. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":18,"sectionNumber":1,"value":"A fire was kindled in their company. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":18,"sectionNumber":2,"value":"The flame burned up the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":19,"sectionNumber":1,"value":"They made a calf in Horeb, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":19,"sectionNumber":2,"value":"and worshiped a molten image. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":20,"sectionNumber":1,"value":"Thus they exchanged their glory "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":20,"sectionNumber":2,"value":"for an image of a bull that eats grass. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":21,"sectionNumber":1,"value":"They forgot God, their Savior, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":21,"sectionNumber":2,"value":"who had done great things in Egypt, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":22,"sectionNumber":1,"value":"wondrous works in the land of Ham, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":22,"sectionNumber":2,"value":"and awesome things by the Red Sea. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":23,"sectionNumber":1,"value":"Therefore he said that he would destroy them, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":23,"sectionNumber":2,"value":"had Moses, his chosen, not stood before him in the breach, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":23,"sectionNumber":3,"value":"to turn away his wrath, so that he wouldn’t destroy them. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":24,"sectionNumber":1,"value":"Yes, they despised the pleasant land. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":24,"sectionNumber":2,"value":"They didn’t believe his word, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":25,"sectionNumber":1,"value":"but murmured in their tents, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":25,"sectionNumber":2,"value":"and didn’t listen to Yahweh’s voice. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":26,"sectionNumber":1,"value":"Therefore he swore to them "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":26,"sectionNumber":2,"value":"that he would overthrow them in the wilderness, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":27,"sectionNumber":1,"value":"that he would overthrow their offspring among the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":27,"sectionNumber":2,"value":"and scatter them in the lands. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":28,"sectionNumber":1,"value":"They joined themselves also to Baal Peor, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":28,"sectionNumber":2,"value":"and ate the sacrifices of the dead. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":29,"sectionNumber":1,"value":"Thus they provoked him to anger with their deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":29,"sectionNumber":2,"value":"The plague broke in on them. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":30,"sectionNumber":1,"value":"Then Phinehas stood up and executed judgment, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":30,"sectionNumber":2,"value":"so the plague was stopped. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":31,"sectionNumber":1,"value":"That was credited to him for righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":31,"sectionNumber":2,"value":"for all generations to come. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":32,"sectionNumber":1,"value":"They angered him also at the waters of Meribah, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":32,"sectionNumber":2,"value":"so that Moses was troubled for their sakes; "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":33,"sectionNumber":1,"value":"because they were rebellious against his spirit, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":33,"sectionNumber":2,"value":"he spoke rashly with his lips. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":34,"sectionNumber":1,"value":"They didn’t destroy the peoples, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":34,"sectionNumber":2,"value":"as Yahweh commanded them, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":35,"sectionNumber":1,"value":"but mixed themselves with the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":35,"sectionNumber":2,"value":"and learned their works. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":36,"sectionNumber":1,"value":"They served their idols, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":36,"sectionNumber":2,"value":"which became a snare to them. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":37,"sectionNumber":1,"value":"Yes, they sacrificed their sons and their daughters to demons. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":38,"sectionNumber":1,"value":"They shed innocent blood, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":38,"sectionNumber":2,"value":"even the blood of their sons and of their daughters, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":38,"sectionNumber":3,"value":"whom they sacrificed to the idols of Canaan. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":38,"sectionNumber":4,"value":"The land was polluted with blood. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":39,"sectionNumber":1,"value":"Thus they were defiled with their works, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":39,"sectionNumber":2,"value":"and prostituted themselves in their deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":40,"sectionNumber":1,"value":"Therefore Yahweh burned with anger against his people. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":40,"sectionNumber":2,"value":"He abhorred his inheritance. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":41,"sectionNumber":1,"value":"He gave them into the hand of the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":41,"sectionNumber":2,"value":"Those who hated them ruled over them. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":42,"sectionNumber":1,"value":"Their enemies also oppressed them. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":42,"sectionNumber":2,"value":"They were brought into subjection under their hand. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":43,"sectionNumber":1,"value":"He rescued them many times, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":43,"sectionNumber":2,"value":"but they were rebellious in their counsel, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":43,"sectionNumber":3,"value":"and were brought low in their iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":44,"sectionNumber":1,"value":"Nevertheless he regarded their distress, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":44,"sectionNumber":2,"value":"when he heard their cry. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":45,"sectionNumber":1,"value":"He remembered for them his covenant, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":45,"sectionNumber":2,"value":"and repented according to the multitude of his loving kindnesses. "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":46,"sectionNumber":1,"value":"He made them also to be pitied "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":46,"sectionNumber":2,"value":"by all those who carried them captive. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":106,"verseNumber":46,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":106,"verseNumber":47,"sectionNumber":1,"value":"Save us, Yahweh, our God, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":47,"sectionNumber":2,"value":"gather us from among the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":47,"sectionNumber":3,"value":"to give thanks to your holy name, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":47,"sectionNumber":4,"value":"to triumph in your praise! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":106,"verseNumber":47,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":106,"verseNumber":48,"sectionNumber":1,"value":"Blessed be Yahweh, the God of Israel, "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":48,"sectionNumber":2,"value":"from everlasting even to everlasting! "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":48,"sectionNumber":3,"value":"Let all the people say, “Amen.” "},{"type":"line break"},{"type":"line text","chapterNumber":106,"verseNumber":48,"sectionNumber":4,"value":"Praise Yah! "},{"type":"line break"}]