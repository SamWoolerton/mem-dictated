[{"type":"line text","chapterNumber":17,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":17,"verseNumber":1,"sectionNumber":1,"value":"“My spirit is consumed. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":1,"sectionNumber":2,"value":"My days are extinct, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":1,"sectionNumber":3,"value":"and the grave is ready for me. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":2,"sectionNumber":1,"value":"Surely there are mockers with me. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":2,"sectionNumber":2,"value":"My eye dwells on their provocation. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":17,"verseNumber":2,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":17,"verseNumber":3,"sectionNumber":1,"value":"“Now give a pledge. Be collateral for me with yourself. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":3,"sectionNumber":2,"value":"Who is there who will strike hands with me? "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":4,"sectionNumber":1,"value":"For you have hidden their heart from understanding, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":4,"sectionNumber":2,"value":"Therefore you will not exalt them. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":5,"sectionNumber":1,"value":"He who denounces his friends for plunder, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":5,"sectionNumber":2,"value":"Even the eyes of his children will fail. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":17,"verseNumber":5,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":17,"verseNumber":6,"sectionNumber":1,"value":"“But he has made me a byword of the people. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":6,"sectionNumber":2,"value":"They spit in my face. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":7,"sectionNumber":1,"value":"My eye also is dim by reason of sorrow. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":7,"sectionNumber":2,"value":"All my members are as a shadow. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":8,"sectionNumber":1,"value":"Upright men will be astonished at this. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":8,"sectionNumber":2,"value":"The innocent will stir himself up against the godless. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":9,"sectionNumber":1,"value":"Yet the righteous will hold to his way. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":9,"sectionNumber":2,"value":"He who has clean hands will grow stronger and stronger. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":10,"sectionNumber":1,"value":"But as for you all, come back. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":10,"sectionNumber":2,"value":"I will not find a wise man among you. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":11,"sectionNumber":1,"value":"My days are past. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":11,"sectionNumber":2,"value":"My plans are broken off, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":11,"sectionNumber":3,"value":"as are the thoughts of my heart. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":12,"sectionNumber":1,"value":"They change the night into day, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":12,"sectionNumber":2,"value":"saying ‘The light is near’ in the presence of darkness. "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":13,"sectionNumber":1,"value":"If I look for Sheol as my house, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":13,"sectionNumber":2,"value":"if I have spread my couch in the darkness, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":14,"sectionNumber":1,"value":"if I have said to corruption, ‘You are my father;’ "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":14,"sectionNumber":2,"value":"to the worm, ‘My mother,’ and ‘My sister,’ "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":15,"sectionNumber":1,"value":"where then is my hope? "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":15,"sectionNumber":2,"value":"as for my hope, who will see it? "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":16,"sectionNumber":1,"value":"Shall it go down with me to the gates of Sheol, "},{"type":"line break"},{"type":"line text","chapterNumber":17,"verseNumber":16,"sectionNumber":2,"value":"or descend together into the dust?” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"paragraph start"}]