[{"type":"paragraph text","chapterNumber":26,"verseNumber":1,"sectionNumber":1,"value":"Then Job answered, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":26,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":26,"verseNumber":2,"sectionNumber":1,"value":"“How have you helped him who is without power! "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":2,"sectionNumber":2,"value":"How have you saved the arm that has no strength! "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":3,"sectionNumber":1,"value":"How have you counseled him who has no wisdom, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":3,"sectionNumber":2,"value":"and plentifully declared sound knowledge! "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":4,"sectionNumber":1,"value":"To whom have you uttered words? "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":4,"sectionNumber":2,"value":"Whose spirit came out of you? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":26,"verseNumber":4,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":26,"verseNumber":5,"sectionNumber":1,"value":"“The departed spirits tremble, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":5,"sectionNumber":2,"value":"those beneath the waters and all that live in them. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":6,"sectionNumber":1,"value":"Sheol is naked before God, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":6,"sectionNumber":2,"value":"and Abaddon has no covering. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":7,"sectionNumber":1,"value":"He stretches out the north over empty space, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":7,"sectionNumber":2,"value":"and hangs the earth on nothing. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":8,"sectionNumber":1,"value":"He binds up the waters in his thick clouds, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":8,"sectionNumber":2,"value":"and the cloud is not burst under them. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":9,"sectionNumber":1,"value":"He encloses the face of his throne, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":9,"sectionNumber":2,"value":"and spreads his cloud on it. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":10,"sectionNumber":1,"value":"He has described a boundary on the surface of the waters, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":10,"sectionNumber":2,"value":"and to the confines of light and darkness. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":11,"sectionNumber":1,"value":"The pillars of heaven tremble "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":11,"sectionNumber":2,"value":"and are astonished at his rebuke. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":12,"sectionNumber":1,"value":"He stirs up the sea with his power, "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":12,"sectionNumber":2,"value":"and by his understanding he strikes through Rahab. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":13,"sectionNumber":1,"value":"By his Spirit the heavens are garnished. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":13,"sectionNumber":2,"value":"His hand has pierced the swift serpent. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":14,"sectionNumber":1,"value":"Behold, these are but the outskirts of his ways. "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":14,"sectionNumber":2,"value":"How small a whisper do we hear of him! "},{"type":"line break"},{"type":"line text","chapterNumber":26,"verseNumber":14,"sectionNumber":3,"value":"But the thunder of his power who can understand?” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]