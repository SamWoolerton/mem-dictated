[{"type":"line text","chapterNumber":129,"verseNumber":6,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":129,"verseNumber":1,"sectionNumber":1,"value":"Many times they have afflicted me from my youth up. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":1,"sectionNumber":2,"value":"Let Israel now say, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":2,"sectionNumber":1,"value":"many times they have afflicted me from my youth up, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":2,"sectionNumber":2,"value":"yet they have not prevailed against me. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":3,"sectionNumber":1,"value":"The plowers plowed on my back. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":3,"sectionNumber":2,"value":"They made their furrows long. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":4,"sectionNumber":1,"value":"Yahweh is righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":4,"sectionNumber":2,"value":"He has cut apart the cords of the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":5,"sectionNumber":1,"value":"Let them be disappointed and turned backward, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":5,"sectionNumber":2,"value":"all those who hate Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":6,"sectionNumber":1,"value":"Let them be as the grass on the housetops, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":6,"sectionNumber":2,"value":"which withers before it grows up, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":7,"sectionNumber":1,"value":"with which the reaper doesn’t fill his hand, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":7,"sectionNumber":2,"value":"nor he who binds sheaves, his bosom. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":8,"sectionNumber":1,"value":"Neither do those who go by say, "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":8,"sectionNumber":2,"value":"“The blessing of Yahweh be on you. "},{"type":"line break"},{"type":"line text","chapterNumber":129,"verseNumber":8,"sectionNumber":3,"value":"We bless you in Yahweh’s name.” "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]