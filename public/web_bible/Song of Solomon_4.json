[{"type":"paragraph text","chapterNumber":4,"verseNumber":1,"sectionNumber":1,"value":"Behold, you are beautiful, my love. "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":2,"value":"Behold, you are beautiful. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":3,"value":"Your eyes are like doves behind your veil. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":4,"value":"Your hair is as a flock of goats, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":5,"value":"that descend from Mount Gilead. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":1,"value":"Your teeth are like a newly shorn flock, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":2,"value":"which have come up from the washing, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":3,"value":"where every one of them has twins. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":4,"value":"None is bereaved among them. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":1,"value":"Your lips are like scarlet thread. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":2,"value":"Your mouth is lovely. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":3,"value":"Your temples are like a piece of a pomegranate behind your veil. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":1,"value":"Your neck is like David’s tower built for an armory, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":2,"value":"on which a thousand shields hang, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":3,"value":"all the shields of the mighty men. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":1,"value":"Your two breasts are like two fawns "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":2,"value":"that are twins of a roe, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":3,"value":"which feed among the lilies. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":1,"value":"Until the day is cool, and the shadows flee away, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":2,"value":"I will go to the mountain of myrrh, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":3,"value":"to the hill of frankincense. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":1,"value":"You are all beautiful, my love. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":2,"value":"There is no spot in you. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":1,"value":"Come with me from Lebanon, my bride, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":2,"value":"with me from Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":3,"value":"Look from the top of Amana, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":4,"value":"from the top of Senir and Hermon, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":5,"value":"from the lions’ dens, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":6,"value":"from the mountains of the leopards. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":7,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":1,"value":"You have ravished my heart, my sister, my bride. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":2,"value":"You have ravished my heart with one of your eyes, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":3,"value":"with one chain of your neck. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":1,"value":"How beautiful is your love, my sister, my bride! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":2,"value":"How much better is your love than wine, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":3,"value":"the fragrance of your perfumes than all kinds of spices! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":1,"value":"Your lips, my bride, drip like the honeycomb. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":2,"value":"Honey and milk are under your tongue. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":3,"value":"The smell of your garments is like the smell of Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":1,"value":"My sister, my bride, is a locked up garden; "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":2,"value":"a locked up spring, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":3,"value":"a sealed fountain. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":1,"value":"Your shoots are an orchard of pomegranates, with precious fruits, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":2,"value":"henna with spikenard plants, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":1,"value":"spikenard and saffron, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":2,"value":"calamus and cinnamon, with every kind of incense tree; "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":3,"value":"myrrh and aloes, with all the best spices, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":1,"value":"a fountain of gardens, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":2,"value":"a well of living waters, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":3,"value":"flowing streams from Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":1,"value":"Awake, north wind, and come, you south! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":2,"value":"Blow on my garden, that its spices may flow out. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":3,"value":"Let my beloved come into his garden, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":4,"value":"and taste his precious fruits. "},{"type":"line break"}]