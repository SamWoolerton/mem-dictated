[{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":1,"verseNumber":1,"sectionNumber":1,"value":"The Song of songs, which is Solomon’s. "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":1,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":1,"verseNumber":2,"sectionNumber":1,"value":"Let him kiss me with the kisses of his mouth; "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":2,"sectionNumber":2,"value":"for your love is better than wine. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":1,"value":"Your oils have a pleasing fragrance. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":2,"value":"Your name is oil poured out, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":3,"value":"therefore the virgins love you. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":1,"value":"Take me away with you. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":2,"value":"Let’s hurry. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":3,"value":"The king has brought me into his rooms. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":4,"value":"We will be glad and rejoice in you. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":5,"value":"We will praise your love more than wine! "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":6,"value":"They are right to love you. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":1,"value":"I am dark, but lovely, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":2,"value":"you daughters of Jerusalem, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":3,"value":"like Kedar’s tents, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":4,"value":"like Solomon’s curtains. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":1,"value":"Don’t stare at me because I am dark, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":2,"value":"because the sun has scorched me. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":3,"value":"My mother’s sons were angry with me. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":4,"value":"They made me keeper of the vineyards. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":5,"value":"I haven’t kept my own vineyard. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":7,"sectionNumber":1,"value":"Tell me, you whom my soul loves, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":7,"sectionNumber":2,"value":"where you graze your flock, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":7,"sectionNumber":3,"value":"where you rest them at noon; "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":7,"sectionNumber":4,"value":"for why should I be as one who is veiled "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":7,"sectionNumber":5,"value":"beside the flocks of your companions? "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":8,"sectionNumber":1,"value":"If you don’t know, most beautiful among women, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":8,"sectionNumber":2,"value":"follow the tracks of the sheep. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":8,"sectionNumber":3,"value":"Graze your young goats beside the shepherds’ tents. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":1,"verseNumber":8,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":1,"verseNumber":9,"sectionNumber":1,"value":"I have compared you, my love, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":9,"sectionNumber":2,"value":"to a steed in Pharaoh’s chariots. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":10,"sectionNumber":1,"value":"Your cheeks are beautiful with earrings, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":10,"sectionNumber":2,"value":"your neck with strings of jewels. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":11,"sectionNumber":1,"value":"We will make you earrings of gold, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":11,"sectionNumber":2,"value":"with studs of silver. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":12,"sectionNumber":1,"value":"While the king sat at his table, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":12,"sectionNumber":2,"value":"my perfume spread its fragrance. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":13,"sectionNumber":1,"value":"My beloved is to me a sachet of myrrh, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":13,"sectionNumber":2,"value":"that lies between my breasts. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":14,"sectionNumber":1,"value":"My beloved is to me a cluster of henna blossoms "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":14,"sectionNumber":2,"value":"from the vineyards of En Gedi. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":15,"sectionNumber":1,"value":"Behold, you are beautiful, my love. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":15,"sectionNumber":2,"value":"Behold, you are beautiful. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":15,"sectionNumber":3,"value":"Your eyes are like doves. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":16,"sectionNumber":1,"value":"Behold, you are beautiful, my beloved, yes, pleasant; "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":16,"sectionNumber":2,"value":"and our couch is verdant. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":17,"sectionNumber":1,"value":"The beams of our house are cedars. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":17,"sectionNumber":2,"value":"Our rafters are firs. "},{"type":"line break"}]