[{"type":"line text","chapterNumber":120,"verseNumber":176,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":120,"verseNumber":1,"sectionNumber":1,"value":"In my distress, I cried to Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":1,"sectionNumber":2,"value":"He answered me. "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":2,"sectionNumber":1,"value":"Deliver my soul, Yahweh, from lying lips, "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":2,"sectionNumber":2,"value":"from a deceitful tongue. "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":3,"sectionNumber":1,"value":"What will be given to you, and what will be done more to you, "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":3,"sectionNumber":2,"value":"you deceitful tongue? "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":4,"sectionNumber":1,"value":"Sharp arrows of the mighty, "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":4,"sectionNumber":2,"value":"with coals of juniper. "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":5,"sectionNumber":1,"value":"Woe is me, that I live in Meshech, "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":5,"sectionNumber":2,"value":"that I dwell among the tents of Kedar! "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":6,"sectionNumber":1,"value":"My soul has had her dwelling too long "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":6,"sectionNumber":2,"value":"with him who hates peace. "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":7,"sectionNumber":1,"value":"I am for peace, "},{"type":"line break"},{"type":"line text","chapterNumber":120,"verseNumber":7,"sectionNumber":2,"value":"but when I speak, they are for war. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents."},{"type":"stanza start"}]