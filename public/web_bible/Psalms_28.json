[{"type":"line text","chapterNumber":28,"verseNumber":14,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":28,"verseNumber":1,"sectionNumber":1,"value":"To you, Yahweh, I call. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":1,"sectionNumber":2,"value":"My rock, don’t be deaf to me, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":1,"sectionNumber":3,"value":"lest, if you are silent to me, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":1,"sectionNumber":4,"value":"I would become like those who go down into the pit. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":2,"sectionNumber":1,"value":"Hear the voice of my petitions, when I cry to you, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":2,"sectionNumber":2,"value":"when I lift up my hands toward your Most Holy Place. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":3,"sectionNumber":1,"value":"Don’t draw me away with the wicked, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":3,"sectionNumber":2,"value":"with the workers of iniquity who speak peace with their neighbors, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":3,"sectionNumber":3,"value":"but mischief is in their hearts. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":4,"sectionNumber":1,"value":"Give them according to their work, and according to the wickedness of their doings. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":4,"sectionNumber":2,"value":"Give them according to the operation of their hands. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":4,"sectionNumber":3,"value":"Bring back on them what they deserve. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":5,"sectionNumber":1,"value":"Because they don’t respect the works of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":5,"sectionNumber":2,"value":"nor the operation of his hands, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":5,"sectionNumber":3,"value":"he will break them down and not build them up. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":28,"verseNumber":5,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":28,"verseNumber":6,"sectionNumber":1,"value":"Blessed be Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":6,"sectionNumber":2,"value":"because he has heard the voice of my petitions. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":7,"sectionNumber":1,"value":"Yahweh is my strength and my shield. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":7,"sectionNumber":2,"value":"My heart has trusted in him, and I am helped. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":7,"sectionNumber":3,"value":"Therefore my heart greatly rejoices. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":7,"sectionNumber":4,"value":"With my song I will thank him. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":8,"sectionNumber":1,"value":"Yahweh is their strength. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":8,"sectionNumber":2,"value":"He is a stronghold of salvation to his anointed. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":9,"sectionNumber":1,"value":"Save your people, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":9,"sectionNumber":2,"value":"and bless your inheritance. "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":9,"sectionNumber":3,"value":"Be their shepherd also, "},{"type":"line break"},{"type":"line text","chapterNumber":28,"verseNumber":9,"sectionNumber":4,"value":"and bear them up forever. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm by David."},{"type":"stanza start"}]