[{"type":"line text","chapterNumber":135,"verseNumber":3,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":135,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":1,"sectionNumber":2,"value":"Praise Yahweh’s name! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":1,"sectionNumber":3,"value":"Praise him, you servants of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":2,"sectionNumber":1,"value":"you who stand in Yahweh’s house, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":2,"sectionNumber":2,"value":"in the courts of our God’s house. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":3,"sectionNumber":1,"value":"Praise Yah, for Yahweh is good. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":3,"sectionNumber":2,"value":"Sing praises to his name, for that is pleasant. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":4,"sectionNumber":1,"value":"For Yah has chosen Jacob for himself, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":4,"sectionNumber":2,"value":"Israel for his own possession. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":5,"sectionNumber":1,"value":"For I know that Yahweh is great, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":5,"sectionNumber":2,"value":"that our Lord is above all gods. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":6,"sectionNumber":1,"value":"Whatever Yahweh pleased, that he has done, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":6,"sectionNumber":2,"value":"in heaven and in earth, in the seas and in all deeps. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":7,"sectionNumber":1,"value":"He causes the clouds to rise from the ends of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":7,"sectionNumber":2,"value":"He makes lightnings with the rain. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":7,"sectionNumber":3,"value":"He brings the wind out of his treasuries. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":8,"sectionNumber":1,"value":"He struck the firstborn of Egypt, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":8,"sectionNumber":2,"value":"both of man and animal. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":9,"sectionNumber":1,"value":"He sent signs and wonders into the middle of you, Egypt, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":9,"sectionNumber":2,"value":"on Pharaoh, and on all his servants. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":10,"sectionNumber":1,"value":"He struck many nations, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":10,"sectionNumber":2,"value":"and killed mighty kings, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":11,"sectionNumber":1,"value":"Sihon king of the Amorites, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":11,"sectionNumber":2,"value":"Og king of Bashan, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":11,"sectionNumber":3,"value":"and all the kingdoms of Canaan, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":12,"sectionNumber":1,"value":"and gave their land for a heritage, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":12,"sectionNumber":2,"value":"a heritage to Israel, his people. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":13,"sectionNumber":1,"value":"Your name, Yahweh, endures forever; "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":13,"sectionNumber":2,"value":"your renown, Yahweh, throughout all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":14,"sectionNumber":1,"value":"For Yahweh will judge his people "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":14,"sectionNumber":2,"value":"and have compassion on his servants. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":135,"verseNumber":14,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":135,"verseNumber":15,"sectionNumber":1,"value":"The idols of the nations are silver and gold, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":15,"sectionNumber":2,"value":"the work of men’s hands. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":16,"sectionNumber":1,"value":"They have mouths, but they can’t speak. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":16,"sectionNumber":2,"value":"They have eyes, but they can’t see. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":17,"sectionNumber":1,"value":"They have ears, but they can’t hear, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":17,"sectionNumber":2,"value":"neither is there any breath in their mouths. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":18,"sectionNumber":1,"value":"Those who make them will be like them, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":18,"sectionNumber":2,"value":"yes, everyone who trusts in them. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":19,"sectionNumber":1,"value":"House of Israel, praise Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":19,"sectionNumber":2,"value":"House of Aaron, praise Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":20,"sectionNumber":1,"value":"House of Levi, praise Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":20,"sectionNumber":2,"value":"You who fear Yahweh, praise Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":21,"sectionNumber":1,"value":"Blessed be Yahweh from Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":21,"sectionNumber":2,"value":"who dwells at Jerusalem. "},{"type":"line break"},{"type":"line text","chapterNumber":135,"verseNumber":21,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"}]