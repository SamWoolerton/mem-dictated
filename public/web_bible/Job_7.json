[{"type":"line text","chapterNumber":7,"verseNumber":30,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":7,"verseNumber":1,"sectionNumber":1,"value":"“Isn’t a man forced to labor on earth? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":1,"sectionNumber":2,"value":"Aren’t his days like the days of a hired hand? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":2,"sectionNumber":1,"value":"As a servant who earnestly desires the shadow, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":2,"sectionNumber":2,"value":"as a hireling who looks for his wages, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":3,"sectionNumber":1,"value":"so am I made to possess months of misery, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":3,"sectionNumber":2,"value":"wearisome nights are appointed to me. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":4,"sectionNumber":1,"value":"When I lie down, I say, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":4,"sectionNumber":2,"value":"‘When will I arise, and the night be gone?’ "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":4,"sectionNumber":3,"value":"I toss and turn until the dawning of the day. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":5,"sectionNumber":1,"value":"My flesh is clothed with worms and clods of dust. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":5,"sectionNumber":2,"value":"My skin closes up, and breaks out afresh. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":6,"sectionNumber":1,"value":"My days are swifter than a weaver’s shuttle, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":6,"sectionNumber":2,"value":"and are spent without hope. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":7,"sectionNumber":1,"value":"Oh remember that my life is a breath. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":7,"sectionNumber":2,"value":"My eye will no more see good. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":8,"sectionNumber":1,"value":"The eye of him who sees me will see me no more. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":8,"sectionNumber":2,"value":"Your eyes will be on me, but I will not be. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":9,"sectionNumber":1,"value":"As the cloud is consumed and vanishes away, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":9,"sectionNumber":2,"value":"so he who goes down to Sheol will come up no more. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":10,"sectionNumber":1,"value":"He will return no more to his house, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":10,"sectionNumber":2,"value":"neither will his place know him any more. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":7,"verseNumber":10,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":7,"verseNumber":11,"sectionNumber":1,"value":"“Therefore I will not keep silent. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":11,"sectionNumber":2,"value":"I will speak in the anguish of my spirit. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":11,"sectionNumber":3,"value":"I will complain in the bitterness of my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":12,"sectionNumber":1,"value":"Am I a sea, or a sea monster, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":12,"sectionNumber":2,"value":"that you put a guard over me? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":13,"sectionNumber":1,"value":"When I say, ‘My bed will comfort me. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":13,"sectionNumber":2,"value":"My couch will ease my complaint;’ "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":14,"sectionNumber":1,"value":"then you scare me with dreams, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":14,"sectionNumber":2,"value":"and terrify me through visions: "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":15,"sectionNumber":1,"value":"so that my soul chooses strangling, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":15,"sectionNumber":2,"value":"death rather than my bones. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":16,"sectionNumber":1,"value":"I loathe my life. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":16,"sectionNumber":2,"value":"I don’t want to live forever. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":16,"sectionNumber":3,"value":"Leave me alone, for my days are but a breath. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":17,"sectionNumber":1,"value":"What is man, that you should magnify him, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":17,"sectionNumber":2,"value":"that you should set your mind on him, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":18,"sectionNumber":1,"value":"that you should visit him every morning, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":18,"sectionNumber":2,"value":"and test him every moment? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":19,"sectionNumber":1,"value":"How long will you not look away from me, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":19,"sectionNumber":2,"value":"nor leave me alone until I swallow down my spittle? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":20,"sectionNumber":1,"value":"If I have sinned, what do I do to you, you watcher of men? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":20,"sectionNumber":2,"value":"Why have you set me as a mark for you, "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":20,"sectionNumber":3,"value":"so that I am a burden to myself? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":21,"sectionNumber":1,"value":"Why do you not pardon my disobedience, and take away my iniquity? "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":21,"sectionNumber":2,"value":"For now will I lie down in the dust. "},{"type":"line break"},{"type":"line text","chapterNumber":7,"verseNumber":21,"sectionNumber":3,"value":"You will seek me diligently, but I will not be.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]