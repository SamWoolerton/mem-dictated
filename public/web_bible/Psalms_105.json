[{"type":"line text","chapterNumber":105,"verseNumber":35,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":105,"verseNumber":1,"sectionNumber":1,"value":"Give thanks to Yahweh! Call on his name! "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":1,"sectionNumber":2,"value":"Make his doings known among the peoples. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":2,"sectionNumber":1,"value":"Sing to him, sing praises to him! "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":2,"sectionNumber":2,"value":"Tell of all his marvelous works. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":3,"sectionNumber":1,"value":"Glory in his holy name. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":3,"sectionNumber":2,"value":"Let the heart of those who seek Yahweh rejoice. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":4,"sectionNumber":1,"value":"Seek Yahweh and his strength. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":4,"sectionNumber":2,"value":"Seek his face forever more. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":5,"sectionNumber":1,"value":"Remember his marvelous works that he has done: "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":5,"sectionNumber":2,"value":"his wonders, and the judgments of his mouth, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":6,"sectionNumber":1,"value":"you offspring of Abraham, his servant, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":6,"sectionNumber":2,"value":"you children of Jacob, his chosen ones. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":7,"sectionNumber":1,"value":"He is Yahweh, our God. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":7,"sectionNumber":2,"value":"His judgments are in all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":8,"sectionNumber":1,"value":"He has remembered his covenant forever, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":8,"sectionNumber":2,"value":"the word which he commanded to a thousand generations, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":9,"sectionNumber":1,"value":"the covenant which he made with Abraham, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":9,"sectionNumber":2,"value":"his oath to Isaac, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":10,"sectionNumber":1,"value":"and confirmed it to Jacob for a statute; "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":10,"sectionNumber":2,"value":"to Israel for an everlasting covenant, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":11,"sectionNumber":1,"value":"saying, “To you I will give the land of Canaan, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":11,"sectionNumber":2,"value":"the lot of your inheritance,” "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":12,"sectionNumber":1,"value":"when they were but a few men in number, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":12,"sectionNumber":2,"value":"yes, very few, and foreigners in it. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":13,"sectionNumber":1,"value":"They went about from nation to nation, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":13,"sectionNumber":2,"value":"from one kingdom to another people. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":14,"sectionNumber":1,"value":"He allowed no one to do them wrong. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":14,"sectionNumber":2,"value":"Yes, he reproved kings for their sakes, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":15,"sectionNumber":1,"value":"“Don’t touch my anointed ones! "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":15,"sectionNumber":2,"value":"Do my prophets no harm!” "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":16,"sectionNumber":1,"value":"He called for a famine on the land. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":16,"sectionNumber":2,"value":"He destroyed the food supplies. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":17,"sectionNumber":1,"value":"He sent a man before them. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":17,"sectionNumber":2,"value":"Joseph was sold for a slave. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":18,"sectionNumber":1,"value":"They bruised his feet with shackles. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":18,"sectionNumber":2,"value":"His neck was locked in irons, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":19,"sectionNumber":1,"value":"until the time that his word happened, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":19,"sectionNumber":2,"value":"and Yahweh’s word proved him true. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":20,"sectionNumber":1,"value":"The king sent and freed him, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":20,"sectionNumber":2,"value":"even the ruler of peoples, and let him go free. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":21,"sectionNumber":1,"value":"He made him lord of his house, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":21,"sectionNumber":2,"value":"and ruler of all of his possessions, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":22,"sectionNumber":1,"value":"to discipline his princes at his pleasure, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":22,"sectionNumber":2,"value":"and to teach his elders wisdom. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":23,"sectionNumber":1,"value":"Israel also came into Egypt. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":23,"sectionNumber":2,"value":"Jacob lived in the land of Ham. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":24,"sectionNumber":1,"value":"He increased his people greatly, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":24,"sectionNumber":2,"value":"and made them stronger than their adversaries. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":25,"sectionNumber":1,"value":"He turned their heart to hate his people, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":25,"sectionNumber":2,"value":"to conspire against his servants. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":26,"sectionNumber":1,"value":"He sent Moses, his servant, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":26,"sectionNumber":2,"value":"and Aaron, whom he had chosen. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":27,"sectionNumber":1,"value":"They performed miracles among them, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":27,"sectionNumber":2,"value":"and wonders in the land of Ham. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":28,"sectionNumber":1,"value":"He sent darkness, and made it dark. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":28,"sectionNumber":2,"value":"They didn’t rebel against his words. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":29,"sectionNumber":1,"value":"He turned their waters into blood, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":29,"sectionNumber":2,"value":"and killed their fish. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":30,"sectionNumber":1,"value":"Their land swarmed with frogs, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":30,"sectionNumber":2,"value":"even in the rooms of their kings. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":31,"sectionNumber":1,"value":"He spoke, and swarms of flies came, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":31,"sectionNumber":2,"value":"and lice in all their borders. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":32,"sectionNumber":1,"value":"He gave them hail for rain, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":32,"sectionNumber":2,"value":"with lightning in their land. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":33,"sectionNumber":1,"value":"He struck their vines and also their fig trees, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":33,"sectionNumber":2,"value":"and shattered the trees of their country. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":34,"sectionNumber":1,"value":"He spoke, and the locusts came "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":34,"sectionNumber":2,"value":"with the grasshoppers, without number, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":35,"sectionNumber":1,"value":"ate up every plant in their land, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":35,"sectionNumber":2,"value":"and ate up the fruit of their ground. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":36,"sectionNumber":1,"value":"He struck also all the firstborn in their land, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":36,"sectionNumber":2,"value":"the first fruits of all their manhood. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":37,"sectionNumber":1,"value":"He brought them out with silver and gold. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":37,"sectionNumber":2,"value":"There was not one feeble person among his tribes. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":38,"sectionNumber":1,"value":"Egypt was glad when they departed, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":38,"sectionNumber":2,"value":"for the fear of them had fallen on them. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":39,"sectionNumber":1,"value":"He spread a cloud for a covering, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":39,"sectionNumber":2,"value":"fire to give light in the night. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":40,"sectionNumber":1,"value":"They asked, and he brought quails, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":40,"sectionNumber":2,"value":"and satisfied them with the bread of the sky. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":41,"sectionNumber":1,"value":"He opened the rock, and waters gushed out. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":41,"sectionNumber":2,"value":"They ran as a river in the dry places. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":42,"sectionNumber":1,"value":"For he remembered his holy word, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":42,"sectionNumber":2,"value":"and Abraham, his servant. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":43,"sectionNumber":1,"value":"He brought his people out with joy, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":43,"sectionNumber":2,"value":"his chosen with singing. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":44,"sectionNumber":1,"value":"He gave them the lands of the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":44,"sectionNumber":2,"value":"They took the labor of the peoples in possession, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":45,"sectionNumber":1,"value":"that they might keep his statutes, "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":45,"sectionNumber":2,"value":"and observe his laws. "},{"type":"line break"},{"type":"line text","chapterNumber":105,"verseNumber":45,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"}]