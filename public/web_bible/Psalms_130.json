[{"type":"line text","chapterNumber":130,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":130,"verseNumber":1,"sectionNumber":1,"value":"Out of the depths I have cried to you, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":2,"sectionNumber":1,"value":"Lord, hear my voice. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":2,"sectionNumber":2,"value":"Let your ears be attentive to the voice of my petitions. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":3,"sectionNumber":1,"value":"If you, Yah, kept a record of sins, "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":3,"sectionNumber":2,"value":"Lord, who could stand? "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":4,"sectionNumber":1,"value":"But there is forgiveness with you, "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":4,"sectionNumber":2,"value":"therefore you are feared. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":5,"sectionNumber":1,"value":"I wait for Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":5,"sectionNumber":2,"value":"My soul waits. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":5,"sectionNumber":3,"value":"I hope in his word. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":6,"sectionNumber":1,"value":"My soul longs for the Lord more than watchmen long for the morning, "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":6,"sectionNumber":2,"value":"more than watchmen for the morning. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":7,"sectionNumber":1,"value":"Israel, hope in Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":7,"sectionNumber":2,"value":"for there is loving kindness with Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":7,"sectionNumber":3,"value":"Abundant redemption is with him. "},{"type":"line break"},{"type":"line text","chapterNumber":130,"verseNumber":8,"sectionNumber":1,"value":"He will redeem Israel from all their sins. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents. By David."},{"type":"stanza start"}]