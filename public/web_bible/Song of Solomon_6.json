[{"type":"line text","chapterNumber":6,"verseNumber":16,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":6,"verseNumber":1,"sectionNumber":1,"value":"Where has your beloved gone, you fairest among women? "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":1,"sectionNumber":2,"value":"Where has your beloved turned, that we may seek him with you? "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":2,"sectionNumber":1,"value":"My beloved has gone down to his garden, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":2,"sectionNumber":2,"value":"to the beds of spices, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":2,"sectionNumber":3,"value":"to pasture his flock in the gardens, and to gather lilies. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":3,"sectionNumber":1,"value":"I am my beloved’s, and my beloved is mine. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":3,"sectionNumber":2,"value":"He browses among the lilies. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":6,"verseNumber":3,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":6,"verseNumber":4,"sectionNumber":1,"value":"You are beautiful, my love, as Tirzah, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":4,"sectionNumber":2,"value":"lovely as Jerusalem, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":4,"sectionNumber":3,"value":"awesome as an army with banners. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":5,"sectionNumber":1,"value":"Turn away your eyes from me, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":5,"sectionNumber":2,"value":"for they have overcome me. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":5,"sectionNumber":3,"value":"Your hair is like a flock of goats, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":5,"sectionNumber":4,"value":"that lie along the side of Gilead. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":6,"sectionNumber":1,"value":"Your teeth are like a flock of ewes, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":6,"sectionNumber":2,"value":"which have come up from the washing, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":6,"sectionNumber":3,"value":"of which every one has twins; "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":6,"sectionNumber":4,"value":"not one is bereaved among them. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":7,"sectionNumber":1,"value":"Your temples are like a piece of a pomegranate behind your veil. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":6,"verseNumber":7,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":6,"verseNumber":8,"sectionNumber":1,"value":"There are sixty queens, eighty concubines, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":8,"sectionNumber":2,"value":"and virgins without number. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":1,"value":"My dove, my perfect one, is unique. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":2,"value":"She is her mother’s only daughter. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":3,"value":"She is the favorite one of her who bore her. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":4,"value":"The daughters saw her, and called her blessed. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":5,"value":"The queens and the concubines saw her, and they praised her. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":6,"verseNumber":9,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":6,"verseNumber":10,"sectionNumber":1,"value":"Who is she who looks out as the morning, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":10,"sectionNumber":2,"value":"beautiful as the moon, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":10,"sectionNumber":3,"value":"clear as the sun, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":10,"sectionNumber":4,"value":"and awesome as an army with banners? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":6,"verseNumber":10,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":6,"verseNumber":11,"sectionNumber":1,"value":"I went down into the nut tree grove, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":11,"sectionNumber":2,"value":"to see the green plants of the valley, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":11,"sectionNumber":3,"value":"to see whether the vine budded, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":11,"sectionNumber":4,"value":"and the pomegranates were in flower. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":12,"sectionNumber":1,"value":"Without realizing it, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":12,"sectionNumber":2,"value":"my desire set me with my royal people’s chariots. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":13,"sectionNumber":1,"value":"Return, return, Shulammite! "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":13,"sectionNumber":2,"value":"Return, return, that we may gaze at you. "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":13,"sectionNumber":3,"value":"Why do you desire to gaze at the Shulammite, "},{"type":"line break"},{"type":"line text","chapterNumber":6,"verseNumber":13,"sectionNumber":4,"value":"as at the dance of Mahanaim? "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]