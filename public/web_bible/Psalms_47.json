[{"type":"line text","chapterNumber":47,"verseNumber":11,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":47,"verseNumber":1,"sectionNumber":1,"value":"Oh clap your hands, all you nations. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":1,"sectionNumber":2,"value":"Shout to God with the voice of triumph! "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":2,"sectionNumber":1,"value":"For Yahweh Most High is awesome. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":2,"sectionNumber":2,"value":"He is a great King over all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":3,"sectionNumber":1,"value":"He subdues nations under us, "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":3,"sectionNumber":2,"value":"and peoples under our feet. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":4,"sectionNumber":1,"value":"He chooses our inheritance for us, "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":4,"sectionNumber":2,"value":"the glory of Jacob whom he loved. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":5,"sectionNumber":1,"value":"God has gone up with a shout, "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":5,"sectionNumber":2,"value":"Yahweh with the sound of a trumpet. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":6,"sectionNumber":1,"value":"Sing praises to God! Sing praises! "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":6,"sectionNumber":2,"value":"Sing praises to our King! Sing praises! "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":7,"sectionNumber":1,"value":"For God is the King of all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":7,"sectionNumber":2,"value":"Sing praises with understanding. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":8,"sectionNumber":1,"value":"God reigns over the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":8,"sectionNumber":2,"value":"God sits on his holy throne. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":9,"sectionNumber":1,"value":"The princes of the peoples are gathered together, "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":9,"sectionNumber":2,"value":"the people of the God of Abraham. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":9,"sectionNumber":3,"value":"For the shields of the earth belong to God. "},{"type":"line break"},{"type":"line text","chapterNumber":47,"verseNumber":9,"sectionNumber":4,"value":"He is greatly exalted! "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song. A Psalm by the sons of Korah."},{"type":"stanza start"}]