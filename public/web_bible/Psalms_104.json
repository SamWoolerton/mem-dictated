[{"type":"line text","chapterNumber":104,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":104,"verseNumber":1,"sectionNumber":1,"value":"Bless Yahweh, my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":1,"sectionNumber":2,"value":"Yahweh, my God, you are very great. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":1,"sectionNumber":3,"value":"You are clothed with honor and majesty. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":2,"sectionNumber":1,"value":"He covers himself with light as with a garment. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":2,"sectionNumber":2,"value":"He stretches out the heavens like a curtain. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":3,"sectionNumber":1,"value":"He lays the beams of his rooms in the waters. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":3,"sectionNumber":2,"value":"He makes the clouds his chariot. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":3,"sectionNumber":3,"value":"He walks on the wings of the wind. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":4,"sectionNumber":1,"value":"He makes his messengers winds, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":4,"sectionNumber":2,"value":"and his servants flames of fire. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":5,"sectionNumber":1,"value":"He laid the foundations of the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":5,"sectionNumber":2,"value":"that it should not be moved forever. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":6,"sectionNumber":1,"value":"You covered it with the deep as with a cloak. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":6,"sectionNumber":2,"value":"The waters stood above the mountains. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":7,"sectionNumber":1,"value":"At your rebuke they fled. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":7,"sectionNumber":2,"value":"At the voice of your thunder they hurried away. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":8,"sectionNumber":1,"value":"The mountains rose, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":8,"sectionNumber":2,"value":"the valleys sank down, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":8,"sectionNumber":3,"value":"to the place which you had assigned to them. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":9,"sectionNumber":1,"value":"You have set a boundary that they may not pass over, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":9,"sectionNumber":2,"value":"that they don’t turn again to cover the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":10,"sectionNumber":1,"value":"He sends springs into the valleys. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":10,"sectionNumber":2,"value":"They run among the mountains. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":11,"sectionNumber":1,"value":"They give drink to every animal of the field. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":11,"sectionNumber":2,"value":"The wild donkeys quench their thirst. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":12,"sectionNumber":1,"value":"The birds of the sky nest by them. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":12,"sectionNumber":2,"value":"They sing among the branches. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":13,"sectionNumber":1,"value":"He waters the mountains from his rooms. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":13,"sectionNumber":2,"value":"The earth is filled with the fruit of your works. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":14,"sectionNumber":1,"value":"He causes the grass to grow for the livestock, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":14,"sectionNumber":2,"value":"and plants for man to cultivate, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":14,"sectionNumber":3,"value":"that he may produce food out of the earth: "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":15,"sectionNumber":1,"value":"wine that makes the heart of man glad, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":15,"sectionNumber":2,"value":"oil to make his face to shine, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":15,"sectionNumber":3,"value":"and bread that strengthens man’s heart. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":16,"sectionNumber":1,"value":"Yahweh’s trees are well watered, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":16,"sectionNumber":2,"value":"the cedars of Lebanon, which he has planted; "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":17,"sectionNumber":1,"value":"where the birds make their nests. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":17,"sectionNumber":2,"value":"The stork makes its home in the cypress trees. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":18,"sectionNumber":1,"value":"The high mountains are for the wild goats. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":18,"sectionNumber":2,"value":"The rocks are a refuge for the rock badgers. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":19,"sectionNumber":1,"value":"He appointed the moon for seasons. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":19,"sectionNumber":2,"value":"The sun knows when to set. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":20,"sectionNumber":1,"value":"You make darkness, and it is night, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":20,"sectionNumber":2,"value":"in which all the animals of the forest prowl. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":21,"sectionNumber":1,"value":"The young lions roar after their prey, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":21,"sectionNumber":2,"value":"and seek their food from God. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":22,"sectionNumber":1,"value":"The sun rises, and they steal away, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":22,"sectionNumber":2,"value":"and lie down in their dens. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":23,"sectionNumber":1,"value":"Man goes out to his work, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":23,"sectionNumber":2,"value":"to his labor until the evening. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":24,"sectionNumber":1,"value":"Yahweh, how many are your works! "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":24,"sectionNumber":2,"value":"In wisdom, you have made them all. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":24,"sectionNumber":3,"value":"The earth is full of your riches. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":25,"sectionNumber":1,"value":"There is the sea, great and wide, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":25,"sectionNumber":2,"value":"in which are innumerable living things, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":25,"sectionNumber":3,"value":"both small and large animals. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":26,"sectionNumber":1,"value":"There the ships go, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":26,"sectionNumber":2,"value":"and leviathan, whom you formed to play there. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":27,"sectionNumber":1,"value":"These all wait for you, "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":27,"sectionNumber":2,"value":"that you may give them their food in due season. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":28,"sectionNumber":1,"value":"You give to them; they gather. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":28,"sectionNumber":2,"value":"You open your hand; they are satisfied with good. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":29,"sectionNumber":1,"value":"You hide your face; they are troubled. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":29,"sectionNumber":2,"value":"You take away their breath; they die and return to the dust. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":30,"sectionNumber":1,"value":"You send out your Spirit and they are created. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":30,"sectionNumber":2,"value":"You renew the face of the ground. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":31,"sectionNumber":1,"value":"Let Yahweh’s glory endure forever. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":31,"sectionNumber":2,"value":"Let Yahweh rejoice in his works. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":32,"sectionNumber":1,"value":"He looks at the earth, and it trembles. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":32,"sectionNumber":2,"value":"He touches the mountains, and they smoke. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":33,"sectionNumber":1,"value":"I will sing to Yahweh as long as I live. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":33,"sectionNumber":2,"value":"I will sing praise to my God while I have any being. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":34,"sectionNumber":1,"value":"Let my meditation be sweet to him. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":34,"sectionNumber":2,"value":"I will rejoice in Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":35,"sectionNumber":1,"value":"Let sinners be consumed out of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":35,"sectionNumber":2,"value":"Let the wicked be no more. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":35,"sectionNumber":3,"value":"Bless Yahweh, my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":104,"verseNumber":35,"sectionNumber":4,"value":"Praise Yah! "},{"type":"line break"}]