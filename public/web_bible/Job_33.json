[{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":1,"sectionNumber":1,"value":"“However, Job, please hear my speech, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":1,"sectionNumber":2,"value":"and listen to all my words. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":1,"value":"See now, I have opened my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":2,"sectionNumber":2,"value":"My tongue has spoken in my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":1,"value":"My words will utter the uprightness of my heart. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":3,"sectionNumber":2,"value":"That which my lips know they will speak sincerely. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":4,"sectionNumber":1,"value":"The Spirit of God has made me, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":4,"sectionNumber":2,"value":"and the breath of the Almighty gives me life. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":1,"value":"If you can, answer me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":5,"sectionNumber":2,"value":"Set your words in order before me, and stand up. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":6,"sectionNumber":1,"value":"Behold, I am toward God even as you are. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":6,"sectionNumber":2,"value":"I am also formed out of the clay. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":1,"value":"Behold, my terror will not make you afraid, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":2,"value":"neither will my pressure be heavy on you. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":8,"sectionNumber":1,"value":"“Surely you have spoken in my hearing, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":8,"sectionNumber":2,"value":"I have heard the voice of your words, saying, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":1,"value":"‘I am clean, without disobedience. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":9,"sectionNumber":2,"value":"I am innocent, neither is there iniquity in me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":1,"value":"Behold, he finds occasions against me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":10,"sectionNumber":2,"value":"He counts me for his enemy. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":1,"value":"He puts my feet in the stocks. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":2,"value":"He marks all my paths.’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":11,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":1,"value":"“Behold, I will answer you. In this you are not just, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":2,"value":"for God is greater than man. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":12,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":13,"sectionNumber":1,"value":"Why do you strive against him, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":13,"sectionNumber":2,"value":"because he doesn’t give account of any of his matters? "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":14,"sectionNumber":1,"value":"For God speaks once, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":14,"sectionNumber":2,"value":"yes twice, though man pays no attention. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":15,"sectionNumber":1,"value":"In a dream, in a vision of the night, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":15,"sectionNumber":2,"value":"when deep sleep falls on men, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":15,"sectionNumber":3,"value":"in slumbering on the bed; "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":1,"value":"Then he opens the ears of men, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":16,"sectionNumber":2,"value":"and seals their instruction, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":1,"value":"that he may withdraw man from his purpose, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":17,"sectionNumber":2,"value":"and hide pride from man. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":18,"sectionNumber":1,"value":"He keeps back his soul from the pit, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":18,"sectionNumber":2,"value":"and his life from perishing by the sword. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":18,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":1,"value":"He is chastened also with pain on his bed, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":19,"sectionNumber":2,"value":"with continual strife in his bones, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":20,"sectionNumber":1,"value":"so that his life abhors bread, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":20,"sectionNumber":2,"value":"and his soul dainty food. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":1,"value":"His flesh is so consumed away that it can’t be seen. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":21,"sectionNumber":2,"value":"His bones that were not seen stick out. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":1,"value":"Yes, his soul draws near to the pit, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":2,"value":"and his life to the destroyers. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":22,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":1,"value":"“If there is beside him an angel, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":2,"value":"an interpreter, one among a thousand, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":23,"sectionNumber":3,"value":"to show to man what is right for him; "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":1,"value":"then God is gracious to him, and says, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":2,"value":"‘Deliver him from going down to the pit, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":24,"sectionNumber":3,"value":"I have found a ransom.’ "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":25,"sectionNumber":1,"value":"His flesh will be fresher than a child’s. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":25,"sectionNumber":2,"value":"He returns to the days of his youth. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":1,"value":"He prays to God, and he is favorable to him, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":2,"value":"so that he sees his face with joy. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":26,"sectionNumber":3,"value":"He restores to man his righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":1,"value":"He sings before men, and says, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":2,"value":"‘I have sinned, and perverted that which was right, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":27,"sectionNumber":3,"value":"and it didn’t profit me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":1,"value":"He has redeemed my soul from going into the pit. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":2,"value":"My life will see the light.’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":33,"verseNumber":28,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":1,"value":"“Behold, God does all these things, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":29,"sectionNumber":2,"value":"twice, yes three times, with a man, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":30,"sectionNumber":1,"value":"to bring back his soul from the pit, "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":30,"sectionNumber":2,"value":"that he may be enlightened with the light of the living. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":31,"sectionNumber":1,"value":"Mark well, Job, and listen to me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":31,"sectionNumber":2,"value":"Hold your peace, and I will speak. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":32,"sectionNumber":1,"value":"If you have anything to say, answer me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":32,"sectionNumber":2,"value":"Speak, for I desire to justify you. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":33,"sectionNumber":1,"value":"If not, listen to me. "},{"type":"line break"},{"type":"line text","chapterNumber":33,"verseNumber":33,"sectionNumber":2,"value":"Hold your peace, and I will teach you wisdom.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"paragraph start"}]