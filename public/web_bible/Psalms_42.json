[{"type":"line text","chapterNumber":42,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":1,"value":"As the deer pants for the water brooks, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":1,"sectionNumber":2,"value":"so my soul pants after you, God. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":2,"sectionNumber":1,"value":"My soul thirsts for God, for the living God. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":2,"sectionNumber":2,"value":"When shall I come and appear before God? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":3,"sectionNumber":1,"value":"My tears have been my food day and night, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":3,"sectionNumber":2,"value":"while they continually ask me, “Where is your God?” "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":1,"value":"These things I remember, and pour out my soul within me, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":2,"value":"how I used to go with the crowd, and led them to God’s house, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":4,"sectionNumber":3,"value":"with the voice of joy and praise, a multitude keeping a holy day. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":1,"value":"Why are you in despair, my soul? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":2,"value":"Why are you disturbed within me? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":3,"value":"Hope in God! "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":5,"sectionNumber":4,"value":"For I shall still praise him for the saving help of his presence. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":1,"value":"My God, my soul is in despair within me. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":2,"value":"Therefore I remember you from the land of the Jordan, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":6,"sectionNumber":3,"value":"the heights of Hermon, from the hill Mizar. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":1,"value":"Deep calls to deep at the noise of your waterfalls. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":2,"value":"All your waves and your billows have swept over me. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":42,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":1,"value":"Yahweh will command his loving kindness in the daytime. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":2,"value":"In the night his song shall be with me: "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":8,"sectionNumber":3,"value":"a prayer to the God of my life. "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":1,"value":"I will ask God, my rock, “Why have you forgotten me? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":9,"sectionNumber":2,"value":"Why do I go mourning because of the oppression of the enemy?” "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":1,"value":"As with a sword in my bones, my adversaries reproach me, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":10,"sectionNumber":2,"value":"while they continually ask me, “Where is your God?” "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":1,"value":"Why are you in despair, my soul? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":2,"value":"Why are you disturbed within me? "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":3,"value":"Hope in God! For I shall still praise him, "},{"type":"line break"},{"type":"line text","chapterNumber":42,"verseNumber":11,"sectionNumber":4,"value":"the saving help of my countenance, and my God. "},{"type":"line break"}]