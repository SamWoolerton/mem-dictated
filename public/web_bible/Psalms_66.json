[{"type":"line text","chapterNumber":66,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":66,"verseNumber":1,"sectionNumber":1,"value":"Make a joyful shout to God, all the earth! "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":2,"sectionNumber":1,"value":"Sing to the glory of his name! "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":2,"sectionNumber":2,"value":"Offer glory and praise! "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":3,"sectionNumber":1,"value":"Tell God, “How awesome are your deeds! "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":3,"sectionNumber":2,"value":"Through the greatness of your power, your enemies submit themselves to you. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":4,"sectionNumber":1,"value":"All the earth will worship you, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":4,"sectionNumber":2,"value":"and will sing to you; "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":4,"sectionNumber":3,"value":"they will sing to your name.” "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":5,"sectionNumber":1,"value":"Come, and see God’s deeds— "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":5,"sectionNumber":2,"value":"awesome work on behalf of the children of men. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":6,"sectionNumber":1,"value":"He turned the sea into dry land. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":6,"sectionNumber":2,"value":"They went through the river on foot. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":6,"sectionNumber":3,"value":"There, we rejoiced in him. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":7,"sectionNumber":1,"value":"He rules by his might forever. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":7,"sectionNumber":2,"value":"His eyes watch the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":7,"sectionNumber":3,"value":"Don’t let the rebellious rise up against him. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":8,"sectionNumber":1,"value":"Praise our God, you peoples! "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":8,"sectionNumber":2,"value":"Make the sound of his praise heard, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":9,"sectionNumber":1,"value":"who preserves our life among the living, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":9,"sectionNumber":2,"value":"and doesn’t allow our feet to be moved. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":10,"sectionNumber":1,"value":"For you, God, have tested us. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":10,"sectionNumber":2,"value":"You have refined us, as silver is refined. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":11,"sectionNumber":1,"value":"You brought us into prison. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":11,"sectionNumber":2,"value":"You laid a burden on our backs. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":12,"sectionNumber":1,"value":"You allowed men to ride over our heads. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":12,"sectionNumber":2,"value":"We went through fire and through water, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":12,"sectionNumber":3,"value":"but you brought us to the place of abundance. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":13,"sectionNumber":1,"value":"I will come into your temple with burnt offerings. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":13,"sectionNumber":2,"value":"I will pay my vows to you,  "},{"type":"line text","chapterNumber":66,"verseNumber":14,"sectionNumber":1,"value":"which my lips promised, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":14,"sectionNumber":2,"value":"and my mouth spoke, when I was in distress. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":15,"sectionNumber":1,"value":"I will offer to you burnt offerings of fat animals, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":15,"sectionNumber":2,"value":"with the offering of rams, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":15,"sectionNumber":3,"value":"I will offer bulls with goats. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":16,"sectionNumber":1,"value":"Come and hear, all you who fear God. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":16,"sectionNumber":2,"value":"I will declare what he has done for my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":17,"sectionNumber":1,"value":"I cried to him with my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":17,"sectionNumber":2,"value":"He was extolled with my tongue. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":18,"sectionNumber":1,"value":"If I cherished sin in my heart, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":18,"sectionNumber":2,"value":"the Lord wouldn’t have listened. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":19,"sectionNumber":1,"value":"But most certainly, God has listened. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":19,"sectionNumber":2,"value":"He has heard the voice of my prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":20,"sectionNumber":1,"value":"Blessed be God, who has not turned away my prayer, "},{"type":"line break"},{"type":"line text","chapterNumber":66,"verseNumber":20,"sectionNumber":2,"value":"nor his loving kindness from me. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. With stringed instruments. A Psalm. A song."},{"type":"stanza start"}]