[{"type":"line text","chapterNumber":9,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":9,"verseNumber":1,"sectionNumber":1,"value":"I will give thanks to Yahweh with my whole heart. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":1,"sectionNumber":2,"value":"I will tell of all your marvelous works. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":2,"sectionNumber":1,"value":"I will be glad and rejoice in you. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":2,"sectionNumber":2,"value":"I will sing praise to your name, O Most High. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":3,"sectionNumber":1,"value":"When my enemies turn back, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":3,"sectionNumber":2,"value":"they stumble and perish in your presence. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":4,"sectionNumber":1,"value":"For you have maintained my just cause. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":4,"sectionNumber":2,"value":"You sit on the throne judging righteously. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":5,"sectionNumber":1,"value":"You have rebuked the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":5,"sectionNumber":2,"value":"You have destroyed the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":5,"sectionNumber":3,"value":"You have blotted out their name forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":6,"sectionNumber":1,"value":"The enemy is overtaken by endless ruin. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":6,"sectionNumber":2,"value":"The very memory of the cities which you have overthrown has perished. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":7,"sectionNumber":1,"value":"But Yahweh reigns forever. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":7,"sectionNumber":2,"value":"He has prepared his throne for judgment. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":8,"sectionNumber":1,"value":"He will judge the world in righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":8,"sectionNumber":2,"value":"He will administer judgment to the peoples in uprightness. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":9,"sectionNumber":1,"value":"Yahweh will also be a high tower for the oppressed; "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":9,"sectionNumber":2,"value":"a high tower in times of trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":10,"sectionNumber":1,"value":"Those who know your name will put their trust in you, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":10,"sectionNumber":2,"value":"for you, Yahweh, have not forsaken those who seek you. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":11,"sectionNumber":1,"value":"Sing praises to Yahweh, who dwells in Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":11,"sectionNumber":2,"value":"and declare among the people what he has done. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":12,"sectionNumber":1,"value":"For he who avenges blood remembers them. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":12,"sectionNumber":2,"value":"He doesn’t forget the cry of the afflicted. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":13,"sectionNumber":1,"value":"Have mercy on me, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":13,"sectionNumber":2,"value":"See my affliction by those who hate me, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":13,"sectionNumber":3,"value":"and lift me up from the gates of death, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":14,"sectionNumber":1,"value":"that I may show all of your praise. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":14,"sectionNumber":2,"value":"I will rejoice in your salvation in the gates of the daughter of Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":15,"sectionNumber":1,"value":"The nations have sunk down in the pit that they made. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":15,"sectionNumber":2,"value":"In the net which they hid, their own foot is taken. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":16,"sectionNumber":1,"value":"Yahweh has made himself known. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":16,"sectionNumber":2,"value":"He has executed judgment. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":16,"sectionNumber":3,"value":"The wicked is snared by the work of his own hands. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":17,"sectionNumber":1,"value":"The wicked shall be turned back to Sheol, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":17,"sectionNumber":2,"value":"even all the nations that forget God. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":18,"sectionNumber":1,"value":"For the needy shall not always be forgotten, "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":18,"sectionNumber":2,"value":"nor the hope of the poor perish forever. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":19,"sectionNumber":1,"value":"Arise, Yahweh! Don’t let man prevail. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":19,"sectionNumber":2,"value":"Let the nations be judged in your sight. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":20,"sectionNumber":1,"value":"Put them in fear, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":9,"verseNumber":20,"sectionNumber":2,"value":"Let the nations know that they are only men. "},{"type":"line break"}]