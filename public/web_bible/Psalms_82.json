[{"type":"line text","chapterNumber":82,"verseNumber":16,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":82,"verseNumber":1,"sectionNumber":1,"value":"God presides in the great assembly. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":1,"sectionNumber":2,"value":"He judges among the gods. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":2,"sectionNumber":1,"value":"“How long will you judge unjustly, "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":2,"sectionNumber":2,"value":"and show partiality to the wicked?” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":82,"verseNumber":2,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":82,"verseNumber":3,"sectionNumber":1,"value":"“Defend the weak, the poor, and the fatherless. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":3,"sectionNumber":2,"value":"Maintain the rights of the poor and oppressed. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":4,"sectionNumber":1,"value":"Rescue the weak and needy. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":4,"sectionNumber":2,"value":"Deliver them out of the hand of the wicked.” "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":5,"sectionNumber":1,"value":"They don’t know, neither do they understand. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":5,"sectionNumber":2,"value":"They walk back and forth in darkness. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":5,"sectionNumber":3,"value":"All the foundations of the earth are shaken. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":6,"sectionNumber":1,"value":"I said, “You are gods, "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":6,"sectionNumber":2,"value":"all of you are sons of the Most High. "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":7,"sectionNumber":1,"value":"Nevertheless you shall die like men, "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":7,"sectionNumber":2,"value":"and fall like one of the rulers.” "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":8,"sectionNumber":1,"value":"Arise, God, judge the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":82,"verseNumber":8,"sectionNumber":2,"value":"for you inherit all of the nations. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A song. A Psalm by Asaph."},{"type":"stanza start"}]