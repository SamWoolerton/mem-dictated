[{"type":"paragraph text","chapterNumber":40,"verseNumber":1,"sectionNumber":1,"value":"Moreover Yahweh answered Job, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":2,"sectionNumber":1,"value":"“Shall he who argues contend with the Almighty? "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":2,"sectionNumber":2,"value":"He who argues with God, let him answer it.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":40,"verseNumber":3,"sectionNumber":1,"value":"Then Job answered Yahweh, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":3,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":4,"sectionNumber":1,"value":"“Behold, I am of small account. What will I answer you? "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":4,"sectionNumber":2,"value":"I lay my hand on my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":5,"sectionNumber":1,"value":"I have spoken once, and I will not answer; "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":5,"sectionNumber":2,"value":"Yes, twice, but I will proceed no further.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"paragraph start"},{"type":"paragraph text","chapterNumber":40,"verseNumber":6,"sectionNumber":1,"value":"Then Yahweh answered Job out of the whirlwind, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":6,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":7,"sectionNumber":1,"value":"“Now brace yourself like a man. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":7,"sectionNumber":2,"value":"I will question you, and you will answer me. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":8,"sectionNumber":1,"value":"Will you even annul my judgment? "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":8,"sectionNumber":2,"value":"Will you condemn me, that you may be justified? "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":9,"sectionNumber":1,"value":"Or do you have an arm like God? "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":9,"sectionNumber":2,"value":"Can you thunder with a voice like him? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":9,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":10,"sectionNumber":1,"value":"“Now deck yourself with excellency and dignity. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":10,"sectionNumber":2,"value":"Array yourself with honor and majesty. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":11,"sectionNumber":1,"value":"Pour out the fury of your anger. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":11,"sectionNumber":2,"value":"Look at everyone who is proud, and bring him low. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":12,"sectionNumber":1,"value":"Look at everyone who is proud, and humble him. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":12,"sectionNumber":2,"value":"Crush the wicked in their place. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":13,"sectionNumber":1,"value":"Hide them in the dust together. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":13,"sectionNumber":2,"value":"Bind their faces in the hidden place. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":14,"sectionNumber":1,"value":"Then I will also admit to you "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":14,"sectionNumber":2,"value":"that your own right hand can save you. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":14,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":15,"sectionNumber":1,"value":"“See now, behemoth, which I made as well as you. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":15,"sectionNumber":2,"value":"He eats grass as an ox. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":16,"sectionNumber":1,"value":"Look now, his strength is in his thighs. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":16,"sectionNumber":2,"value":"His force is in the muscles of his belly. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":17,"sectionNumber":1,"value":"He moves his tail like a cedar. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":17,"sectionNumber":2,"value":"The sinews of his thighs are knit together. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":18,"sectionNumber":1,"value":"His bones are like tubes of bronze. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":18,"sectionNumber":2,"value":"His limbs are like bars of iron. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":40,"verseNumber":18,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":40,"verseNumber":19,"sectionNumber":1,"value":"He is the chief of the ways of God. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":19,"sectionNumber":2,"value":"He who made him gives him his sword. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":20,"sectionNumber":1,"value":"Surely the mountains produce food for him, "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":20,"sectionNumber":2,"value":"where all the animals of the field play. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":21,"sectionNumber":1,"value":"He lies under the lotus trees, "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":21,"sectionNumber":2,"value":"in the covert of the reed, and the marsh. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":22,"sectionNumber":1,"value":"The lotuses cover him with their shade. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":22,"sectionNumber":2,"value":"The willows of the brook surround him. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":23,"sectionNumber":1,"value":"Behold, if a river overflows, he doesn’t tremble. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":23,"sectionNumber":2,"value":"He is confident, though the Jordan swells even to his mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":24,"sectionNumber":1,"value":"Shall any take him when he is on the watch, "},{"type":"line break"},{"type":"line text","chapterNumber":40,"verseNumber":24,"sectionNumber":2,"value":"or pierce through his nose with a snare? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"stanza start"}]