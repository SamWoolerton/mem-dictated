[{"type":"line text","chapterNumber":63,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":63,"verseNumber":1,"sectionNumber":1,"value":"God, you are my God. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":1,"sectionNumber":2,"value":"I will earnestly seek you. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":1,"sectionNumber":3,"value":"My soul thirsts for you. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":1,"sectionNumber":4,"value":"My flesh longs for you, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":1,"sectionNumber":5,"value":"in a dry and weary land, where there is no water. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":2,"sectionNumber":1,"value":"So I have seen you in the sanctuary, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":2,"sectionNumber":2,"value":"watching your power and your glory. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":3,"sectionNumber":1,"value":"Because your loving kindness is better than life, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":3,"sectionNumber":2,"value":"my lips shall praise you. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":4,"sectionNumber":1,"value":"So I will bless you while I live. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":4,"sectionNumber":2,"value":"I will lift up my hands in your name. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":5,"sectionNumber":1,"value":"My soul shall be satisfied as with the richest food. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":5,"sectionNumber":2,"value":"My mouth shall praise you with joyful lips, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":6,"sectionNumber":1,"value":"when I remember you on my bed, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":6,"sectionNumber":2,"value":"and think about you in the night watches. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":7,"sectionNumber":1,"value":"For you have been my help. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":7,"sectionNumber":2,"value":"I will rejoice in the shadow of your wings. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":8,"sectionNumber":1,"value":"My soul stays close to you. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":8,"sectionNumber":2,"value":"Your right hand holds me up. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":9,"sectionNumber":1,"value":"But those who seek my soul to destroy it "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":9,"sectionNumber":2,"value":"shall go into the lower parts of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":10,"sectionNumber":1,"value":"They shall be given over to the power of the sword. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":10,"sectionNumber":2,"value":"They shall be jackal food. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":11,"sectionNumber":1,"value":"But the king shall rejoice in God. "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":11,"sectionNumber":2,"value":"Everyone who swears by him will praise him, "},{"type":"line break"},{"type":"line text","chapterNumber":63,"verseNumber":11,"sectionNumber":3,"value":"for the mouth of those who speak lies shall be silenced. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]