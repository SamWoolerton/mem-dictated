[{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":1,"value":"Answer me when I call, God of my righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":2,"value":"Give me relief from my distress. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":3,"value":"Have mercy on me, and hear my prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":1,"value":"You sons of men, how long shall my glory be turned into dishonor? "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":2,"value":"Will you love vanity and seek after falsehood? "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":1,"value":"But know that Yahweh has set apart for himself him who is godly: "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":2,"value":"Yahweh will hear when I call to him. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":1,"value":"Stand in awe, and don’t sin. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":2,"value":"Search your own heart on your bed, and be still. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":1,"value":"Offer the sacrifices of righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":2,"value":"Put your trust in Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":1,"value":"Many say, “Who will show us any good?” "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":2,"value":"Yahweh, let the light of your face shine on us. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":1,"value":"You have put gladness in my heart, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":2,"value":"more than when their grain and their new wine are increased. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":1,"value":"In peace I will both lay myself down and sleep, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":2,"value":"for you, Yahweh alone, make me live in safety. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician, with the flutes. A Psalm by David."},{"type":"stanza start"}]