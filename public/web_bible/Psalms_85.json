[{"type":"line text","chapterNumber":85,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":85,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, you have been favorable to your land. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":1,"sectionNumber":2,"value":"You have restored the fortunes of Jacob. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":2,"sectionNumber":1,"value":"You have forgiven the iniquity of your people. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":2,"sectionNumber":2,"value":"You have covered all their sin. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":3,"sectionNumber":1,"value":"You have taken away all your wrath. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":3,"sectionNumber":2,"value":"You have turned from the fierceness of your anger. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":4,"sectionNumber":1,"value":"Turn us, God of our salvation, "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":4,"sectionNumber":2,"value":"and cause your indignation toward us to cease. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":5,"sectionNumber":1,"value":"Will you be angry with us forever? "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":5,"sectionNumber":2,"value":"Will you draw out your anger to all generations? "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":6,"sectionNumber":1,"value":"Wonâ€™t you revive us again, "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":6,"sectionNumber":2,"value":"that your people may rejoice in you? "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":7,"sectionNumber":1,"value":"Show us your loving kindness, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":7,"sectionNumber":2,"value":"Grant us your salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":8,"sectionNumber":1,"value":"I will hear what God, Yahweh, will speak, "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":8,"sectionNumber":2,"value":"for he will speak peace to his people, his saints; "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":8,"sectionNumber":3,"value":"but let them not turn again to folly. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":9,"sectionNumber":1,"value":"Surely his salvation is near those who fear him, "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":9,"sectionNumber":2,"value":"that glory may dwell in our land. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":10,"sectionNumber":1,"value":"Mercy and truth meet together. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":10,"sectionNumber":2,"value":"Righteousness and peace have kissed each other. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":11,"sectionNumber":1,"value":"Truth springs out of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":11,"sectionNumber":2,"value":"Righteousness has looked down from heaven. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":12,"sectionNumber":1,"value":"Yes, Yahweh will give that which is good. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":12,"sectionNumber":2,"value":"Our land will yield its increase. "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":13,"sectionNumber":1,"value":"Righteousness goes before him, "},{"type":"line break"},{"type":"line text","chapterNumber":85,"verseNumber":13,"sectionNumber":2,"value":"And prepares the way for his steps. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Prayer by David."},{"type":"stanza start"}]