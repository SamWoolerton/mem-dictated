[{"type":"line text","chapterNumber":95,"verseNumber":23,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":95,"verseNumber":1,"sectionNumber":1,"value":"Oh come, let’s sing to Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":1,"sectionNumber":2,"value":"Let’s shout aloud to the rock of our salvation! "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":2,"sectionNumber":1,"value":"Let’s come before his presence with thanksgiving. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":2,"sectionNumber":2,"value":"Let’s extol him with songs! "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":3,"sectionNumber":1,"value":"For Yahweh is a great God, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":3,"sectionNumber":2,"value":"a great King above all gods. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":4,"sectionNumber":1,"value":"In his hand are the deep places of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":4,"sectionNumber":2,"value":"The heights of the mountains are also his. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":5,"sectionNumber":1,"value":"The sea is his, and he made it. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":5,"sectionNumber":2,"value":"His hands formed the dry land. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":6,"sectionNumber":1,"value":"Oh come, let’s worship and bow down. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":6,"sectionNumber":2,"value":"Let’s kneel before Yahweh, our Maker, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":7,"sectionNumber":1,"value":"for he is our God. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":7,"sectionNumber":2,"value":"We are the people of his pasture, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":7,"sectionNumber":3,"value":"and the sheep in his care. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":7,"sectionNumber":4,"value":"Today, oh that you would hear his voice! "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":8,"sectionNumber":1,"value":"Don’t harden your heart, as at Meribah, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":8,"sectionNumber":2,"value":"as in the day of Massah in the wilderness, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":9,"sectionNumber":1,"value":"when your fathers tempted me, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":9,"sectionNumber":2,"value":"tested me, and saw my work. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":10,"sectionNumber":1,"value":"Forty long years I was grieved with that generation, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":10,"sectionNumber":2,"value":"and said, “It is a people that errs in their heart. "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":10,"sectionNumber":3,"value":"They have not known my ways.” "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":11,"sectionNumber":1,"value":"Therefore I swore in my wrath, "},{"type":"line break"},{"type":"line text","chapterNumber":95,"verseNumber":11,"sectionNumber":2,"value":"“They won’t enter into my rest.” "},{"type":"line break"}]