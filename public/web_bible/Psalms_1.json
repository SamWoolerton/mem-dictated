[{"type":"stanza start"},{"type":"line text","chapterNumber":1,"verseNumber":1,"sectionNumber":1,"value":"Blessed is the man who doesn’t walk in the counsel of the wicked, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":1,"sectionNumber":2,"value":"nor stand on the path of sinners, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":1,"sectionNumber":3,"value":"nor sit in the seat of scoffers; "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":2,"sectionNumber":1,"value":"but his delight is in Yahweh’s law. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":2,"sectionNumber":2,"value":"On his law he meditates day and night. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":1,"value":"He will be like a tree planted by the streams of water, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":2,"value":"that produces its fruit in its season, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":3,"value":"whose leaf also does not wither. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":3,"sectionNumber":4,"value":"Whatever he does shall prosper. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":1,"value":"The wicked are not so, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":4,"sectionNumber":2,"value":"but are like the chaff which the wind drives away. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":1,"value":"Therefore the wicked shall not stand in the judgment, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":5,"sectionNumber":2,"value":"nor sinners in the congregation of the righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":1,"value":"For Yahweh knows the way of the righteous, "},{"type":"line break"},{"type":"line text","chapterNumber":1,"verseNumber":6,"sectionNumber":2,"value":"but the way of the wicked shall perish. "},{"type":"line break"}]