[{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":1,"value":"I have come into my garden, my sister, my bride. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":2,"value":"I have gathered my myrrh with my spice; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":3,"value":"I have eaten my honeycomb with my honey; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":4,"value":"I have drunk my wine with my milk. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":5,"value":"Eat, friends! "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":6,"value":"Drink, yes, drink abundantly, beloved. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":1,"value":"I was asleep, but my heart was awake. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":2,"value":"It is the voice of my beloved who knocks: "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":3,"value":"“Open to me, my sister, my love, my dove, my undefiled; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":4,"value":"for my head is filled with dew, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":5,"value":"and my hair with the dampness of the night.” "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":1,"value":"I have taken off my robe. Indeed, must I put it on? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":2,"value":"I have washed my feet. Indeed, must I soil them? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":1,"value":"My beloved thrust his hand in through the latch opening. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":2,"value":"My heart pounded for him. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":1,"value":"I rose up to open for my beloved. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":2,"value":"My hands dripped with myrrh, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":3,"value":"my fingers with liquid myrrh, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":4,"value":"on the handles of the lock. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":1,"value":"I opened to my beloved; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":2,"value":"but my beloved left, and had gone away. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":3,"value":"My heart went out when he spoke. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":4,"value":"I looked for him, but I didn’t find him. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":5,"value":"I called him, but he didn’t answer. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":1,"value":"The watchmen who go about the city found me. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":2,"value":"They beat me. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":3,"value":"They bruised me. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":4,"value":"The keepers of the walls took my cloak away from me. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":1,"value":"I adjure you, daughters of Jerusalem, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":2,"value":"If you find my beloved, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":3,"value":"that you tell him that I am faint with love. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":1,"value":"How is your beloved better than another beloved, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":2,"value":"you fairest among women? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":3,"value":"How is your beloved better than another beloved, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":4,"value":"that you do so adjure us? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":1,"value":"My beloved is white and ruddy. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":2,"value":"The best among ten thousand. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":1,"value":"His head is like the purest gold. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":2,"value":"His hair is bushy, black as a raven. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":1,"value":"His eyes are like doves beside the water brooks, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":2,"value":"washed with milk, mounted like jewels. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":1,"value":"His cheeks are like a bed of spices with towers of perfumes. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":2,"value":"His lips are like lilies, dropping liquid myrrh. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":1,"value":"His hands are like rings of gold set with beryl. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":2,"value":"His body is like ivory work overlaid with sapphires. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":1,"value":"His legs are like pillars of marble set on sockets of fine gold. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":2,"value":"His appearance is like Lebanon, excellent as the cedars. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":1,"value":"His mouth is sweetness; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":2,"value":"yes, he is altogether lovely. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":3,"value":"This is my beloved, and this is my friend, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":4,"value":"daughters of Jerusalem. "},{"type":"line break"}]