[{"type":"line text","chapterNumber":34,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":34,"verseNumber":1,"sectionNumber":1,"value":"  I will bless Yahweh at all times. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":1,"sectionNumber":2,"value":"His praise will always be in my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":2,"sectionNumber":1,"value":"My soul shall boast in Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":2,"sectionNumber":2,"value":"The humble shall hear of it and be glad. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":3,"sectionNumber":1,"value":"Oh magnify Yahweh with me. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":3,"sectionNumber":2,"value":"Let’s exalt his name together. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":4,"sectionNumber":1,"value":"I sought Yahweh, and he answered me, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":4,"sectionNumber":2,"value":"and delivered me from all my fears. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":5,"sectionNumber":1,"value":"They looked to him, and were radiant. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":5,"sectionNumber":2,"value":"Their faces shall never be covered with shame. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":6,"sectionNumber":1,"value":"This poor man cried, and Yahweh heard him, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":6,"sectionNumber":2,"value":"and saved him out of all his troubles. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":7,"sectionNumber":1,"value":"Yahweh’s angel encamps around those who fear him, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":7,"sectionNumber":2,"value":"and delivers them. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":8,"sectionNumber":1,"value":"Oh taste and see that Yahweh is good. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":8,"sectionNumber":2,"value":"Blessed is the man who takes refuge in him. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":9,"sectionNumber":1,"value":"Oh fear Yahweh, you his saints, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":9,"sectionNumber":2,"value":"for there is no lack with those who fear him. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":10,"sectionNumber":1,"value":"The young lions do lack, and suffer hunger, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":10,"sectionNumber":2,"value":"but those who seek Yahweh shall not lack any good thing. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":34,"verseNumber":10,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":34,"verseNumber":11,"sectionNumber":1,"value":"Come, you children, listen to me. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":11,"sectionNumber":2,"value":"I will teach you the fear of Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":12,"sectionNumber":1,"value":"Who is someone who desires life, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":12,"sectionNumber":2,"value":"and loves many days, that he may see good? "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":13,"sectionNumber":1,"value":"Keep your tongue from evil, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":13,"sectionNumber":2,"value":"and your lips from speaking lies. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":14,"sectionNumber":1,"value":"Depart from evil, and do good. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":14,"sectionNumber":2,"value":"Seek peace, and pursue it. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":15,"sectionNumber":1,"value":"Yahweh’s eyes are toward the righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":15,"sectionNumber":2,"value":"His ears listen to their cry. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":16,"sectionNumber":1,"value":"Yahweh’s face is against those who do evil, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":16,"sectionNumber":2,"value":"to cut off their memory from the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":17,"sectionNumber":1,"value":"The righteous cry, and Yahweh hears, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":17,"sectionNumber":2,"value":"and delivers them out of all their troubles. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":18,"sectionNumber":1,"value":"Yahweh is near to those who have a broken heart, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":18,"sectionNumber":2,"value":"and saves those who have a crushed spirit. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":19,"sectionNumber":1,"value":"Many are the afflictions of the righteous, "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":19,"sectionNumber":2,"value":"but Yahweh delivers him out of them all. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":20,"sectionNumber":1,"value":"He protects all of his bones. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":20,"sectionNumber":2,"value":"Not one of them is broken. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":21,"sectionNumber":1,"value":"Evil shall kill the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":21,"sectionNumber":2,"value":"Those who hate the righteous shall be condemned. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":22,"sectionNumber":1,"value":"Yahweh redeems the soul of his servants. "},{"type":"line break"},{"type":"line text","chapterNumber":34,"verseNumber":22,"sectionNumber":2,"value":"None of those who take refuge in him shall be condemned. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"By David."},{"type":"stanza start"}]