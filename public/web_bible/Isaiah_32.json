[{"type":"line text","chapterNumber":32,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":32,"verseNumber":1,"sectionNumber":1,"value":"Behold, a king shall reign in righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":1,"sectionNumber":2,"value":"and princes shall rule in justice. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":2,"sectionNumber":1,"value":"A man shall be as a hiding place from the wind, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":2,"sectionNumber":2,"value":"and a covert from the storm, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":2,"sectionNumber":3,"value":"as streams of water in a dry place, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":2,"sectionNumber":4,"value":"as the shade of a large rock in a weary land. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":3,"sectionNumber":1,"value":"The eyes of those who see will not be dim, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":3,"sectionNumber":2,"value":"and the ears of those who hear will listen. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":4,"sectionNumber":1,"value":"The heart of the rash will understand knowledge, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":4,"sectionNumber":2,"value":"and the tongue of the stammerers will be ready to speak plainly. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":5,"sectionNumber":1,"value":"The fool will no longer be called noble, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":5,"sectionNumber":2,"value":"nor the scoundrel be highly respected. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":1,"value":"For the fool will speak folly, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":2,"value":"and his heart will work iniquity, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":3,"value":"to practice profanity, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":4,"value":"and to utter error against Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":5,"value":"to make empty the soul of the hungry, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":6,"sectionNumber":6,"value":"and to cause the drink of the thirsty to fail. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":7,"sectionNumber":1,"value":"The ways of the scoundrel are evil. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":7,"sectionNumber":2,"value":"He devises wicked plans to destroy the humble with lying words, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":7,"sectionNumber":3,"value":"even when the needy speaks right. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":8,"sectionNumber":1,"value":"But the noble devises noble things; "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":8,"sectionNumber":2,"value":"and he will continue in noble things. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":32,"verseNumber":8,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":32,"verseNumber":9,"sectionNumber":1,"value":"Rise up, you women who are at ease! Hear my voice! "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":9,"sectionNumber":2,"value":"You careless daughters, give ear to my speech! "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":10,"sectionNumber":1,"value":"For days beyond a year you will be troubled, you careless women; "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":10,"sectionNumber":2,"value":"for the vintage will fail. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":10,"sectionNumber":3,"value":"The harvest won’t come. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":11,"sectionNumber":1,"value":"Tremble, you women who are at ease! "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":11,"sectionNumber":2,"value":"Be troubled, you careless ones! "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":11,"sectionNumber":3,"value":"Strip yourselves, make yourselves naked, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":11,"sectionNumber":4,"value":"and put sackcloth on your waist. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":12,"sectionNumber":1,"value":"Beat your breasts for the pleasant fields, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":12,"sectionNumber":2,"value":"for the fruitful vine. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":13,"sectionNumber":1,"value":"Thorns and briers will come up on my people’s land; "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":13,"sectionNumber":2,"value":"yes, on all the houses of joy in the joyous city. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":14,"sectionNumber":1,"value":"For the palace will be forsaken. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":14,"sectionNumber":2,"value":"The populous city will be deserted. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":14,"sectionNumber":3,"value":"The hill and the watchtower will be for dens forever, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":14,"sectionNumber":4,"value":"a delight for wild donkeys, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":14,"sectionNumber":5,"value":"a pasture of flocks, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":15,"sectionNumber":1,"value":"until the Spirit is poured on us from on high, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":15,"sectionNumber":2,"value":"and the wilderness becomes a fruitful field, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":15,"sectionNumber":3,"value":"and the fruitful field is considered a forest. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":32,"verseNumber":15,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":32,"verseNumber":16,"sectionNumber":1,"value":"Then justice will dwell in the wilderness; "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":16,"sectionNumber":2,"value":"and righteousness will remain in the fruitful field. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":17,"sectionNumber":1,"value":"The work of righteousness will be peace, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":17,"sectionNumber":2,"value":"and the effect of righteousness, quietness and confidence forever. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":18,"sectionNumber":1,"value":"My people will live in a peaceful habitation, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":18,"sectionNumber":2,"value":"in safe dwellings, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":18,"sectionNumber":3,"value":"and in quiet resting places, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":19,"sectionNumber":1,"value":"though hail flattens the forest, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":19,"sectionNumber":2,"value":"and the city is leveled completely. "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":20,"sectionNumber":1,"value":"Blessed are you who sow beside all waters, "},{"type":"line break"},{"type":"line text","chapterNumber":32,"verseNumber":20,"sectionNumber":2,"value":"who send out the feet of the ox and the donkey. "},{"type":"line break"}]