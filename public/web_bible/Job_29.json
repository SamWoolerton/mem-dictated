[{"type":"paragraph text","chapterNumber":29,"verseNumber":1,"sectionNumber":1,"value":"Job again took up his parable, and said, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":29,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":29,"verseNumber":2,"sectionNumber":1,"value":"“Oh that I were as in the months of old, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":2,"sectionNumber":2,"value":"as in the days when God watched over me; "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":3,"sectionNumber":1,"value":"when his lamp shone on my head, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":3,"sectionNumber":2,"value":"and by his light I walked through darkness, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":4,"sectionNumber":1,"value":"as I was in my prime, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":4,"sectionNumber":2,"value":"when the friendship of God was in my tent, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":5,"sectionNumber":1,"value":"when the Almighty was yet with me, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":5,"sectionNumber":2,"value":"and my children were around me, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":6,"sectionNumber":1,"value":"when my steps were washed with butter, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":6,"sectionNumber":2,"value":"and the rock poured out streams of oil for me, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":7,"sectionNumber":1,"value":"when I went out to the city gate, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":7,"sectionNumber":2,"value":"when I prepared my seat in the street. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":8,"sectionNumber":1,"value":"The young men saw me and hid themselves. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":8,"sectionNumber":2,"value":"The aged rose up and stood. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":1,"value":"The princes refrained from talking, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":2,"value":"and laid their hand on their mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":10,"sectionNumber":1,"value":"The voice of the nobles was hushed, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":10,"sectionNumber":2,"value":"and their tongue stuck to the roof of their mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":11,"sectionNumber":1,"value":"For when the ear heard me, then it blessed me; "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":11,"sectionNumber":2,"value":"and when the eye saw me, it commended me: "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":12,"sectionNumber":1,"value":"Because I delivered the poor who cried, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":12,"sectionNumber":2,"value":"and the fatherless also, who had no one to help him, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":13,"sectionNumber":1,"value":"the blessing of him who was ready to perish came on me, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":13,"sectionNumber":2,"value":"and I caused the widow’s heart to sing for joy. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":14,"sectionNumber":1,"value":"I put on righteousness, and it clothed me. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":14,"sectionNumber":2,"value":"My justice was as a robe and a diadem. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":15,"sectionNumber":1,"value":"I was eyes to the blind, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":15,"sectionNumber":2,"value":"and feet to the lame. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":16,"sectionNumber":1,"value":"I was a father to the needy. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":16,"sectionNumber":2,"value":"I researched the cause of him who I didn’t know. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":17,"sectionNumber":1,"value":"I broke the jaws of the unrighteous "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":17,"sectionNumber":2,"value":"and plucked the prey out of his teeth. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":18,"sectionNumber":1,"value":"Then I said, ‘I will die in my own house, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":18,"sectionNumber":2,"value":"I will count my days as the sand. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":19,"sectionNumber":1,"value":"My root is spread out to the waters. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":19,"sectionNumber":2,"value":"The dew lies all night on my branch. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":20,"sectionNumber":1,"value":"My glory is fresh in me. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":20,"sectionNumber":2,"value":"My bow is renewed in my hand.’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":29,"verseNumber":20,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":29,"verseNumber":21,"sectionNumber":1,"value":"“Men listened to me, waited, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":21,"sectionNumber":2,"value":"and kept silence for my counsel. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":22,"sectionNumber":1,"value":"After my words they didn’t speak again. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":22,"sectionNumber":2,"value":"My speech fell on them. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":23,"sectionNumber":1,"value":"They waited for me as for the rain. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":23,"sectionNumber":2,"value":"Their mouths drank as with the spring rain. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":24,"sectionNumber":1,"value":"I smiled on them when they had no confidence. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":24,"sectionNumber":2,"value":"They didn’t reject the light of my face. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":25,"sectionNumber":1,"value":"I chose out their way, and sat as chief. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":25,"sectionNumber":2,"value":"I lived as a king in the army, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":25,"sectionNumber":3,"value":"as one who comforts the mourners. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"stanza start"}]