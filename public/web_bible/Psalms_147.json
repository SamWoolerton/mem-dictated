[{"type":"line text","chapterNumber":147,"verseNumber":10,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":147,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":1,"sectionNumber":2,"value":"for it is good to sing praises to our God; "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":1,"sectionNumber":3,"value":"for it is pleasant and fitting to praise him. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":2,"sectionNumber":1,"value":"Yahweh builds up Jerusalem. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":2,"sectionNumber":2,"value":"He gathers together the outcasts of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":3,"sectionNumber":1,"value":"He heals the broken in heart, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":3,"sectionNumber":2,"value":"and binds up their wounds. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":4,"sectionNumber":1,"value":"He counts the number of the stars. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":4,"sectionNumber":2,"value":"He calls them all by their names. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":5,"sectionNumber":1,"value":"Great is our Lord, and mighty in power. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":5,"sectionNumber":2,"value":"His understanding is infinite. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":6,"sectionNumber":1,"value":"Yahweh upholds the humble. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":6,"sectionNumber":2,"value":"He brings the wicked down to the ground. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":7,"sectionNumber":1,"value":"Sing to Yahweh with thanksgiving. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":7,"sectionNumber":2,"value":"Sing praises on the harp to our God, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":8,"sectionNumber":1,"value":"who covers the sky with clouds, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":8,"sectionNumber":2,"value":"who prepares rain for the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":8,"sectionNumber":3,"value":"who makes grass grow on the mountains. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":9,"sectionNumber":1,"value":"He provides food for the livestock, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":9,"sectionNumber":2,"value":"and for the young ravens when they call. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":10,"sectionNumber":1,"value":"He doesn’t delight in the strength of the horse. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":10,"sectionNumber":2,"value":"He takes no pleasure in the legs of a man. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":11,"sectionNumber":1,"value":"Yahweh takes pleasure in those who fear him, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":11,"sectionNumber":2,"value":"in those who hope in his loving kindness. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":12,"sectionNumber":1,"value":"Praise Yahweh, Jerusalem! "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":12,"sectionNumber":2,"value":"Praise your God, Zion! "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":13,"sectionNumber":1,"value":"For he has strengthened the bars of your gates. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":13,"sectionNumber":2,"value":"He has blessed your children within you. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":14,"sectionNumber":1,"value":"He makes peace in your borders. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":14,"sectionNumber":2,"value":"He fills you with the finest of the wheat. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":15,"sectionNumber":1,"value":"He sends out his commandment to the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":15,"sectionNumber":2,"value":"His word runs very swiftly. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":16,"sectionNumber":1,"value":"He gives snow like wool, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":16,"sectionNumber":2,"value":"and scatters frost like ashes. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":17,"sectionNumber":1,"value":"He hurls down his hail like pebbles. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":17,"sectionNumber":2,"value":"Who can stand before his cold? "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":18,"sectionNumber":1,"value":"He sends out his word, and melts them. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":18,"sectionNumber":2,"value":"He causes his wind to blow, and the waters flow. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":19,"sectionNumber":1,"value":"He shows his word to Jacob, "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":19,"sectionNumber":2,"value":"his statutes and his ordinances to Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":20,"sectionNumber":1,"value":"He has not done this for just any nation. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":20,"sectionNumber":2,"value":"They don’t know his ordinances. "},{"type":"line break"},{"type":"line text","chapterNumber":147,"verseNumber":20,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]