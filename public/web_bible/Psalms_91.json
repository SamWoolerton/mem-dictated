[{"type":"line text","chapterNumber":91,"verseNumber":17,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":91,"verseNumber":1,"sectionNumber":1,"value":"He who dwells in the secret place of the Most High "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":1,"sectionNumber":2,"value":"will rest in the shadow of the Almighty. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":2,"sectionNumber":1,"value":"I will say of Yahweh, “He is my refuge and my fortress; "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":2,"sectionNumber":2,"value":"my God, in whom I trust.” "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":3,"sectionNumber":1,"value":"For he will deliver you from the snare of the fowler, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":3,"sectionNumber":2,"value":"and from the deadly pestilence. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":4,"sectionNumber":1,"value":"He will cover you with his feathers. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":4,"sectionNumber":2,"value":"Under his wings you will take refuge. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":4,"sectionNumber":3,"value":"His faithfulness is your shield and rampart. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":5,"sectionNumber":1,"value":"You shall not be afraid of the terror by night, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":5,"sectionNumber":2,"value":"nor of the arrow that flies by day, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":6,"sectionNumber":1,"value":"nor of the pestilence that walks in darkness, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":6,"sectionNumber":2,"value":"nor of the destruction that wastes at noonday. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":7,"sectionNumber":1,"value":"A thousand may fall at your side, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":7,"sectionNumber":2,"value":"and ten thousand at your right hand; "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":7,"sectionNumber":3,"value":"but it will not come near you. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":8,"sectionNumber":1,"value":"You will only look with your eyes, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":8,"sectionNumber":2,"value":"and see the recompense of the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":9,"sectionNumber":1,"value":"Because you have made Yahweh your refuge, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":9,"sectionNumber":2,"value":"and the Most High your dwelling place, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":10,"sectionNumber":1,"value":"no evil shall happen to you, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":10,"sectionNumber":2,"value":"neither shall any plague come near your dwelling. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":11,"sectionNumber":1,"value":"For he will put his angels in charge of you, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":11,"sectionNumber":2,"value":"to guard you in all your ways. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":12,"sectionNumber":1,"value":"They will bear you up in their hands, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":12,"sectionNumber":2,"value":"so that you won’t dash your foot against a stone. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":13,"sectionNumber":1,"value":"You will tread on the lion and cobra. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":13,"sectionNumber":2,"value":"You will trample the young lion and the serpent underfoot. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":14,"sectionNumber":1,"value":"“Because he has set his love on me, therefore I will deliver him. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":14,"sectionNumber":2,"value":"I will set him on high, because he has known my name. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":15,"sectionNumber":1,"value":"He will call on me, and I will answer him. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":15,"sectionNumber":2,"value":"I will be with him in trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":15,"sectionNumber":3,"value":"I will deliver him, and honor him. "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":16,"sectionNumber":1,"value":"I will satisfy him with long life, "},{"type":"line break"},{"type":"line text","chapterNumber":91,"verseNumber":16,"sectionNumber":2,"value":"and show him my salvation.” "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm. A song for the Sabbath day."},{"type":"stanza start"}]