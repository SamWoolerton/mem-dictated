[{"type":"line text","chapterNumber":4,"verseNumber":66,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":1,"value":"How the gold has become dim! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":2,"value":"The most pure gold has changed! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":3,"value":"The stones of the sanctuary are poured out "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":4,"value":"at the head of every street. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":1,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":1,"value":"The precious sons of Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":2,"value":"comparable to fine gold, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":3,"value":"how they are esteemed as earthen pitchers, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":4,"value":"the work of the hands of the potter! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":2,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":1,"value":"Even the jackals offer their breast. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":2,"value":"They nurse their young ones. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":3,"value":"But the daughter of my people has become cruel, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":4,"value":"like the ostriches in the wilderness. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":3,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":1,"value":"The tongue of the nursing child clings to the roof of his mouth for thirst. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":2,"value":"The young children ask bread, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":3,"value":"and no one breaks it for them. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":4,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":1,"value":"Those who ate delicacies are desolate in the streets. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":2,"value":"Those who were brought up in purple embrace dunghills. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":5,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":1,"value":"For the iniquity of the daughter of my people is greater than the sin of Sodom, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":2,"value":"which was overthrown as in a moment. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":3,"value":"No hands were laid on her. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":6,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":1,"value":"Her nobles were purer than snow. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":2,"value":"They were whiter than milk. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":3,"value":"They were more ruddy in body than rubies. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":4,"value":"Their polishing was like sapphire. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":7,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":1,"value":"Their appearance is blacker than a coal. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":2,"value":"They are not known in the streets. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":3,"value":"Their skin clings to their bones. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":4,"value":"It is withered. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":5,"value":"It has become like a stick. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":8,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":1,"value":"Those who are killed with the sword are better than those who are killed with hunger; "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":2,"value":"For these pine away, stricken through, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":3,"value":"for lack of the fruits of the field. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":9,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":1,"value":"The hands of the pitiful women have boiled their own children. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":2,"value":"They were their food in the destruction of the daughter of my people. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":10,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":1,"value":"Yahweh has accomplished his wrath. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":2,"value":"He has poured out his fierce anger. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":3,"value":"He has kindled a fire in Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":4,"value":"which has devoured its foundations. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":11,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":1,"value":"The kings of the earth didn’t believe, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":2,"value":"neither did all the inhabitants of the world, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":3,"value":"that the adversary and the enemy would enter into the gates of Jerusalem. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":12,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":1,"value":"It is because of the sins of her prophets "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":2,"value":"and the iniquities of her priests, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":3,"value":"That have shed the blood of the just in the middle of her. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":13,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":1,"value":"They wander as blind men in the streets. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":2,"value":"They are polluted with blood, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":3,"value":"So that men can’t touch their garments. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":14,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":1,"value":"“Go away!” they cried to them. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":2,"value":"“Unclean! Go away! Go away! Don’t touch! "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":3,"value":"When they fled away and wandered, men said among the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":4,"value":"“They can’t live here any more.” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":15,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":1,"value":"Yahweh’s anger has scattered them. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":2,"value":"He will not pay attention to them any more. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":3,"value":"They didn’t respect the persons of the priests. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":4,"value":"They didn’t favor the elders. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":16,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":17,"sectionNumber":1,"value":"Our eyes still fail, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":17,"sectionNumber":2,"value":"looking in vain for our help. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":17,"sectionNumber":3,"value":"In our watching we have watched for a nation that could not save. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":17,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":1,"value":"They hunt our steps, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":2,"value":"so that we can’t go in our streets. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":3,"value":"Our end is near. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":4,"value":"Our days are fulfilled, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":5,"value":"for our end has come. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":18,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":19,"sectionNumber":1,"value":"Our pursuers were swifter than the eagles of the sky. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":19,"sectionNumber":2,"value":"They chased us on the mountains. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":19,"sectionNumber":3,"value":"They set an ambush for us in the wilderness. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":19,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":1,"value":"The breath of our nostrils, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":2,"value":"the anointed of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":3,"value":"was taken in their pits; "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":4,"value":"of whom we said, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":5,"value":"under his shadow we will live among the nations. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":20,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":1,"value":"Rejoice and be glad, daughter of Edom, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":2,"value":"that dwells in the land of Uz. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":3,"value":"The cup will pass through to you also. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":4,"value":"You will be drunken, "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":5,"value":"and will make yourself naked. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":4,"verseNumber":21,"sectionNumber":6,"value":" "},{"type":"line text","chapterNumber":4,"verseNumber":22,"sectionNumber":1,"value":"The punishment of your iniquity is accomplished, daughter of Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":22,"sectionNumber":2,"value":"He will no more carry you away into captivity. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":22,"sectionNumber":3,"value":"He will visit your iniquity, daughter of Edom. "},{"type":"line break"},{"type":"line text","chapterNumber":4,"verseNumber":22,"sectionNumber":4,"value":"He will uncover your sins. "},{"type":"line break"}]