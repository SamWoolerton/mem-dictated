[{"type":"line text","chapterNumber":92,"verseNumber":16,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":92,"verseNumber":1,"sectionNumber":1,"value":"It is a good thing to give thanks to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":1,"sectionNumber":2,"value":"to sing praises to your name, Most High, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":2,"sectionNumber":1,"value":"to proclaim your loving kindness in the morning, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":2,"sectionNumber":2,"value":"and your faithfulness every night, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":3,"sectionNumber":1,"value":"with the ten-stringed lute, with the harp, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":3,"sectionNumber":2,"value":"and with the melody of the lyre. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":4,"sectionNumber":1,"value":"For you, Yahweh, have made me glad through your work. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":4,"sectionNumber":2,"value":"I will triumph in the works of your hands. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":5,"sectionNumber":1,"value":"How great are your works, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":5,"sectionNumber":2,"value":"Your thoughts are very deep. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":6,"sectionNumber":1,"value":"A senseless man doesn’t know, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":6,"sectionNumber":2,"value":"neither does a fool understand this: "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":7,"sectionNumber":1,"value":"though the wicked spring up as the grass, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":7,"sectionNumber":2,"value":"and all the evildoers flourish, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":7,"sectionNumber":3,"value":"they will be destroyed forever. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":8,"sectionNumber":1,"value":"But you, Yahweh, are on high forever more. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":9,"sectionNumber":1,"value":"For, behold, your enemies, Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":9,"sectionNumber":2,"value":"for, behold, your enemies shall perish. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":9,"sectionNumber":3,"value":"All the evildoers will be scattered. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":10,"sectionNumber":1,"value":"But you have exalted my horn like that of the wild ox. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":10,"sectionNumber":2,"value":"I am anointed with fresh oil. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":11,"sectionNumber":1,"value":"My eye has also seen my enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":11,"sectionNumber":2,"value":"My ears have heard of the wicked enemies who rise up against me. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":12,"sectionNumber":1,"value":"The righteous shall flourish like the palm tree. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":12,"sectionNumber":2,"value":"He will grow like a cedar in Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":13,"sectionNumber":1,"value":"They are planted in Yahweh’s house. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":13,"sectionNumber":2,"value":"They will flourish in our God’s courts. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":14,"sectionNumber":1,"value":"They will still produce fruit in old age. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":14,"sectionNumber":2,"value":"They will be full of sap and green, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":15,"sectionNumber":1,"value":"to show that Yahweh is upright. "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":15,"sectionNumber":2,"value":"He is my rock, "},{"type":"line break"},{"type":"line text","chapterNumber":92,"verseNumber":15,"sectionNumber":3,"value":"and there is no unrighteousness in him. "},{"type":"line break"}]