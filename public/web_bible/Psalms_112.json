[{"type":"line text","chapterNumber":112,"verseNumber":10,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":112,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":1,"sectionNumber":2,"value":"Blessed is the man who fears Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":1,"sectionNumber":3,"value":"who delights greatly in his commandments. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":2,"sectionNumber":1,"value":"His offspring will be mighty in the land. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":2,"sectionNumber":2,"value":"The generation of the upright will be blessed. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":3,"sectionNumber":1,"value":"Wealth and riches are in his house. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":3,"sectionNumber":2,"value":"His righteousness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":4,"sectionNumber":1,"value":"Light dawns in the darkness for the upright, "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":4,"sectionNumber":2,"value":"gracious, merciful, and righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":5,"sectionNumber":1,"value":"It is well with the man who deals graciously and lends. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":5,"sectionNumber":2,"value":"He will maintain his cause in judgment. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":6,"sectionNumber":1,"value":"For he will never be shaken. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":6,"sectionNumber":2,"value":"The righteous will be remembered forever. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":7,"sectionNumber":1,"value":"He will not be afraid of evil news. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":7,"sectionNumber":2,"value":"His heart is steadfast, trusting in Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":8,"sectionNumber":1,"value":"His heart is established. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":8,"sectionNumber":2,"value":"He will not be afraid in the end when he sees his adversaries. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":9,"sectionNumber":1,"value":"He has dispersed, he has given to the poor. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":9,"sectionNumber":2,"value":"His righteousness endures forever. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":9,"sectionNumber":3,"value":"His horn will be exalted with honor. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":10,"sectionNumber":1,"value":"The wicked will see it, and be grieved. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":10,"sectionNumber":2,"value":"He shall gnash with his teeth, and melt away. "},{"type":"line break"},{"type":"line text","chapterNumber":112,"verseNumber":10,"sectionNumber":3,"value":"The desire of the wicked will perish. "},{"type":"line break"}]