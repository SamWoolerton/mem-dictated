[{"type":"line text","chapterNumber":96,"verseNumber":11,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":96,"verseNumber":1,"sectionNumber":1,"value":"Sing to Yahweh a new song! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":1,"sectionNumber":2,"value":"Sing to Yahweh, all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":2,"sectionNumber":1,"value":"Sing to Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":2,"sectionNumber":2,"value":"Bless his name! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":2,"sectionNumber":3,"value":"Proclaim his salvation from day to day! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":3,"sectionNumber":1,"value":"Declare his glory among the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":3,"sectionNumber":2,"value":"his marvelous works among all the peoples. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":4,"sectionNumber":1,"value":"For Yahweh is great, and greatly to be praised! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":4,"sectionNumber":2,"value":"He is to be feared above all gods. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":5,"sectionNumber":1,"value":"For all the gods of the peoples are idols, "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":5,"sectionNumber":2,"value":"but Yahweh made the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":6,"sectionNumber":1,"value":"Honor and majesty are before him. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":6,"sectionNumber":2,"value":"Strength and beauty are in his sanctuary. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":7,"sectionNumber":1,"value":"Ascribe to Yahweh, you families of nations, "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":7,"sectionNumber":2,"value":"ascribe to Yahweh glory and strength. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":8,"sectionNumber":1,"value":"Ascribe to Yahweh the glory due to his name. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":8,"sectionNumber":2,"value":"Bring an offering, and come into his courts. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":9,"sectionNumber":1,"value":"Worship Yahweh in holy array. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":9,"sectionNumber":2,"value":"Tremble before him, all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":10,"sectionNumber":1,"value":"Say among the nations, “Yahweh reigns.” "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":10,"sectionNumber":2,"value":"The world is also established. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":10,"sectionNumber":3,"value":"It can’t be moved. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":10,"sectionNumber":4,"value":"He will judge the peoples with equity. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":11,"sectionNumber":1,"value":"Let the heavens be glad, and let the earth rejoice. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":11,"sectionNumber":2,"value":"Let the sea roar, and its fullness! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":12,"sectionNumber":1,"value":"Let the field and all that is in it exult! "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":12,"sectionNumber":2,"value":"Then all the trees of the woods shall sing for joy "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":13,"sectionNumber":1,"value":"before Yahweh; for he comes, "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":13,"sectionNumber":2,"value":"for he comes to judge the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":13,"sectionNumber":3,"value":"He will judge the world with righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":96,"verseNumber":13,"sectionNumber":4,"value":"the peoples with his truth. "},{"type":"line break"}]