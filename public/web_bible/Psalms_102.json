[{"type":"line text","chapterNumber":102,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":102,"verseNumber":1,"sectionNumber":1,"value":"Hear my prayer, Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":1,"sectionNumber":2,"value":"Let my cry come to you. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":2,"sectionNumber":1,"value":"Don’t hide your face from me in the day of my distress. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":2,"sectionNumber":2,"value":"Turn your ear to me. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":2,"sectionNumber":3,"value":"Answer me quickly in the day when I call. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":3,"sectionNumber":1,"value":"For my days consume away like smoke. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":3,"sectionNumber":2,"value":"My bones are burned as a torch. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":4,"sectionNumber":1,"value":"My heart is blighted like grass, and withered, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":4,"sectionNumber":2,"value":"for I forget to eat my bread. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":5,"sectionNumber":1,"value":"By reason of the voice of my groaning, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":5,"sectionNumber":2,"value":"my bones stick to my skin. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":6,"sectionNumber":1,"value":"I am like a pelican of the wilderness. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":6,"sectionNumber":2,"value":"I have become as an owl of the waste places. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":7,"sectionNumber":1,"value":"I watch, and have become like a sparrow that is alone on the housetop. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":8,"sectionNumber":1,"value":"My enemies reproach me all day. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":8,"sectionNumber":2,"value":"Those who are mad at me use my name as a curse. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":9,"sectionNumber":1,"value":"For I have eaten ashes like bread, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":9,"sectionNumber":2,"value":"and mixed my drink with tears, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":10,"sectionNumber":1,"value":"because of your indignation and your wrath; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":10,"sectionNumber":2,"value":"for you have taken me up and thrown me away. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":11,"sectionNumber":1,"value":"My days are like a long shadow. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":11,"sectionNumber":2,"value":"I have withered like grass. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":102,"verseNumber":11,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":102,"verseNumber":12,"sectionNumber":1,"value":"But you, Yahweh, will remain forever; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":12,"sectionNumber":2,"value":"your renown endures to all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":13,"sectionNumber":1,"value":"You will arise and have mercy on Zion; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":13,"sectionNumber":2,"value":"for it is time to have pity on her. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":13,"sectionNumber":3,"value":"Yes, the set time has come. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":14,"sectionNumber":1,"value":"For your servants take pleasure in her stones, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":14,"sectionNumber":2,"value":"and have pity on her dust. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":15,"sectionNumber":1,"value":"So the nations will fear Yahweh’s name, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":15,"sectionNumber":2,"value":"all the kings of the earth your glory. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":16,"sectionNumber":1,"value":"For Yahweh has built up Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":16,"sectionNumber":2,"value":"He has appeared in his glory. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":17,"sectionNumber":1,"value":"He has responded to the prayer of the destitute, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":17,"sectionNumber":2,"value":"and has not despised their prayer. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":18,"sectionNumber":1,"value":"This will be written for the generation to come. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":18,"sectionNumber":2,"value":"A people which will be created will praise Yah. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":19,"sectionNumber":1,"value":"For he has looked down from the height of his sanctuary. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":19,"sectionNumber":2,"value":"From heaven, Yahweh saw the earth; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":20,"sectionNumber":1,"value":"to hear the groans of the prisoner; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":20,"sectionNumber":2,"value":"to free those who are condemned to death; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":21,"sectionNumber":1,"value":"that men may declare Yahweh’s name in Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":21,"sectionNumber":2,"value":"and his praise in Jerusalem; "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":22,"sectionNumber":1,"value":"when the peoples are gathered together, "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":22,"sectionNumber":2,"value":"the kingdoms, to serve Yahweh. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":102,"verseNumber":22,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":102,"verseNumber":23,"sectionNumber":1,"value":"He weakened my strength along the course. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":23,"sectionNumber":2,"value":"He shortened my days. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":24,"sectionNumber":1,"value":"I said, “My God, don’t take me away in the middle of my days. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":24,"sectionNumber":2,"value":"Your years are throughout all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":25,"sectionNumber":1,"value":"Of old, you laid the foundation of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":25,"sectionNumber":2,"value":"The heavens are the work of your hands. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":26,"sectionNumber":1,"value":"They will perish, but you will endure. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":26,"sectionNumber":2,"value":"Yes, all of them will wear out like a garment. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":26,"sectionNumber":3,"value":"You will change them like a cloak, and they will be changed. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":27,"sectionNumber":1,"value":"But you are the same. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":27,"sectionNumber":2,"value":"Your years will have no end. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":28,"sectionNumber":1,"value":"The children of your servants will continue. "},{"type":"line break"},{"type":"line text","chapterNumber":102,"verseNumber":28,"sectionNumber":2,"value":"Their offspring will be established before you.” "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"By David."},{"type":"stanza start"}]