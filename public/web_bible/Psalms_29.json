[{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":29,"verseNumber":1,"sectionNumber":1,"value":"Ascribe to Yahweh, you sons of the mighty, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":1,"sectionNumber":2,"value":"ascribe to Yahweh glory and strength. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":2,"sectionNumber":1,"value":"Ascribe to Yahweh the glory due to his name. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":2,"sectionNumber":2,"value":"Worship Yahweh in holy array. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":29,"verseNumber":2,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":29,"verseNumber":3,"sectionNumber":1,"value":"Yahweh’s voice is on the waters. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":3,"sectionNumber":2,"value":"The God of glory thunders, even Yahweh on many waters. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":4,"sectionNumber":1,"value":"Yahweh’s voice is powerful. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":4,"sectionNumber":2,"value":"Yahweh’s voice is full of majesty. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":5,"sectionNumber":1,"value":"Yahweh’s voice breaks the cedars. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":5,"sectionNumber":2,"value":"Yes, Yahweh breaks in pieces the cedars of Lebanon. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":6,"sectionNumber":1,"value":"He makes them also to skip like a calf; "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":6,"sectionNumber":2,"value":"Lebanon and Sirion like a young, wild ox. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":7,"sectionNumber":1,"value":"Yahweh’s voice strikes with flashes of lightning. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":8,"sectionNumber":1,"value":"Yahweh’s voice shakes the wilderness. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":8,"sectionNumber":2,"value":"Yahweh shakes the wilderness of Kadesh. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":1,"value":"Yahweh’s voice makes the deer calve, "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":2,"value":"and strips the forests bare. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":3,"value":"In his temple everything says, “Glory!” "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":29,"verseNumber":9,"sectionNumber":4,"value":" "},{"type":"line text","chapterNumber":29,"verseNumber":10,"sectionNumber":1,"value":"Yahweh sat enthroned at the Flood. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":10,"sectionNumber":2,"value":"Yes, Yahweh sits as King forever. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":11,"sectionNumber":1,"value":"Yahweh will give strength to his people. "},{"type":"line break"},{"type":"line text","chapterNumber":29,"verseNumber":11,"sectionNumber":2,"value":"Yahweh will bless his people with peace. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm. A Song for the Dedication of the Temple. By David."},{"type":"stanza start"}]