[{"type":"line text","chapterNumber":132,"verseNumber":3,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":132,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, remember David and all his affliction, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":2,"sectionNumber":1,"value":"how he swore to Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":2,"sectionNumber":2,"value":"and vowed to the Mighty One of Jacob: "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":3,"sectionNumber":1,"value":"“Surely I will not come into the structure of my house, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":3,"sectionNumber":2,"value":"nor go up into my bed; "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":4,"sectionNumber":1,"value":"I will not give sleep to my eyes, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":4,"sectionNumber":2,"value":"or slumber to my eyelids; "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":5,"sectionNumber":1,"value":"until I find out a place for Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":5,"sectionNumber":2,"value":"a dwelling for the Mighty One of Jacob.” "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":6,"sectionNumber":1,"value":"Behold, we heard of it in Ephrathah. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":6,"sectionNumber":2,"value":"We found it in the field of Jaar: "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":7,"sectionNumber":1,"value":"“We will go into his dwelling place. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":7,"sectionNumber":2,"value":"We will worship at his footstool. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":8,"sectionNumber":1,"value":"Arise, Yahweh, into your resting place, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":8,"sectionNumber":2,"value":"you, and the ark of your strength. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":9,"sectionNumber":1,"value":"Let your priests be clothed with righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":9,"sectionNumber":2,"value":"Let your saints shout for joy!” "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":10,"sectionNumber":1,"value":"For your servant David’s sake, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":10,"sectionNumber":2,"value":"don’t turn away the face of your anointed one. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":11,"sectionNumber":1,"value":"Yahweh has sworn to David in truth. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":11,"sectionNumber":2,"value":"He will not turn from it: "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":11,"sectionNumber":3,"value":"“I will set the fruit of your body on your throne. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":12,"sectionNumber":1,"value":"If your children will keep my covenant, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":12,"sectionNumber":2,"value":"my testimony that I will teach them, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":12,"sectionNumber":3,"value":"their children also will sit on your throne forever more.” "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":13,"sectionNumber":1,"value":"For Yahweh has chosen Zion. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":13,"sectionNumber":2,"value":"He has desired it for his habitation. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":14,"sectionNumber":1,"value":"“This is my resting place forever. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":14,"sectionNumber":2,"value":"I will live here, for I have desired it. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":15,"sectionNumber":1,"value":"I will abundantly bless her provision. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":15,"sectionNumber":2,"value":"I will satisfy her poor with bread. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":16,"sectionNumber":1,"value":"I will also clothe her priests with salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":16,"sectionNumber":2,"value":"Her saints will shout aloud for joy. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":17,"sectionNumber":1,"value":"I will make the horn of David to bud there. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":17,"sectionNumber":2,"value":"I have ordained a lamp for my anointed. "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":18,"sectionNumber":1,"value":"I will clothe his enemies with shame, "},{"type":"line break"},{"type":"line text","chapterNumber":132,"verseNumber":18,"sectionNumber":2,"value":"but on himself, his crown will shine.” "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Song of Ascents. By David."},{"type":"stanza start"}]