[{"type":"line text","chapterNumber":2,"verseNumber":17,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":2,"verseNumber":1,"sectionNumber":1,"value":"I am a rose of Sharon, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":1,"sectionNumber":2,"value":"a lily of the valleys. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":2,"sectionNumber":1,"value":"As a lily among thorns, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":2,"sectionNumber":2,"value":"so is my love among the daughters. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":3,"sectionNumber":1,"value":"As the apple tree among the trees of the wood, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":3,"sectionNumber":2,"value":"so is my beloved among the sons. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":3,"sectionNumber":3,"value":"I sat down under his shadow with great delight, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":3,"sectionNumber":4,"value":"his fruit was sweet to my taste. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":4,"sectionNumber":1,"value":"He brought me to the banquet hall. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":4,"sectionNumber":2,"value":"His banner over me is love. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":5,"sectionNumber":1,"value":"Strengthen me with raisins, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":5,"sectionNumber":2,"value":"refresh me with apples; "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":5,"sectionNumber":3,"value":"for I am faint with love. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":6,"sectionNumber":1,"value":"His left hand is under my head. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":6,"sectionNumber":2,"value":"His right hand embraces me. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":7,"sectionNumber":1,"value":"I adjure you, daughters of Jerusalem, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":7,"sectionNumber":2,"value":"by the roes, or by the hinds of the field, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":7,"sectionNumber":3,"value":"that you not stir up, nor awaken love, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":7,"sectionNumber":4,"value":"until it so desires. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":2,"verseNumber":7,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":2,"verseNumber":8,"sectionNumber":1,"value":"The voice of my beloved! "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":8,"sectionNumber":2,"value":"Behold, he comes, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":8,"sectionNumber":3,"value":"leaping on the mountains, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":8,"sectionNumber":4,"value":"skipping on the hills. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":9,"sectionNumber":1,"value":"My beloved is like a roe or a young deer. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":9,"sectionNumber":2,"value":"Behold, he stands behind our wall! "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":9,"sectionNumber":3,"value":"He looks in at the windows. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":9,"sectionNumber":4,"value":"He glances through the lattice. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":2,"verseNumber":9,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":2,"verseNumber":10,"sectionNumber":1,"value":"My beloved spoke, and said to me, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":10,"sectionNumber":2,"value":"“Rise up, my love, my beautiful one, and come away. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":11,"sectionNumber":1,"value":"For behold, the winter is past. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":11,"sectionNumber":2,"value":"The rain is over and gone. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":12,"sectionNumber":1,"value":"The flowers appear on the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":12,"sectionNumber":2,"value":"The time of the singing has come, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":12,"sectionNumber":3,"value":"and the voice of the turtledove is heard in our land. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":13,"sectionNumber":1,"value":"The fig tree ripens her green figs. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":13,"sectionNumber":2,"value":"The vines are in blossom. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":13,"sectionNumber":3,"value":"They give out their fragrance. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":13,"sectionNumber":4,"value":"Arise, my love, my beautiful one, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":13,"sectionNumber":5,"value":"and come away.” "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":14,"sectionNumber":1,"value":"My dove in the clefts of the rock, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":14,"sectionNumber":2,"value":"in the hiding places of the mountainside, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":14,"sectionNumber":3,"value":"let me see your face. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":14,"sectionNumber":4,"value":"let me hear your voice; "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":14,"sectionNumber":5,"value":"for your voice is sweet and your face is lovely. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":15,"sectionNumber":1,"value":"Catch for us the foxes, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":15,"sectionNumber":2,"value":"the little foxes that plunder the vineyards; "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":15,"sectionNumber":3,"value":"for our vineyards are in blossom. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":16,"sectionNumber":1,"value":"My beloved is mine, and I am his. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":16,"sectionNumber":2,"value":"He browses among the lilies. "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":17,"sectionNumber":1,"value":"Until the day is cool, and the shadows flee away, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":17,"sectionNumber":2,"value":"turn, my beloved, "},{"type":"line break"},{"type":"line text","chapterNumber":2,"verseNumber":17,"sectionNumber":3,"value":"and be like a roe or a young deer on the mountains of Bether. "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]