[{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":1,"value":"Now you shall gather yourself in troops, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":2,"value":"daughter of troops. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":3,"value":"He has laid siege against us. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":4,"value":"They will strike the judge of Israel with a rod on the cheek. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":1,"value":"But you, Bethlehem Ephrathah, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":2,"value":"being small among the clans of Judah, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":3,"value":"out of you one will come out to me that is to be ruler in Israel; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":4,"value":"whose goings out are from of old, from ancient times. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":1,"value":"Therefore he will abandon them until the time that she who is in labor gives birth. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":2,"value":"Then the rest of his brothers will return to the children of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":1,"value":"He shall stand, and shall shepherd in the strength of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":2,"value":"in the majesty of the name of Yahweh his God: "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":3,"value":"and they will live, for then he will be great to the ends of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":1,"value":"He will be our peace when Assyria invades our land, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":2,"value":"and when he marches through our fortresses, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":3,"value":"then we will raise against him seven shepherds, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":4,"value":"and eight leaders of men. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":1,"value":"They will rule the land of Assyria with the sword, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":2,"value":"and the land of Nimrod in its gates. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":3,"value":"He will deliver us from the Assyrian, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":4,"value":"when he invades our land, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":5,"value":"and when he marches within our border. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":1,"value":"The remnant of Jacob will be among many peoples, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":2,"value":"like dew from Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":3,"value":"like showers on the grass, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":4,"value":"that don’t wait for man, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":5,"value":"nor wait for the sons of men. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":1,"value":"The remnant of Jacob will be among the nations, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":2,"value":"among many peoples, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":3,"value":"like a lion among the animals of the forest, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":4,"value":"like a young lion among the flocks of sheep; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":5,"value":"who, if he goes through, treads down and tears in pieces, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":6,"value":"and there is no one to deliver. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":1,"value":"Let your hand be lifted up above your adversaries, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":2,"value":"and let all of your enemies be cut off. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":1,"value":"“It will happen in that day”, says Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":2,"value":"“that I will cut off your horses out from among you, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":3,"value":"and will destroy your chariots. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":1,"value":"I will cut off the cities of your land, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":2,"value":"and will tear down all your strongholds. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":1,"value":"I will destroy witchcraft from your hand; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":2,"value":"and you shall have no soothsayers. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":1,"value":"I will cut off your engraved images and your pillars out from among you; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":2,"value":"and you shall no more worship the work of your hands. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":1,"value":"I will uproot your Asherah poles out from among you; "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":2,"value":"and I will destroy your cities. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":1,"value":"I will execute vengeance in anger, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":2,"value":"and wrath on the nations that didn’t listen.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]