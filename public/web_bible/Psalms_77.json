[{"type":"line text","chapterNumber":77,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":77,"verseNumber":1,"sectionNumber":1,"value":"My cry goes to God! "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":1,"sectionNumber":2,"value":"Indeed, I cry to God for help, "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":1,"sectionNumber":3,"value":"and for him to listen to me. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":2,"sectionNumber":1,"value":"In the day of my trouble I sought the Lord. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":2,"sectionNumber":2,"value":"My hand was stretched out in the night, and didn’t get tired. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":2,"sectionNumber":3,"value":"My soul refused to be comforted. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":3,"sectionNumber":1,"value":"I remember God, and I groan. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":3,"sectionNumber":2,"value":"I complain, and my spirit is overwhelmed. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":77,"verseNumber":3,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":77,"verseNumber":4,"sectionNumber":1,"value":"You hold my eyelids open. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":4,"sectionNumber":2,"value":"I am so troubled that I can’t speak. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":5,"sectionNumber":1,"value":"I have considered the days of old, "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":5,"sectionNumber":2,"value":"the years of ancient times. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":6,"sectionNumber":1,"value":"I remember my song in the night. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":6,"sectionNumber":2,"value":"I consider in my own heart; "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":6,"sectionNumber":3,"value":"my spirit diligently inquires: "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":7,"sectionNumber":1,"value":"“Will the Lord reject us forever? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":7,"sectionNumber":2,"value":"Will he be favorable no more? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":8,"sectionNumber":1,"value":"Has his loving kindness vanished forever? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":8,"sectionNumber":2,"value":"Does his promise fail for generations? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":9,"sectionNumber":1,"value":"Has God forgotten to be gracious? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":9,"sectionNumber":2,"value":"Has he, in anger, withheld his compassion?” "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":10,"sectionNumber":1,"value":"Then I thought, “I will appeal to this: "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":10,"sectionNumber":2,"value":"the years of the right hand of the Most High.” "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":11,"sectionNumber":1,"value":"I will remember Yah’s deeds; "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":11,"sectionNumber":2,"value":"for I will remember your wonders of old. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":12,"sectionNumber":1,"value":"I will also meditate on all your work, "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":12,"sectionNumber":2,"value":"and consider your doings. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":13,"sectionNumber":1,"value":"Your way, God, is in the sanctuary. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":13,"sectionNumber":2,"value":"What god is great like God? "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":14,"sectionNumber":1,"value":"You are the God who does wonders. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":14,"sectionNumber":2,"value":"You have made your strength known among the peoples. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":15,"sectionNumber":1,"value":"You have redeemed your people with your arm, "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":15,"sectionNumber":2,"value":"the sons of Jacob and Joseph. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":16,"sectionNumber":1,"value":"The waters saw you, God. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":16,"sectionNumber":2,"value":"The waters saw you, and they writhed. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":16,"sectionNumber":3,"value":"The depths also convulsed. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":17,"sectionNumber":1,"value":"The clouds poured out water. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":17,"sectionNumber":2,"value":"The skies resounded with thunder. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":17,"sectionNumber":3,"value":"Your arrows also flashed around. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":18,"sectionNumber":1,"value":"The voice of your thunder was in the whirlwind. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":18,"sectionNumber":2,"value":"The lightnings lit up the world. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":18,"sectionNumber":3,"value":"The earth trembled and shook. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":19,"sectionNumber":1,"value":"Your way was through the sea; "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":19,"sectionNumber":2,"value":"your paths through the great waters. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":19,"sectionNumber":3,"value":"Your footsteps were not known. "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":20,"sectionNumber":1,"value":"You led your people like a flock, "},{"type":"line break"},{"type":"line text","chapterNumber":77,"verseNumber":20,"sectionNumber":2,"value":"by the hand of Moses and Aaron. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"header","value":"A contemplation by Asaph."},{"type":"stanza start"}]