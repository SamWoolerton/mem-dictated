[{"type":"line text","chapterNumber":103,"verseNumber":28,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":103,"verseNumber":1,"sectionNumber":1,"value":"Praise Yahweh, my soul! "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":1,"sectionNumber":2,"value":"All that is within me, praise his holy name! "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":2,"sectionNumber":1,"value":"Praise Yahweh, my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":2,"sectionNumber":2,"value":"and don’t forget all his benefits, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":3,"sectionNumber":1,"value":"who forgives all your sins, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":3,"sectionNumber":2,"value":"who heals all your diseases, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":4,"sectionNumber":1,"value":"who redeems your life from destruction, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":4,"sectionNumber":2,"value":"who crowns you with loving kindness and tender mercies, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":5,"sectionNumber":1,"value":"who satisfies your desire with good things, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":5,"sectionNumber":2,"value":"so that your youth is renewed like the eagle’s. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":6,"sectionNumber":1,"value":"Yahweh executes righteous acts, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":6,"sectionNumber":2,"value":"and justice for all who are oppressed. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":7,"sectionNumber":1,"value":"He made known his ways to Moses, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":7,"sectionNumber":2,"value":"his deeds to the children of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":8,"sectionNumber":1,"value":"Yahweh is merciful and gracious, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":8,"sectionNumber":2,"value":"slow to anger, and abundant in loving kindness. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":9,"sectionNumber":1,"value":"He will not always accuse; "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":9,"sectionNumber":2,"value":"neither will he stay angry forever. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":10,"sectionNumber":1,"value":"He has not dealt with us according to our sins, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":10,"sectionNumber":2,"value":"nor repaid us for our iniquities. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":11,"sectionNumber":1,"value":"For as the heavens are high above the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":11,"sectionNumber":2,"value":"so great is his loving kindness toward those who fear him. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":12,"sectionNumber":1,"value":"As far as the east is from the west, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":12,"sectionNumber":2,"value":"so far has he removed our transgressions from us. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":13,"sectionNumber":1,"value":"Like a father has compassion on his children, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":13,"sectionNumber":2,"value":"so Yahweh has compassion on those who fear him. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":14,"sectionNumber":1,"value":"For he knows how we are made. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":14,"sectionNumber":2,"value":"He remembers that we are dust. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":15,"sectionNumber":1,"value":"As for man, his days are like grass. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":15,"sectionNumber":2,"value":"As a flower of the field, so he flourishes. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":16,"sectionNumber":1,"value":"For the wind passes over it, and it is gone. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":16,"sectionNumber":2,"value":"Its place remembers it no more. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":17,"sectionNumber":1,"value":"But Yahweh’s loving kindness is from everlasting to everlasting with those who fear him, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":17,"sectionNumber":2,"value":"his righteousness to children’s children, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":18,"sectionNumber":1,"value":"to those who keep his covenant, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":18,"sectionNumber":2,"value":"to those who remember to obey his precepts. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":19,"sectionNumber":1,"value":"Yahweh has established his throne in the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":19,"sectionNumber":2,"value":"His kingdom rules over all. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":20,"sectionNumber":1,"value":"Praise Yahweh, you angels of his, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":20,"sectionNumber":2,"value":"who are mighty in strength, who fulfill his word, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":20,"sectionNumber":3,"value":"obeying the voice of his word. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":21,"sectionNumber":1,"value":"Praise Yahweh, all you armies of his, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":21,"sectionNumber":2,"value":"you servants of his, who do his pleasure. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":22,"sectionNumber":1,"value":"Praise Yahweh, all you works of his, "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":22,"sectionNumber":2,"value":"in all places of his dominion. "},{"type":"line break"},{"type":"line text","chapterNumber":103,"verseNumber":22,"sectionNumber":3,"value":"Praise Yahweh, my soul! "},{"type":"line break"}]