[{"type":"line text","chapterNumber":114,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":114,"verseNumber":1,"sectionNumber":1,"value":"When Israel went out of Egypt, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":1,"sectionNumber":2,"value":"the house of Jacob from a people of foreign language, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":2,"sectionNumber":1,"value":"Judah became his sanctuary, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":2,"sectionNumber":2,"value":"Israel his dominion. "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":3,"sectionNumber":1,"value":"The sea saw it, and fled. "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":3,"sectionNumber":2,"value":"The Jordan was driven back. "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":4,"sectionNumber":1,"value":"The mountains skipped like rams, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":4,"sectionNumber":2,"value":"the little hills like lambs. "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":5,"sectionNumber":1,"value":"What was it, you sea, that you fled? "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":5,"sectionNumber":2,"value":"You Jordan, that you turned back? "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":6,"sectionNumber":1,"value":"You mountains, that you skipped like rams; "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":6,"sectionNumber":2,"value":"you little hills, like lambs? "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":7,"sectionNumber":1,"value":"Tremble, you earth, at the presence of the Lord, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":7,"sectionNumber":2,"value":"at the presence of the God of Jacob, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":8,"sectionNumber":1,"value":"who turned the rock into a pool of water, "},{"type":"line break"},{"type":"line text","chapterNumber":114,"verseNumber":8,"sectionNumber":2,"value":"the flint into a spring of waters. "},{"type":"line break"}]