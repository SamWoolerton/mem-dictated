[{"type":"line text","chapterNumber":143,"verseNumber":7,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":143,"verseNumber":1,"sectionNumber":1,"value":"Hear my prayer, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":1,"sectionNumber":2,"value":"Listen to my petitions. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":1,"sectionNumber":3,"value":"In your faithfulness and righteousness, relieve me. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":2,"sectionNumber":1,"value":"Don’t enter into judgment with your servant, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":2,"sectionNumber":2,"value":"for in your sight no man living is righteous. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":3,"sectionNumber":1,"value":"For the enemy pursues my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":3,"sectionNumber":2,"value":"He has struck my life down to the ground. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":3,"sectionNumber":3,"value":"He has made me live in dark places, as those who have been long dead. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":4,"sectionNumber":1,"value":"Therefore my spirit is overwhelmed within me. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":4,"sectionNumber":2,"value":"My heart within me is desolate. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":5,"sectionNumber":1,"value":"I remember the days of old. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":5,"sectionNumber":2,"value":"I meditate on all your doings. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":5,"sectionNumber":3,"value":"I contemplate the work of your hands. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":6,"sectionNumber":1,"value":"I spread out my hands to you. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":6,"sectionNumber":2,"value":"My soul thirsts for you, like a parched land. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":7,"sectionNumber":1,"value":"Hurry to answer me, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":7,"sectionNumber":2,"value":"My spirit fails. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":7,"sectionNumber":3,"value":"Don’t hide your face from me, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":7,"sectionNumber":4,"value":"so that I don’t become like those who go down into the pit. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":8,"sectionNumber":1,"value":"Cause me to hear your loving kindness in the morning, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":8,"sectionNumber":2,"value":"for I trust in you. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":8,"sectionNumber":3,"value":"Cause me to know the way in which I should walk, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":8,"sectionNumber":4,"value":"for I lift up my soul to you. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":9,"sectionNumber":1,"value":"Deliver me, Yahweh, from my enemies. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":9,"sectionNumber":2,"value":"I flee to you to hide me. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":10,"sectionNumber":1,"value":"Teach me to do your will, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":10,"sectionNumber":2,"value":"for you are my God. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":10,"sectionNumber":3,"value":"Your Spirit is good. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":10,"sectionNumber":4,"value":"Lead me in the land of uprightness. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":11,"sectionNumber":1,"value":"Revive me, Yahweh, for your name’s sake. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":11,"sectionNumber":2,"value":"In your righteousness, bring my soul out of trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":12,"sectionNumber":1,"value":"In your loving kindness, cut off my enemies, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":12,"sectionNumber":2,"value":"and destroy all those who afflict my soul, "},{"type":"line break"},{"type":"line text","chapterNumber":143,"verseNumber":12,"sectionNumber":3,"value":"For I am your servant. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"By David."},{"type":"stanza start"}]