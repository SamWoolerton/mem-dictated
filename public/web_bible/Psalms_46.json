[{"type":"line text","chapterNumber":46,"verseNumber":17,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":46,"verseNumber":1,"sectionNumber":1,"value":"God is our refuge and strength, "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":1,"sectionNumber":2,"value":"a very present help in trouble. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":2,"sectionNumber":1,"value":"Therefore we won’t be afraid, though the earth changes, "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":2,"sectionNumber":2,"value":"though the mountains are shaken into the heart of the seas; "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":3,"sectionNumber":1,"value":"though its waters roar and are troubled, "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":3,"sectionNumber":2,"value":"though the mountains tremble with their swelling. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":46,"verseNumber":3,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":46,"verseNumber":4,"sectionNumber":1,"value":"There is a river, the streams of which make the city of God glad, "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":4,"sectionNumber":2,"value":"the holy place of the tents of the Most High. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":5,"sectionNumber":1,"value":"God is within her. She shall not be moved. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":5,"sectionNumber":2,"value":"God will help her at dawn. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":6,"sectionNumber":1,"value":"The nations raged. The kingdoms were moved. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":6,"sectionNumber":2,"value":"He lifted his voice and the earth melted. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":7,"sectionNumber":1,"value":"Yahweh of Armies is with us. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":7,"sectionNumber":2,"value":"The God of Jacob is our refuge. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":46,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":46,"verseNumber":8,"sectionNumber":1,"value":"Come, see Yahweh’s works, "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":8,"sectionNumber":2,"value":"what desolations he has made in the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":9,"sectionNumber":1,"value":"He makes wars cease to the end of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":9,"sectionNumber":2,"value":"He breaks the bow, and shatters the spear. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":9,"sectionNumber":3,"value":"He burns the chariots in the fire. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":10,"sectionNumber":1,"value":"“Be still, and know that I am God. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":10,"sectionNumber":2,"value":"I will be exalted among the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":10,"sectionNumber":3,"value":"I will be exalted in the earth.” "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":11,"sectionNumber":1,"value":"Yahweh of Armies is with us. "},{"type":"line break"},{"type":"line text","chapterNumber":46,"verseNumber":11,"sectionNumber":2,"value":"The God of Jacob is our refuge. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by the sons of Korah."},{"type":"stanza start"}]