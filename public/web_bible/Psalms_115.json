[{"type":"line text","chapterNumber":115,"verseNumber":8,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":115,"verseNumber":1,"sectionNumber":1,"value":"Not to us, Yahweh, not to us, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":1,"sectionNumber":2,"value":"but to your name give glory, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":1,"sectionNumber":3,"value":"for your loving kindness, and for your truth’s sake. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":2,"sectionNumber":1,"value":"Why should the nations say, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":2,"sectionNumber":2,"value":"“Where is their God, now?” "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":3,"sectionNumber":1,"value":"But our God is in the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":3,"sectionNumber":2,"value":"He does whatever he pleases. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":4,"sectionNumber":1,"value":"Their idols are silver and gold, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":4,"sectionNumber":2,"value":"the work of men’s hands. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":5,"sectionNumber":1,"value":"They have mouths, but they don’t speak. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":5,"sectionNumber":2,"value":"They have eyes, but they don’t see. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":6,"sectionNumber":1,"value":"They have ears, but they don’t hear. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":6,"sectionNumber":2,"value":"They have noses, but they don’t smell. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":7,"sectionNumber":1,"value":"They have hands, but they don’t feel. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":7,"sectionNumber":2,"value":"They have feet, but they don’t walk, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":7,"sectionNumber":3,"value":"neither do they speak through their throat. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":8,"sectionNumber":1,"value":"Those who make them will be like them; "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":8,"sectionNumber":2,"value":"yes, everyone who trusts in them. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":9,"sectionNumber":1,"value":"Israel, trust in Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":9,"sectionNumber":2,"value":"He is their help and their shield. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":10,"sectionNumber":1,"value":"House of Aaron, trust in Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":10,"sectionNumber":2,"value":"He is their help and their shield. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":11,"sectionNumber":1,"value":"You who fear Yahweh, trust in Yahweh! "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":11,"sectionNumber":2,"value":"He is their help and their shield. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":12,"sectionNumber":1,"value":"Yahweh remembers us. He will bless us. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":12,"sectionNumber":2,"value":"He will bless the house of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":12,"sectionNumber":3,"value":"He will bless the house of Aaron. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":13,"sectionNumber":1,"value":"He will bless those who fear Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":13,"sectionNumber":2,"value":"both small and great. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":14,"sectionNumber":1,"value":"May Yahweh increase you more and more, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":14,"sectionNumber":2,"value":"you and your children. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":15,"sectionNumber":1,"value":"Blessed are you by Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":15,"sectionNumber":2,"value":"who made heaven and earth. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":16,"sectionNumber":1,"value":"The heavens are Yahweh’s heavens, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":16,"sectionNumber":2,"value":"but he has given the earth to the children of men. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":17,"sectionNumber":1,"value":"The dead don’t praise Yah, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":17,"sectionNumber":2,"value":"neither any who go down into silence; "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":18,"sectionNumber":1,"value":"but we will bless Yah, "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":18,"sectionNumber":2,"value":"from this time forward and forever more. "},{"type":"line break"},{"type":"line text","chapterNumber":115,"verseNumber":18,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"}]