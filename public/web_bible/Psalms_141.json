[{"type":"line text","chapterNumber":141,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":141,"verseNumber":1,"sectionNumber":1,"value":"Yahweh, I have called on you. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":1,"sectionNumber":2,"value":"Come to me quickly! "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":1,"sectionNumber":3,"value":"Listen to my voice when I call to you. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":2,"sectionNumber":1,"value":"Let my prayer be set before you like incense; "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":2,"sectionNumber":2,"value":"the lifting up of my hands like the evening sacrifice. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":3,"sectionNumber":1,"value":"Set a watch, Yahweh, before my mouth. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":3,"sectionNumber":2,"value":"Keep the door of my lips. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":4,"sectionNumber":1,"value":"Don’t incline my heart to any evil thing, "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":4,"sectionNumber":2,"value":"to practice deeds of wickedness with men who work iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":4,"sectionNumber":3,"value":"Don’t let me eat of their delicacies. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":5,"sectionNumber":1,"value":"Let the righteous strike me, it is kindness; "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":5,"sectionNumber":2,"value":"let him reprove me, it is like oil on the head; "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":5,"sectionNumber":3,"value":"don’t let my head refuse it; "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":5,"sectionNumber":4,"value":"Yet my prayer is always against evil deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":6,"sectionNumber":1,"value":"Their judges are thrown down by the sides of the rock. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":6,"sectionNumber":2,"value":"They will hear my words, for they are well spoken. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":7,"sectionNumber":1,"value":"“As when one plows and breaks up the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":7,"sectionNumber":2,"value":"our bones are scattered at the mouth of Sheol.” "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":8,"sectionNumber":1,"value":"For my eyes are on you, Yahweh, the Lord. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":8,"sectionNumber":2,"value":"In you, I take refuge. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":8,"sectionNumber":3,"value":"Don’t leave my soul destitute. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":9,"sectionNumber":1,"value":"Keep me from the snare which they have laid for me, "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":9,"sectionNumber":2,"value":"from the traps of the workers of iniquity. "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":10,"sectionNumber":1,"value":"Let the wicked fall together into their own nets "},{"type":"line break"},{"type":"line text","chapterNumber":141,"verseNumber":10,"sectionNumber":2,"value":"while I pass by. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A contemplation by David, when he was in the cave. A Prayer."},{"type":"stanza start"}]