[{"type":"line text","chapterNumber":113,"verseNumber":10,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":113,"verseNumber":1,"sectionNumber":1,"value":"Praise Yah! "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":1,"sectionNumber":2,"value":"Praise, you servants of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":1,"sectionNumber":3,"value":"praise Yahweh’s name. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":2,"sectionNumber":1,"value":"Blessed be Yahweh’s name, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":2,"sectionNumber":2,"value":"from this time forward and forever more. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":3,"sectionNumber":1,"value":"From the rising of the sun to its going down, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":3,"sectionNumber":2,"value":"Yahweh’s name is to be praised. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":4,"sectionNumber":1,"value":"Yahweh is high above all nations, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":4,"sectionNumber":2,"value":"his glory above the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":5,"sectionNumber":1,"value":"Who is like Yahweh, our God, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":5,"sectionNumber":2,"value":"who has his seat on high, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":6,"sectionNumber":1,"value":"Who stoops down to see in heaven and in the earth? "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":7,"sectionNumber":1,"value":"He raises up the poor out of the dust. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":7,"sectionNumber":2,"value":"Lifts up the needy from the ash heap, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":8,"sectionNumber":1,"value":"that he may set him with princes, "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":8,"sectionNumber":2,"value":"even with the princes of his people. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":9,"sectionNumber":1,"value":"He settles the barren woman in her home "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":9,"sectionNumber":2,"value":"as a joyful mother of children. "},{"type":"line break"},{"type":"line text","chapterNumber":113,"verseNumber":9,"sectionNumber":3,"value":"Praise Yah! "},{"type":"line break"}]