[{"type":"line text","chapterNumber":98,"verseNumber":12,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":98,"verseNumber":1,"sectionNumber":1,"value":"Sing to Yahweh a new song, "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":1,"sectionNumber":2,"value":"for he has done marvelous things! "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":1,"sectionNumber":3,"value":"His right hand and his holy arm have worked salvation for him. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":2,"sectionNumber":1,"value":"Yahweh has made known his salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":2,"sectionNumber":2,"value":"He has openly shown his righteousness in the sight of the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":3,"sectionNumber":1,"value":"He has remembered his loving kindness and his faithfulness toward the house of Israel. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":3,"sectionNumber":2,"value":"All the ends of the earth have seen the salvation of our God. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":4,"sectionNumber":1,"value":"Make a joyful noise to Yahweh, all the earth! "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":4,"sectionNumber":2,"value":"Burst out and sing for joy, yes, sing praises! "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":5,"sectionNumber":1,"value":"Sing praises to Yahweh with the harp, "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":5,"sectionNumber":2,"value":"with the harp and the voice of melody. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":6,"sectionNumber":1,"value":"With trumpets and sound of the ramâ€™s horn, "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":6,"sectionNumber":2,"value":"make a joyful noise before the King, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":7,"sectionNumber":1,"value":"Let the sea roar with its fullness; "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":7,"sectionNumber":2,"value":"the world, and those who dwell therein. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":8,"sectionNumber":1,"value":"Let the rivers clap their hands. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":8,"sectionNumber":2,"value":"Let the mountains sing for joy together. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":9,"sectionNumber":1,"value":"Let them sing before Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":9,"sectionNumber":2,"value":"for he comes to judge the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":9,"sectionNumber":3,"value":"He will judge the world with righteousness, "},{"type":"line break"},{"type":"line text","chapterNumber":98,"verseNumber":9,"sectionNumber":4,"value":"and the peoples with equity. "},{"type":"line break"}]