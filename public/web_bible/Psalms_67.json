[{"type":"line text","chapterNumber":67,"verseNumber":20,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":67,"verseNumber":1,"sectionNumber":1,"value":"May God be merciful to us, bless us, "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":1,"sectionNumber":2,"value":"and cause his face to shine on us. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":2,"sectionNumber":1,"value":"That your way may be known on earth, "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":2,"sectionNumber":2,"value":"and your salvation among all nations, "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":3,"sectionNumber":1,"value":"let the peoples praise you, God. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":3,"sectionNumber":2,"value":"Let all the peoples praise you. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":4,"sectionNumber":1,"value":"Oh let the nations be glad and sing for joy, "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":4,"sectionNumber":2,"value":"for you will judge the peoples with equity, "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":4,"sectionNumber":3,"value":"and govern the nations on earth. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":5,"sectionNumber":1,"value":"Let the peoples praise you, God. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":5,"sectionNumber":2,"value":"Let all the peoples praise you. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":6,"sectionNumber":1,"value":"The earth has yielded its increase. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":6,"sectionNumber":2,"value":"God, even our own God, will bless us. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":7,"sectionNumber":1,"value":"God will bless us. "},{"type":"line break"},{"type":"line text","chapterNumber":67,"verseNumber":7,"sectionNumber":2,"value":"All the ends of the earth shall fear him. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David. A song."},{"type":"stanza start"}]