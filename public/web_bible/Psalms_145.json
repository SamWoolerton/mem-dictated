[{"type":"line text","chapterNumber":145,"verseNumber":15,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":145,"verseNumber":1,"sectionNumber":1,"value":"I will exalt you, my God, the King. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":1,"sectionNumber":2,"value":"I will praise your name forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":2,"sectionNumber":1,"value":"Every day I will praise you. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":2,"sectionNumber":2,"value":"I will extol your name forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":3,"sectionNumber":1,"value":"Great is Yahweh, and greatly to be praised! "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":3,"sectionNumber":2,"value":"His greatness is unsearchable. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":4,"sectionNumber":1,"value":"One generation will commend your works to another, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":4,"sectionNumber":2,"value":"and will declare your mighty acts. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":5,"sectionNumber":1,"value":"I will meditate on the glorious majesty of your honor, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":5,"sectionNumber":2,"value":"on your wondrous works. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":6,"sectionNumber":1,"value":"Men will speak of the might of your awesome acts. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":6,"sectionNumber":2,"value":"I will declare your greatness. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":7,"sectionNumber":1,"value":"They will utter the memory of your great goodness, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":7,"sectionNumber":2,"value":"and will sing of your righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":8,"sectionNumber":1,"value":"Yahweh is gracious, merciful, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":8,"sectionNumber":2,"value":"slow to anger, and of great loving kindness. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":9,"sectionNumber":1,"value":"Yahweh is good to all. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":9,"sectionNumber":2,"value":"His tender mercies are over all his works. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":10,"sectionNumber":1,"value":"All your works will give thanks to you, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":10,"sectionNumber":2,"value":"Your saints will extol you. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":11,"sectionNumber":1,"value":"They will speak of the glory of your kingdom, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":11,"sectionNumber":2,"value":"and talk about your power, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":12,"sectionNumber":1,"value":"to make known to the sons of men his mighty acts, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":12,"sectionNumber":2,"value":"the glory of the majesty of his kingdom. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":13,"sectionNumber":1,"value":"Your kingdom is an everlasting kingdom. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":13,"sectionNumber":2,"value":"Your dominion endures throughout all generations. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":13,"sectionNumber":3,"value":"Yahweh is faithful in all his words, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":13,"sectionNumber":4,"value":"and loving in all his deeds. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":14,"sectionNumber":1,"value":"Yahweh upholds all who fall, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":14,"sectionNumber":2,"value":"and raises up all those who are bowed down. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":15,"sectionNumber":1,"value":"The eyes of all wait for you. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":15,"sectionNumber":2,"value":"You give them their food in due season. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":16,"sectionNumber":1,"value":"You open your hand, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":16,"sectionNumber":2,"value":"and satisfy the desire of every living thing. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":17,"sectionNumber":1,"value":"Yahweh is righteous in all his ways, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":17,"sectionNumber":2,"value":"and gracious in all his works. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":18,"sectionNumber":1,"value":"Yahweh is near to all those who call on him, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":18,"sectionNumber":2,"value":"to all who call on him in truth. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":19,"sectionNumber":1,"value":"He will fulfill the desire of those who fear him. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":19,"sectionNumber":2,"value":"He also will hear their cry, and will save them. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":20,"sectionNumber":1,"value":"Yahweh preserves all those who love him, "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":20,"sectionNumber":2,"value":"but he will destroy all the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":21,"sectionNumber":1,"value":"My mouth will speak the praise of Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":145,"verseNumber":21,"sectionNumber":2,"value":"Let all flesh bless his holy name forever and ever. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]