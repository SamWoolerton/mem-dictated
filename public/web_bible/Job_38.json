[{"type":"paragraph text","chapterNumber":38,"verseNumber":1,"sectionNumber":1,"value":"Then Yahweh answered Job out of the whirlwind, "},{"type":"paragraph end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":1,"sectionNumber":2,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":2,"sectionNumber":1,"value":"“Who is this who darkens counsel "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":2,"sectionNumber":2,"value":"by words without knowledge? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":3,"sectionNumber":1,"value":"Brace yourself like a man, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":3,"sectionNumber":2,"value":"for I will question you, then you answer me! "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":3,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":4,"sectionNumber":1,"value":"“Where were you when I laid the foundations of the earth? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":4,"sectionNumber":2,"value":"Declare, if you have understanding. "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":5,"sectionNumber":1,"value":"Who determined its measures, if you know? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":5,"sectionNumber":2,"value":"Or who stretched the line on it? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":6,"sectionNumber":1,"value":"What were its foundations fastened on? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":6,"sectionNumber":2,"value":"Or who laid its cornerstone, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":7,"sectionNumber":1,"value":"when the morning stars sang together, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":7,"sectionNumber":2,"value":"and all the sons of God shouted for joy? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":7,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":8,"sectionNumber":1,"value":"“Or who shut up the sea with doors, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":8,"sectionNumber":2,"value":"when it broke out of the womb, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":9,"sectionNumber":1,"value":"when I made clouds its garment, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":9,"sectionNumber":2,"value":"and wrapped it in thick darkness, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":10,"sectionNumber":1,"value":"marked out for it my bound, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":10,"sectionNumber":2,"value":"set bars and doors, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":11,"sectionNumber":1,"value":"and said, ‘You may come here, but no further. "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":11,"sectionNumber":2,"value":"Your proud waves shall be stopped here?’ "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":11,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":12,"sectionNumber":1,"value":"“Have you commanded the morning in your days, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":12,"sectionNumber":2,"value":"and caused the dawn to know its place, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":13,"sectionNumber":1,"value":"that it might take hold of the ends of the earth, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":13,"sectionNumber":2,"value":"and shake the wicked out of it? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":14,"sectionNumber":1,"value":"It is changed as clay under the seal, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":14,"sectionNumber":2,"value":"and presented as a garment. "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":15,"sectionNumber":1,"value":"From the wicked, their light is withheld. "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":15,"sectionNumber":2,"value":"The high arm is broken. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":15,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":16,"sectionNumber":1,"value":"“Have you entered into the springs of the sea? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":16,"sectionNumber":2,"value":"Or have you walked in the recesses of the deep? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":17,"sectionNumber":1,"value":"Have the gates of death been revealed to you? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":17,"sectionNumber":2,"value":"Or have you seen the gates of the shadow of death? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":18,"sectionNumber":1,"value":"Have you comprehended the earth in its width? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":18,"sectionNumber":2,"value":"Declare, if you know it all. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":18,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":19,"sectionNumber":1,"value":"“What is the way to the dwelling of light? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":19,"sectionNumber":2,"value":"As for darkness, where is its place, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":20,"sectionNumber":1,"value":"that you should take it to its bound, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":20,"sectionNumber":2,"value":"that you should discern the paths to its house? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":21,"sectionNumber":1,"value":"Surely you know, for you were born then, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":21,"sectionNumber":2,"value":"and the number of your days is great! "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":22,"sectionNumber":1,"value":"Have you entered the treasuries of the snow, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":22,"sectionNumber":2,"value":"or have you seen the treasures of the hail, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":23,"sectionNumber":1,"value":"which I have reserved against the time of trouble, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":23,"sectionNumber":2,"value":"against the day of battle and war? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":24,"sectionNumber":1,"value":"By what way is the lightning distributed, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":24,"sectionNumber":2,"value":"or the east wind scattered on the earth? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":24,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":25,"sectionNumber":1,"value":"Who has cut a channel for the flood water, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":25,"sectionNumber":2,"value":"or the path for the thunder storm, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":26,"sectionNumber":1,"value":"to cause it to rain on a land where there is no man, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":26,"sectionNumber":2,"value":"on the wilderness, in which there is no man, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":27,"sectionNumber":1,"value":"to satisfy the waste and desolate ground, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":27,"sectionNumber":2,"value":"to cause the tender grass to grow? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":28,"sectionNumber":1,"value":"Does the rain have a father? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":28,"sectionNumber":2,"value":"Or who fathers the drops of dew? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":29,"sectionNumber":1,"value":"Whose womb did the ice come out of? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":29,"sectionNumber":2,"value":"Who has given birth to the gray frost of the sky? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":30,"sectionNumber":1,"value":"The waters become hard like stone, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":30,"sectionNumber":2,"value":"when the surface of the deep is frozen. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":30,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":31,"sectionNumber":1,"value":"“Can you bind the cluster of the Pleiades, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":31,"sectionNumber":2,"value":"or loosen the cords of Orion? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":32,"sectionNumber":1,"value":"Can you lead the constellations out in their season? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":32,"sectionNumber":2,"value":"Or can you guide the Bear with her cubs? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":33,"sectionNumber":1,"value":"Do you know the laws of the heavens? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":33,"sectionNumber":2,"value":"Can you establish its dominion over the earth? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":33,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":34,"sectionNumber":1,"value":"“Can you lift up your voice to the clouds, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":34,"sectionNumber":2,"value":"That abundance of waters may cover you? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":35,"sectionNumber":1,"value":"Can you send out lightnings, that they may go? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":35,"sectionNumber":2,"value":"Do they report to you, ‘Here we are?’ "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":36,"sectionNumber":1,"value":"Who has put wisdom in the inward parts? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":36,"sectionNumber":2,"value":"Or who has given understanding to the mind? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":37,"sectionNumber":1,"value":"Who can count the clouds by wisdom? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":37,"sectionNumber":2,"value":"Or who can pour out the containers of the sky, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":38,"sectionNumber":1,"value":"when the dust runs into a mass, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":38,"sectionNumber":2,"value":"and the clods of earth stick together? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":38,"verseNumber":38,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":38,"verseNumber":39,"sectionNumber":1,"value":"“Can you hunt the prey for the lioness, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":39,"sectionNumber":2,"value":"or satisfy the appetite of the young lions, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":40,"sectionNumber":1,"value":"when they crouch in their dens, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":40,"sectionNumber":2,"value":"and lie in wait in the thicket? "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":41,"sectionNumber":1,"value":"Who provides for the raven his prey, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":41,"sectionNumber":2,"value":"when his young ones cry to God, "},{"type":"line break"},{"type":"line text","chapterNumber":38,"verseNumber":41,"sectionNumber":3,"value":"and wander for lack of food? "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"}]