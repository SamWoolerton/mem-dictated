[{"type":"line text","chapterNumber":108,"verseNumber":43,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":108,"verseNumber":1,"sectionNumber":1,"value":"My heart is steadfast, God. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":1,"sectionNumber":2,"value":"I will sing and I will make music with my soul. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":2,"sectionNumber":1,"value":"Wake up, harp and lyre! "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":2,"sectionNumber":2,"value":"I will wake up the dawn. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":3,"sectionNumber":1,"value":"I will give thanks to you, Yahweh, among the nations. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":3,"sectionNumber":2,"value":"I will sing praises to you among the peoples. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":4,"sectionNumber":1,"value":"For your loving kindness is great above the heavens. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":4,"sectionNumber":2,"value":"Your faithfulness reaches to the skies. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":5,"sectionNumber":1,"value":"Be exalted, God, above the heavens! "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":5,"sectionNumber":2,"value":"Let your glory be over all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":6,"sectionNumber":1,"value":"That your beloved may be delivered, "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":6,"sectionNumber":2,"value":"save with your right hand, and answer us. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":7,"sectionNumber":1,"value":"God has spoken from his sanctuary: “In triumph, "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":7,"sectionNumber":2,"value":"I will divide Shechem, and measure out the valley of Succoth. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":8,"sectionNumber":1,"value":"Gilead is mine. Manasseh is mine. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":8,"sectionNumber":2,"value":"Ephraim also is my helmet. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":8,"sectionNumber":3,"value":"Judah is my scepter. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":9,"sectionNumber":1,"value":"Moab is my wash pot. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":9,"sectionNumber":2,"value":"I will toss my sandal on Edom. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":9,"sectionNumber":3,"value":"I will shout over Philistia.” "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":10,"sectionNumber":1,"value":"Who will bring me into the fortified city? "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":10,"sectionNumber":2,"value":"Who has led me to Edom? "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":11,"sectionNumber":1,"value":"Haven’t you rejected us, God? "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":11,"sectionNumber":2,"value":"You don’t go out, God, with our armies. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":12,"sectionNumber":1,"value":"Give us help against the enemy, "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":12,"sectionNumber":2,"value":"for the help of man is vain. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":13,"sectionNumber":1,"value":"Through God, we will do valiantly. "},{"type":"line break"},{"type":"line text","chapterNumber":108,"verseNumber":13,"sectionNumber":2,"value":"For it is he who will tread down our enemies. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by David."},{"type":"stanza start"}]