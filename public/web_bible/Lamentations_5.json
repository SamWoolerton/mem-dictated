[{"type":"line text","chapterNumber":5,"verseNumber":22,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":1,"value":"Remember, Yahweh, what has come on us. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":1,"sectionNumber":2,"value":"Look, and see our reproach. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":1,"value":"Our inheritance has been turned over to strangers, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":2,"sectionNumber":2,"value":"our houses to aliens. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":1,"value":"We are orphans and fatherless. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":3,"sectionNumber":2,"value":"Our mothers are as widows. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":1,"value":"We have drunken our water for money. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":4,"sectionNumber":2,"value":"Our wood is sold to us. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":1,"value":"Our pursuers are on our necks. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":5,"sectionNumber":2,"value":"We are weary, and have no rest. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":1,"value":"We have given our hands to the Egyptians, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":6,"sectionNumber":2,"value":"and to the Assyrians, to be satisfied with bread. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":1,"value":"Our fathers sinned, and are no more. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":7,"sectionNumber":2,"value":"We have borne their iniquities. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":1,"value":"Servants rule over us. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":8,"sectionNumber":2,"value":"There is no one to deliver us out of their hand. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":1,"value":"We get our bread at the peril of our lives, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":9,"sectionNumber":2,"value":"because of the sword of the wilderness. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":1,"value":"Our skin is black like an oven, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":10,"sectionNumber":2,"value":"because of the burning heat of famine. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":1,"value":"They ravished the women in Zion, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":11,"sectionNumber":2,"value":"the virgins in the cities of Judah. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":1,"value":"Princes were hanged up by their hands. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":12,"sectionNumber":2,"value":"The faces of elders were not honored. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":1,"value":"The young men carry millstones. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":13,"sectionNumber":2,"value":"The children stumbled under loads of wood. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":1,"value":"The elders have ceased from the gate, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":14,"sectionNumber":2,"value":"and the young men from their music. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":1,"value":"The joy of our heart has ceased. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":15,"sectionNumber":2,"value":"Our dance is turned into mourning. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":1,"value":"The crown has fallen from our head. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":16,"sectionNumber":2,"value":"Woe to us, for we have sinned! "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":17,"sectionNumber":1,"value":"For this our heart is faint. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":17,"sectionNumber":2,"value":"For these things our eyes are dim. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":18,"sectionNumber":1,"value":"For the mountain of Zion, which is desolate. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":18,"sectionNumber":2,"value":"The foxes walk on it. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":5,"verseNumber":18,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":5,"verseNumber":19,"sectionNumber":1,"value":"You, Yahweh, remain forever. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":19,"sectionNumber":2,"value":"Your throne is from generation to generation. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":20,"sectionNumber":1,"value":"Why do you forget us forever, "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":20,"sectionNumber":2,"value":"and forsake us for so long a time? "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":21,"sectionNumber":1,"value":"Turn us to yourself, Yahweh, and we will be turned. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":21,"sectionNumber":2,"value":"Renew our days as of old. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":22,"sectionNumber":1,"value":"But you have utterly rejected us. "},{"type":"line break"},{"type":"line text","chapterNumber":5,"verseNumber":22,"sectionNumber":2,"value":"You are very angry against us. "},{"type":"line break"}]