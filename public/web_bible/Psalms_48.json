[{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":1,"value":"Great is Yahweh, and greatly to be praised, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":1,"sectionNumber":2,"value":"in the city of our God, in his holy mountain. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":1,"value":"Beautiful in elevation, the joy of the whole earth, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":2,"value":"is Mount Zion, on the north sides, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":2,"sectionNumber":3,"value":"the city of the great King. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":3,"sectionNumber":1,"value":"God has shown himself in her citadels as a refuge. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":4,"sectionNumber":1,"value":"For, behold, the kings assembled themselves, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":4,"sectionNumber":2,"value":"they passed by together. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":5,"sectionNumber":1,"value":"They saw it, then they were amazed. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":5,"sectionNumber":2,"value":"They were dismayed. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":5,"sectionNumber":3,"value":"They hurried away. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":6,"sectionNumber":1,"value":"Trembling took hold of them there, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":6,"sectionNumber":2,"value":"pain, as of a woman in travail. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":7,"sectionNumber":1,"value":"With the east wind, you break the ships of Tarshish. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":1,"value":"As we have heard, so we have seen, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":2,"value":"in the city of Yahweh of Armies, in the city of our God. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":8,"sectionNumber":3,"value":"God will establish it forever. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":1,"value":"We have thought about your loving kindness, God, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":9,"sectionNumber":2,"value":"in the middle of your temple. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":1,"value":"As is your name, God, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":2,"value":"so is your praise to the ends of the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":10,"sectionNumber":3,"value":"Your right hand is full of righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":1,"value":"Let Mount Zion be glad! "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":11,"sectionNumber":2,"value":"Let the daughters of Judah rejoice because of your judgments. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":1,"value":"Walk about Zion, and go around her. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":12,"sectionNumber":2,"value":"Number its towers. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":1,"value":"Notice her bulwarks. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":2,"value":"Consider her palaces, "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":13,"sectionNumber":3,"value":"that you may tell it to the next generation. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":14,"sectionNumber":1,"value":"For this God is our God forever and ever. "},{"type":"line break"},{"type":"line text","chapterNumber":48,"verseNumber":14,"sectionNumber":2,"value":"He will be our guide even to death. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"For the Chief Musician. A Psalm by the sons of Korah."},{"type":"stanza start"}]