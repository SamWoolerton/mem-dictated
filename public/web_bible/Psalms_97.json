[{"type":"line text","chapterNumber":97,"verseNumber":13,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":97,"verseNumber":1,"sectionNumber":1,"value":"Yahweh reigns! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":1,"sectionNumber":2,"value":"Let the earth rejoice! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":1,"sectionNumber":3,"value":"Let the multitude of islands be glad! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":2,"sectionNumber":1,"value":"Clouds and darkness are around him. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":2,"sectionNumber":2,"value":"Righteousness and justice are the foundation of his throne. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":3,"sectionNumber":1,"value":"A fire goes before him, "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":3,"sectionNumber":2,"value":"and burns up his adversaries on every side. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":4,"sectionNumber":1,"value":"His lightning lights up the world. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":4,"sectionNumber":2,"value":"The earth sees, and trembles. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":5,"sectionNumber":1,"value":"The mountains melt like wax at the presence of Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":5,"sectionNumber":2,"value":"at the presence of the Lord of the whole earth. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":6,"sectionNumber":1,"value":"The heavens declare his righteousness. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":6,"sectionNumber":2,"value":"All the peoples have seen his glory. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":7,"sectionNumber":1,"value":"Let all them be shamed who serve engraved images, "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":7,"sectionNumber":2,"value":"who boast in their idols. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":7,"sectionNumber":3,"value":"Worship him, all you gods! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":8,"sectionNumber":1,"value":"Zion heard and was glad. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":8,"sectionNumber":2,"value":"The daughters of Judah rejoiced "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":8,"sectionNumber":3,"value":"because of your judgments, Yahweh. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":9,"sectionNumber":1,"value":"For you, Yahweh, are most high above all the earth. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":9,"sectionNumber":2,"value":"You are exalted far above all gods. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":10,"sectionNumber":1,"value":"You who love Yahweh, hate evil! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":10,"sectionNumber":2,"value":"He preserves the souls of his saints. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":10,"sectionNumber":3,"value":"He delivers them out of the hand of the wicked. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":11,"sectionNumber":1,"value":"Light is sown for the righteous, "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":11,"sectionNumber":2,"value":"and gladness for the upright in heart. "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":12,"sectionNumber":1,"value":"Be glad in Yahweh, you righteous people! "},{"type":"line break"},{"type":"line text","chapterNumber":97,"verseNumber":12,"sectionNumber":2,"value":"Give thanks to his holy Name. "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"A Psalm."},{"type":"stanza start"}]