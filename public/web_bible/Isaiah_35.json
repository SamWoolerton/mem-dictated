[{"type":"line text","chapterNumber":35,"verseNumber":17,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":35,"verseNumber":1,"sectionNumber":1,"value":"The wilderness and the dry land will be glad. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":1,"sectionNumber":2,"value":"The desert will rejoice and blossom like a rose. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":1,"value":"It will blossom abundantly, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":2,"value":"and rejoice even with joy and singing. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":3,"value":"Lebanon’s glory will be given to it, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":4,"value":"the excellence of Carmel and Sharon. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":5,"value":"They will see Yahweh’s glory, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":6,"value":"the excellence of our God. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":35,"verseNumber":2,"sectionNumber":7,"value":" "},{"type":"line text","chapterNumber":35,"verseNumber":3,"sectionNumber":1,"value":"Strengthen the weak hands, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":3,"sectionNumber":2,"value":"and make the feeble knees firm. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":4,"sectionNumber":1,"value":"Tell those who have a fearful heart, “Be strong! "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":4,"sectionNumber":2,"value":"Don’t be afraid! "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":4,"sectionNumber":3,"value":"Behold, your God will come with vengeance, God’s retribution. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":4,"sectionNumber":4,"value":"He will come and save you. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":35,"verseNumber":4,"sectionNumber":5,"value":" "},{"type":"line text","chapterNumber":35,"verseNumber":5,"sectionNumber":1,"value":"Then the eyes of the blind will be opened, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":5,"sectionNumber":2,"value":"and the ears of the deaf will be unstopped. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":6,"sectionNumber":1,"value":"Then the lame man will leap like a deer, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":6,"sectionNumber":2,"value":"and the tongue of the mute will sing; "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":6,"sectionNumber":3,"value":"for waters will break out in the wilderness, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":6,"sectionNumber":4,"value":"and streams in the desert. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":7,"sectionNumber":1,"value":"The burning sand will become a pool, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":7,"sectionNumber":2,"value":"and the thirsty ground springs of water. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":7,"sectionNumber":3,"value":"Grass with reeds and rushes will be in the habitation of jackals, where they lay. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":8,"sectionNumber":1,"value":"A highway will be there, a road, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":8,"sectionNumber":2,"value":"and it will be called “The Holy Way”. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":8,"sectionNumber":3,"value":"The unclean shall not pass over it, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":8,"sectionNumber":4,"value":"but it will be for those who walk in the Way. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":8,"sectionNumber":5,"value":"Wicked fools shall not go there. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":9,"sectionNumber":1,"value":"No lion will be there, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":9,"sectionNumber":2,"value":"nor will any ravenous animal go up on it. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":9,"sectionNumber":3,"value":"They will not be found there; "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":9,"sectionNumber":4,"value":"but the redeemed will walk there. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":10,"sectionNumber":1,"value":"Then Yahweh’s ransomed ones will return, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":10,"sectionNumber":2,"value":"and come with singing to Zion; "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":10,"sectionNumber":3,"value":"and everlasting joy will be on their heads. "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":10,"sectionNumber":4,"value":"They will obtain gladness and joy, "},{"type":"line break"},{"type":"line text","chapterNumber":35,"verseNumber":10,"sectionNumber":5,"value":"and sorrow and sighing will flee away.” "},{"type":"line break"},{"type":"stanza end"},{"type":"paragraph start"}]