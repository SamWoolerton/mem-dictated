[{"type":"line text","chapterNumber":24,"verseNumber":6,"sectionNumber":1,"value":" "},{"type":"line text","chapterNumber":24,"verseNumber":1,"sectionNumber":1,"value":"The earth is Yahweh’s, with its fullness; "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":1,"sectionNumber":2,"value":"the world, and those who dwell in it. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":2,"sectionNumber":1,"value":"For he has founded it on the seas, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":2,"sectionNumber":2,"value":"and established it on the floods. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":24,"verseNumber":2,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":24,"verseNumber":3,"sectionNumber":1,"value":"Who may ascend to Yahweh’s hill? "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":3,"sectionNumber":2,"value":"Who may stand in his holy place? "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":4,"sectionNumber":1,"value":"He who has clean hands and a pure heart; "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":4,"sectionNumber":2,"value":"who has not lifted up his soul to falsehood, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":4,"sectionNumber":3,"value":"and has not sworn deceitfully. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":5,"sectionNumber":1,"value":"He shall receive a blessing from Yahweh, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":5,"sectionNumber":2,"value":"righteousness from the God of his salvation. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":6,"sectionNumber":1,"value":"This is the generation of those who seek Him, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":6,"sectionNumber":2,"value":"who seek your face—even Jacob. "},{"type":"line break"},{"type":"stanza end"},{"type":"stanza start"},{"type":"line text","chapterNumber":24,"verseNumber":6,"sectionNumber":3,"value":" "},{"type":"line text","chapterNumber":24,"verseNumber":7,"sectionNumber":1,"value":"Lift up your heads, you gates! "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":7,"sectionNumber":2,"value":"Be lifted up, you everlasting doors, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":7,"sectionNumber":3,"value":"and the King of glory will come in. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":8,"sectionNumber":1,"value":"Who is the King of glory? "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":8,"sectionNumber":2,"value":"Yahweh strong and mighty, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":8,"sectionNumber":3,"value":"Yahweh mighty in battle. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":9,"sectionNumber":1,"value":"Lift up your heads, you gates; "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":9,"sectionNumber":2,"value":"yes, lift them up, you everlasting doors, "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":9,"sectionNumber":3,"value":"and the King of glory will come in. "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":10,"sectionNumber":1,"value":"Who is this King of glory? "},{"type":"line break"},{"type":"line text","chapterNumber":24,"verseNumber":10,"sectionNumber":2,"value":"Yahweh of Armies is the King of glory! "},{"type":"line break"},{"type":"stanza end"},{"type":"header","value":"By David."},{"type":"stanza start"}]